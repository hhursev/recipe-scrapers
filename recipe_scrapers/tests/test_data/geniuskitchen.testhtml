<script type="text/html-template" id="fd-af-item">
    <div class="fd-af-item {{= it.type}}_{{= it.id}}" data-item-selector=".{{= it.type}}_{{= it.id}}" data-type="{{= it.type}}">
        <div class="fd-af-item-header">
            <div class="fd-af-avatar">
                <span class="fd-avatar">
                    <a target="_blank" href="{{= it.memberProfileUrl }}">
                        <img data-src="{{= it.memberAvatar}}"
                            data-target=".fd-avatar"
                            data-crop-mode="fill">
                    </a>
                </span>
            </div>
            <div class="fd-af-metadata">
                <h4 class="fd-af-item-title">{{= it.type}} by <a target="_blank" href="{{= it.memberProfileUrl}}">{{= it.memberName}}</a></h4>
                <span class="fd-timestamp">{{? it.shareUrl && FD.Page.type === 'RECIPE'}}<a href="{{= it.shareUrl}}">{{= it.submitted}}</a>{{??}}{{= it.submitted}}{{?}}</span>
            </div>

            <div class="fd-af-menu">
                {{? it.type !== 'created' && it.type !== 'photo' && !it.selfPost && !it.autoDetected }}
                <a class="fd-af-menu-toggle btn" data-id="{{= it.id}}" href="#"><i class="icon-fdc-ellipsis-horizontal"></i></a>
                <ul class="fd-af-menu-items themeColor">
                    <li >
                        <span class="content-wrapper">
                            <a class="js-flag-close"><i class="icon-fdc-close"></i></a>
                            <div class="themeLinkColor menu-item js-af-flag"
                                 data-item-id="{{= it.id}}"
                                 data-item-type="{{= it.type}}">
                                    <i class="icon-fdc-flag"></i> Report this post</div>
                        </span>
                    </li>

                </ul>
                {{?? it.selfPost && !it.autoDetected && it.type !== 'photo'}}
                <a class="fd-af-edit-post" data-text-target=".{{= it.type}}_{{= it.id}}_text" data-item-id="{{= it.id}}" href="#"><i class="icon-fdc-edit"></i></a>
                {{?}}
            </div>
        </div>

        <div class="fd-af-item-content">
            {{? it.rating}}
            <div class="fd-rating" data-rating="{{= it.rating}}">
                <div class="five-star">
                    <span class="fd-rating-percent" style="width: {{= it.ratingPercentage}}%;"></span>
                </div>
            </div>
            {{?}}

            {{? it.type !== 'photo'}}
                {{? it.description }}
                    <p class="{{= it.type}}_{{= it.id}}_text fd-af-item-text">{{= it.description}}</p>
                {{??}}
                    <p class="{{= it.type}}_{{= it.id}}_text fd-af-item-text">{{? it.text}}{{= it.text}}{{?}}</p>
                {{?}}
            {{?}}

            {{? it.photos && it.photos.length}}
                <div class="fd-af-image-wrapper">
                    <div class="fd-af-image-container">
                        <img class="fd-af-image" data-id="{{= it.photos[0].id}}" data-src="{{= it.photos[0].photoUrl}}">
                    </div>
                </div>
            {{?? it.photoUrl}}
                <div class="fd-af-image-wrapper">
                    <div class="fd-af-image-container">
                        <img class="fd-af-image" data-src="{{= it.photoUrl}}">
                    </div>
                </div>
            {{?}}
        </div>
        {{? it.moderationStatus === 'approved'}}
        {{? it.counts}}
        <div class="fd-af-likes">
            <a class="af-like{{? it.counts.liked}} liked{{?}}" data-item-id="{{= it.id}}" data-item-type="{{= it.type}}" href="#">
                <i class="icon-fdc-like"></i>
                <span class="af-like-label">Like</span>
                <span class="af-like-count">{{? it.counts.like > 0}}{{= it.counts.like}}{{?}}</span>
            </a>
        </div>
        {{?}}

        <div class="fd-af-reply">
            {{? it.replies && it.replyTotal > 0}}
            <a class="fd-af-replies-toggle" href="#">
                <span class="af-like-label"><i class="icon-fdc-comment"></i>Replies</span>
                <span class="af-reply-count">{{= it.replyTotal}}</span>
            </a>
            {{?? it.type !== 'created'}}
            <a class="fd-af-leave-reply" data-item-id="{{= it.id}}" data-item-type="{{= it.type}}" href="#"> <i class="icon-fdc-comment"></i>Reply</a>
            {{?}}
        </div>
        {{?}}

        {{? it.replies}}
        <div class="fd-af-replies" data-toggle="hidden">
            {{~ it.replies :value:index}}
            <div class="fd-af-user-reply {{= value.type}}_{{= value.id}}">
                <span class="fd-avatar sm"><a target="_blank" href="{{= value.memberProfileUrl }}"><img data-src="{{= value.memberAvatar}}" data-target=".fd-avatar" data-crop-mode="fill"></a></span>
                <div class="fd-af-user-reply-wrap">
                    <div class="fd-af-menu">
                        <a class="fd-af-menu-toggle btn" data-id="{{= value.parentId}}" href="#"><i class="icon-fdc-ellipsis-horizontal"></i></a>
                        <ul class="fd-af-menu-items">
                            <li>
                                <span class="content-wrapper">
                                    <a class="js-flag-close"><i class="icon-fdc-close"></i></a>
                                    <div class="themeLinkColor menu-item js-af-flag"
                                         data-item-id="{{= value.parentId}}"
                                         data-item-type="{{= value.parentType}}"
                                         data-reply-id="{{= value.id}}">
                                            <i class="icon-fdc-flag"></i> Report this post</div>
                                </span>
                            </li>
                        </ul>
                    </div>
                    <a target="_blank" href="{{= value.memberProfileUrl}}">{{= value.memberName}}</a>
                    <span class="fd-timestamp">{{= value.submitted}}</span>
                    <p>{{= value.text}}</p>
                    <div class="fd-af-likes">
                        <a class="af-like af-like-reply{{? value.counts.liked}} liked{{?}}" data-item-id="{{= value.parentId}}" data-item-type="{{= value.parentType}}" data-reply-id="{{= value.id}}" href="#">
                            <i class="icon-fdc-like"></i>
                            <span class="af-like-label">Like</span>
                            <span class="af-like-count">{{? value.counts.like > 0}}{{= value.counts.like}}{{?}}</span>
                        </a>
                    </div>
                </div>
            </div>
            {{~}}

            {{? it.loadMoreReplies}}
            <div class="fd-af-load-more-wrapper">
                <button class="fd-af-load-more-replies btn-default" data-item-id="{{= it.id}}" data-item-type="{{= it.type}}" data-next-page="2">Load {{= it.repliesRemaining}} More {{? it.repliesRemaining == 1}}Reply{{?? it.repliesRemaining > 1}}Replies{{??}}{{?}} </button>
            </div>
            {{?}}

            <div class="fd-af-reply">
                <a class="fd-af-leave-reply" href="#" data-item-id="{{= it.id}}" data-item-type="{{= it.type}}" title="Comment">Reply</a>
            </div>
        </div>
        {{?}}

        {{? it.loggedInUser }}
        <div class="fd-af-reply-inline">
            <div class="fd-avatar-wrap">
                <span class="fd-avatar sm"><img data-src="{{= it.userAvatar }}" data-target=".fd-avatar" data-crop-mode="fill"></span>
            </div>
            <form>
                <textarea class="fd-af-user-reply-input" placeholder="Tap here to type your reply"></textarea>
                <button class="fd-af-reply-submit btn-default" data-item-id="{{= it.id}}" data-item-type="{{= it.type}}">Submit</button>
            </form>
        </div>
        {{?}}
    </div>
</script><!-- #fd-af-item => template -->

