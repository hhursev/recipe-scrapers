    <script type="text/javascript">
    function scriptsLoader(){
        var me = this;
        me.scripts;
        me.scrollOffset;
        me.loadTimeout;
        me.callback;
        me.ctrl = false;

        me.init = function(conf){
            me.scripts = conf.scripts || null;
            me.scrollOffset = conf.scrollOffset || null;
            me.loadTimeout = conf.loadTimeout || null;
            me.callback = conf.callback || null;

            if(!me.scripts){
                return;
            }

            if(me.scrollOffset || me.loadTimeout){
                if(me.scrollOffset){
                    window.addEventListener("scroll", function(){
                        if(!me.ctrl && window.pageYOffset > me.scrollOffset){
                            me.ctrl = true;
                            me.loadScript();
                        }
                    }, false);
                }

                if(me.loadTimeout){
                    setTimeout(function() {
                        if(!me.ctrl){
                            me.ctrl = true;
                            me.loadScript();
                        }
                    }, me.loadTimeout);
                }
                return;
            }

            me.loadScript();
        };

        me.loader = function(scriptObj, handler){
            if(scriptObj.src){
                var script = document.createElement("script");
                script.src = scriptObj.src;
                script.async = scriptObj.async || null;
                script.async = scriptObj.defer || null;

                script.onload = script.onreadystatechange = function(){
                    script.onreadystatechange = script.onload = null;
                    handler();
                }

                document.head.appendChild(script);
            }
        };

        me.loadScript = function(){
            if(me.scripts.length != 0){
                me.loader(me.scripts.shift(), me.loadScript);
            } else {
                if(me.callback) {
                    window[me.callback]();
                }
            }
        }
    }
    </script>
    <!-- END SCRIPTS LOADER -->



