,"recipeIngredient": [
"For the Kebab:",
"1 lb. ground lamb (or 1/2 lb. each of ground lamb and ground beef)",
"1 egg",
"4 cloves garlic (peeled and finely minced)",
"1 teaspoon ground cumin",
"1 teaspoon ground coriander",
"1 teaspoon smoked paprika",
"1 teaspoon dried oregano",
"1/2 teaspoon salt",
"1/4 teaspoon ground black pepper",
"For the Sandwich:",
"4 rounds of pita (or naan or flatbread)",
"1 cup assorted lettuce",
"1 large tomato (sliced)",
"1/2 seedless EnglishÂ cucumber (sliced)",
"1/4 large red onion (peeled and sliced)",
"Tzatziki sauce or tahini sauce" ]
,"recipeInstructions": [
{
"@type": "HowToStep"
,"text": "Gather the ingredients."
} ,{
"@type": "HowToStep"
,"text": "Pre-heat the oven to 350 F."
} ,{
"@type": "HowToStep"
,"text": "In a large bowl, combine the ground lamb, egg, garlic, ground cumin, ground coriander, smoked paprika, dried oregano, salt, and black pepper."
} ,{
"@type": "HowToStep"
,"text": "Place the mixture into a loaf pan and cook in the oven for approximately 30 minutes or until the top is a light golden brown."
} ,{
"@type": "HowToStep"
,"text": "You can slice the loaf immediately if you like but, for best results, cool completely, wrap in aluminum foil, and refrigerate until firm."
} ,{
"@type": "HowToStep"
,"text": "To reheat, add a little olive oil to a large skillet, slice the loaf very thinly and crisp up the slices in the hot pan for a few minutes."
} ,{
"@type": "HowToStep"
,"text": "Assemble the sandwiches with the vegetables and sauce."
} ]
,"recipeYield": "4 Servings"
,"totalTime": "PT50M"
}
