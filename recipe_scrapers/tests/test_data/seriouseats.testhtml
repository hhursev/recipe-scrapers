        try {
          out_of_page = $(ad).data("ad-oop");
        } catch (err) {}
        
        if (!unit.length || !size.length || !id.length)
          continue;
        
        ad["no_display"] = false;
        try {
          if ($(ad).data("no-display") == true)
            ad["no_display"] = true;
        } catch (err) {}
          
        if (out_of_page == true)
          _adSlots[index] = googletag.defineOutOfPageSlot('/11927475/' + unit, id).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
        else
          _adSlots[index] = googletag.defineSlot("/11927475/" + unit, size, id).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);;
        
        try {
          var ad_mapping = $(ad).data("ad-mapping");
          if (ad_mapping == "horizontal_mapping")
          {
            _adSlots[index].defineSizeMapping(horizontal_mapping);
          } 
          else if (ad_mapping == "vertical_mapping")
          {
            _adSlots[index].defineSizeMapping(vertical_mapping);
          }
          else if (ad_mapping == "logo_mapping")
            _adSlots[index].defineSizeMapping(logo_mapping);
          else if (ad_mapping == "below_entry_mapping")
            _adSlots[index].defineSizeMapping(below_entry_mapping);

        } catch (err) {}

        // Only apply to standard above-the-fold placements
        if ($.inArray(ad.id, ["homepage-top-billboard", "entry-top-billboard", "sidebar-top"]) >= 0)
        {
          yieldbot.setSlotTargeting(unit, _adSlots[index]);
          yb_slots.push({'slot_name': unit, 'slot_var':_adSlots[index]});
        }  

        if ($(ad).data("ad-block") && dfpKeyCount < maxKeys)
        {
          _adSlots[index].setTargeting("Block", $(ad).data("ad-block"));
          dfpKeyCount++;
        }

        if ($(ad).data("ad-tag") && dfpKeyCount < maxKeys)
        {
          if (_useValidTags == true)
          {
            var validTags = [];
            var tags = $(ad).data("ad-tag").replace(/\s/g, '').toLowerCase().split(",");
            for (var j = 0; j < tags.length; ++j)
            {
              if (_validTags.indexOf(tags[j]) >= 0)
                validTags.push(tags[j]);
            }

            if (validTags.length > 0)
            {
              _adSlots[index].setTargeting("Tag", validTags.join(",").substring(0,40));
              dfpKeyCount++;
            }
          }
          else
          {
            _adSlots[index].setTargeting("Tag", $(ad).data("ad-tag").replace(/\s/g, '').substring(0,40));
            dfpKeyCount++;
          }            
        }
        index++;
      }

      googletag.pubads().enableSingleRequest();
    });

    googletag.enableServices();    
    
    for (var k = 0; k < ads.length; ++k)
    {
      var ad = ads[k];
      if (ad["no_display"] == true)
        continue;
      googletag.display(ad.id);
    }
  });
  }
});

