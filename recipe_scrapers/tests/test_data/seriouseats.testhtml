  <script src="seriouseats_files/homemade-horseradish-recipe-comments.js" type="text/javascript"></script>

  <script type="text/javascript">
    var commentsJSLoaded = false;
    var commentsDisplayed = false;
    var commentCount = '';
    var commentCountText = '';
    var intervalID = 0;
    var numCommentsPerPage = 100;

    var _seStaff = ["ambienttra", "clamond", "daniel gritzer", "ed levine", "fozziebayer", "goodeaterkenji", "leangdamang", "maggie hoffman", "mfalk", "nikiag", "paulcline", "vickywasik"];
    var authorUsername = "josh!";
    // For printing comments with Recipe posts   
    var recipeCommentsToInclude = new Array(); 
    var jumpToTopOfComments = true;
    
    if (numComments > 0) {
      commentCount = numComments + " <span>Comment";
      if (numComments > 1) {
        commentCount += "s";
      }
    } else {
      commentCount = "<span>Post a Comment";
    }
    commentCount += '</span>';

      commentCountText = commentCount;               

    commentsJSLoaded = true;
  
    intervalID = setInterval("showCommentSection()", 1000);

  function showCommentSection()
  {
    if (!commentsDisplayed)
    {
      if (commentsJSLoaded && ($('#commentSection').length))  {
        if ($('#postCommentCount').length) {
          $("#postCommentCount").html(commentCountText);
        }
        if($(".entryCommentCount").length > 0) {
          $(".entryCommentCount").each(function() {
            $(this).html(commentCount);
          });
        }

        if (numComments <= numCommentsPerPage) {
          showComments(0, numComments);
        } else {
          jumpToTopOfComments = false;
          $(".pagination").pagination(numComments, {
            items_per_page:numCommentsPerPage,
            num_edge_entries:1,
            num_display_entries:9,
            next_show_always:false,
            prev_show_always:false,
            link_to:"#comments",
            callback:handlePaginationClick
          });

          jumpToTopOfComments = true;
        }
        commentsDisplayed = true;
      } 
    } else {
      clearInterval(intervalID);
    } 
  }

  function showComments(startNum, endNum)
  {
    var commentContent = '';
    var commentAuthor = '';
    var regex = new RegExp("\n", "g"); 

    var urlRegex =/(\b(https?):\/\/(www)?\.amazon[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;

    for (var i = startNum;i < endNum;i++) {
 
      try {
        commentJSON[i]["text"] = commentJSON[i]["text"].replace(urlRegex, function(url) {  
          var amazonUrl = new URL(url);
          if (amazonUrl["search"].length > 0)
            amazonUrl["search"] += "&tag=serieats-20";
          else
            amazonUrl["search"] += "?tag=serieats-20";
          return amazonUrl.href;
        });
      }
      catch (e) {}
      
      commentAuthor = commentJSON[i]["author_username"];
      commentContent += '<a name="' + commentJSON[i]["comment_id"] + '"></a>';
      commentContent += '<div class="comment';

      var staffLabel = "";
      if (commentAuthor == authorUsername) {
        commentContent += ' author';
      } else if (_seStaff.indexOf(commentAuthor.toLowerCase()) >= 0)
      {
        commentContent += ' staff';
        staffLabel = '<span class="label staff-user">Staff</span>';
      }
      
      commentContent += '" id="' + commentJSON[i]["comment_id"] + '">';
      
        commentContent += '<footer class="metadata"><a href="http://www.seriouseats.com/user/profile/' + commentJSON[i]["author_username"] + '" class="commenter';
        if (commentAuthor == authorUsername) {
          commentContent += ' author'; 
        }
        commentContent += '">' + decodeURIComponent(escape(commentJSON[i]["author_nickname"])) + '</a>' + staffLabel + '<time><a href="http://www.seriouseats.com/recipes/2011/04/homemade-horseradish-recipe.html#' + commentJSON[i]["comment_id"] + '">';
        var date = commentJSON[i]["created_on"];

        var commentDate = new Date(date.substr(0,4), date.substr(4,2)-1, date.substr(6,2), date.substr(8,2), date.substr(10,2), date.substr(12,2));

        commentContent += commentDate.toString("h:mmtt on MM/dd/yy") + '</a></time></footer>';
        commentContent += '<div class="comment-body"><div class="recipe-comment-checkbox"><input type="checkbox" name="include-comment" class="include-comment" value="' + commentJSON[i]["comment_id"] + '"';
        // check to see if this comment is in the recipeCommentsToInclude array
        if ((jQuery.inArray((commentJSON[i]["comment_id"]), recipeCommentsToInclude)) >= 0) {
          commentContent += 'checked ';
        }
        commentContent += '/></div>';
        if (commentJSON[i]["star_rating"] && (commentJSON[i]["star_rating"] > 0))
        {
          var star_full = '<i class="icon icon-nav-star-full"></i>';
          var star_empty = '<i class="icon icon-nav-star-empty"></i>';
          commentContent += '<div class="rating rating-user"><span class="label">recipe rating:</span><span class="info rating-stars">';
          for (var j = 0; j < 5; j++) {
            
            if (j < commentJSON[i]["star_rating"])
              commentContent += star_full;
            else
              commentContent += star_empty;
          }
          commentContent += '</span></div>';
        }
        commentContent += '<div class="comment-content"><p>' + commentJSON[i]["text"].replace(regex, "<br />") + '</p></div></div>';
      commentContent += '</div>';		
    }
    $('#commentSection').html(commentContent);
  
    // Recipe post - checked comments for printing
    $('input[name$="include-comment"]').change(function() {
      
      var comment_id = $(this).attr('value');
      $("div.comment#" + comment_id).toggleClass("comment-print");

      if ($(this).is(':checked')) { 
        var arrayIndex = (jQuery.inArray(comment_id, recipeCommentsToInclude));
        if (arrayIndex < 0) {
          recipeCommentsToInclude.push(comment_id);
          comments_list = recipeCommentsToInclude.join();
        }
      }
      else {
        var arrayIndex = (jQuery.inArray(comment_id, recipeCommentsToInclude));
        if (arrayIndex >= 0) {
          recipeCommentsToInclude.splice(arrayIndex,1);
          comments_list = recipeCommentsToInclude.join();
        }
      }
    });
  }

  function handlePaginationClick(page_index, pagination_container) {
    var max_num = Math.min((page_index+1) * numCommentsPerPage, numComments);
    var commentRange = '';
    if ((max_num < numComments) || (max_num > numCommentsPerPage)) {
      commentRange += "(" + (page_index*numCommentsPerPage + 1) + " - " + max_num + ") of ";
    }

    $('#commentRange').html(commentRange);

    showComments(page_index*numCommentsPerPage, max_num);
    
    if (jumpToTopOfComments) {
      document.getElementById('comments').scrollIntoView();
    }
  
    return false;
  }

  $(document).ready(function()
  {
    if (!commentsDisplayed)
    {
      showCommentSection();
    }
  });
