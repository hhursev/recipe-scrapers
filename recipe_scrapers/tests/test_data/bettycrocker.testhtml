    <div id="ab5481b5-f301-4054-996c-7c18d0f53ebd" class="RegistrationFlowContainer defaultRegistration" data-registration-flow="true" data-proxy-url="">
        <p class="flowHeader" data-bind="html: FlowHeaderText"></p>

        <div class="registrationActionCTA">
            <span data-bind="visible: !$root.RegActionCTAVisible(), html: RegActionDefaultText"></span>
            <div data-bind="visible: $root.RegActionCTAVisible">
                <h2 class="registrationActionCTAHeadline" data-bind="html: RegActionCTAHeadline"></h2>
                <img class="registrationActionCTAImage" alt="" data-bind="visible: RegActionCTAImageUrl, attr: {src: RegActionCTAImageUrl}" />
                <div class="registrationActionCTAText" data-bind="visible: RegActionCTAText, html: RegActionCTAText"></div>
            </div>
        </div>

        <div class="loginView" data-bind="attr: { 'data-step-id': 'LoginView' }, visible: CurrentView() === window.GeneralMills.Registration.Settings.loginViewName, enterKey: Login">
            <a href="#" class="alreadyAMemberLink" data-bind="attr: { id: $root.loginStepId + 'SmallCTAText' }, text: SmallCTAText, click: SwitchToRegistrationView"></a>
            <div class="loginViewForm">
                <h2 data-bind="text: StepName"></h2>
                <span class="alert alert-error" data-bind="text: error, visible: error"></span>
                <fieldset data-bind="foreach: LoginStepFields">
                    <div data-bind="template: ViewTemplateName"></div>
                </fieldset>
                <fieldset data-bind="foreach: LoginStepNewsletterFields">
                    <div data-bind="template: ViewTemplateName"></div>
                </fieldset>
                <div class="forgotPasswordContainer">
                    <a href="#" class="forgotPasswordLink" data-bind="attr: { id: $root.loginStepId + 'forgotPassword' },text: ForgotPasswordText, click: SwitchToForgotPasswordView"></a>
                </div>

                <div class="regButtonContainer">
                    <!-- ko foreach: LoginStepButtons -->
                    <button type="button" data-bind="html: DisplayText, click: $parent.ButtonClick.bind($data, $parent), attr: { 'id':  $root.loginStepId + Id, 'data-action': Action, 'class': Action  }"></button>
                    <!-- /ko -->
                </div>
                
                <div class="loginFederatedCTA" data-bind="template: { 'name': 'gigyaFederatedCTATemplate', 'if': GigyaLoginIsEnabled }"></div>
            </div>
        </div>

        <div class="registrationView" data-bind="attr: { 'data-step-id': 'SocialLinkView' }, visible: CurrentView() === window.GeneralMills.Registration.Settings.socialLinkViewName, enterKey: FederatedLink">
            <div class="registrationSteps">
                <div class="registrationStep">
                    <div class="registrationStepInfo" data-bind="attr: { 'data-step-id': Id, 'data-step-number': 0, 'class': FederatedCurrentUserCssClass }">
                        <h2 data-bind="text: FederatedCurrentUserStepName"></h2>
                        <span data-bind="html: FederatedCurrentUserHeaderText"></span>

                        <fieldset data-bind="foreach: FederatedCurrentUserDisplayFields">
                            <div data-bind="template: ViewTemplateName"></div>
                        </fieldset>
                        <div class="regButtonContainer">
                            <!-- ko foreach: FederatedCurrentUserDisplayButtons -->
                            <button type="button" data-bind="html: DisplayText, click: $root.ButtonClick.bind($data, $parent), attr: { 'id': Id, 'data-action': Action, 'class': Action  }"></button>
                            <!-- /ko -->
                        </div>
                        <span data-bind="html: FederatedCurrentUserFooterText"></span>
                    </div>
                </div>
                <span class="alert alert-error" data-bind="text: $root.error(), visible: $root.error()"></span>
            </div>
            <div data-bind="visible: $root.CurrentStep() == 0">
                
            </div>
        </div>

        <div class="registrationView" data-bind="template: {name: '_registration.registrationStep.tmpl', data: {'RegistrationSteps': RegistrationSteps, 'ViewName': 'RegistrationView'}}, attr: { 'data-step-id': 'RegistrationView' }, visible: CurrentView() === window.GeneralMills.Registration.Settings.registrationViewName, enterKey: RegistrationEnterKeyPress"></div>
        <div class="registrationView" data-bind="template: {name: '_registration.registrationStep.tmpl', data: {'RegistrationSteps': SocialRegistrationSteps, 'ViewName': 'RegistrationView'}}, attr: { 'data-step-id': 'SocialRegistrationView' }, visible: CurrentView() === window.GeneralMills.Registration.Settings.socialRegViewName, enterKey: RegistrationEnterKeyPress"></div>

        <span class="alert alert-error" data-bind="visible: CurrentView() === window.GeneralMills.Registration.Settings.socialRegViewName && GigyaLoginIsEnabled && SocialRegistrationSteps.length == 0">There are no social login steps defined in Sitecore for this flow!</span>

        <div class="legalAgeView" data-step-id="legalAgeView" data-bind="visible: CurrentView() === window.GeneralMills.Registration.Settings.validAgeErrorViewName">
            <h2 data-bind="text: GlobalConfiguration.LegalAgeHeaderText"></h2>
            <span data-bind="html: GlobalConfiguration.LegalAgeErrorText"></span>
        </div>

        <div class="forgotPasswordView" data-bind="attr: { 'data-step-id': Id }, visible: CurrentView() === window.GeneralMills.Registration.Settings.resetPasswordViewName, enterKey: ResetPassword">
            <h2 data-bind="text: ResetPasswordTitle"></h2>
            <span class="alert alert-success resetPasswordMessage" data-bind="html: PasswordResetSuccessMessage, visible: PasswordResetSuccessMessage"></span>
            <div data-bind="visible: !(PasswordResetSuccessMessage())">
                <span data-bind="html: ResetPasswordMessage" class="resetPasswordMessage"></span>
                <fieldset data-bind="foreach: PasswordResetFields">
                    <div data-bind="template: ViewTemplateName"></div>
                </fieldset>
                <span class="alert alert-error" data-bind="text: error(), visible: error()"></span>
                <div class="regButtonContainer">
                    <!-- ko foreach: PasswordResetButtons -->
                    <button type="button" data-bind="html: DisplayText, click: $parent.ButtonClick.bind($data, $parent), attr: { 'id': Id, 'data-action': Action, 'class': Action  }"></button>
                    <!-- /ko -->
                </div>
            </div>
        </div>

        <span data-bind="html: FlowFooterText"></span>

        <iframe id="proxyIFrame" class="proxyFrame" frameborder="0" scrolling="no" height="0" width="0" style="display: none;"></iframe>
    </div>

    <!-- Templates -->
    <script id="_registration.registrationStep.tmpl" type="text/html">
        <!-- ko ifnot: $root.ShowThankYouStep -->
        <!-- ko if: $root.AlreadyAMemberText.length > 0 -->
        <a href="#" class="alreadyAMemberLink" data-bind="attr: { id: 'gmi' + $root.Id + '_' + ViewName + '_alreadyAMemberLink' }, text: $root.AlreadyAMemberText, click: $root.SwitchToLoginView"></a>
        <!-- /ko -->
        <!-- /ko -->
        <div data-bind="template: { 'name': 'progressBarTemplate', 'if': !$root.DisplayProgressBarBelowStep }"></div>
        <div class="registrationSteps">
            <!-- ko foreach: RegistrationSteps -->
            <div class="registrationStep" data-bind="visible: IsCurrentStep">
                <div class="registrationStepInfo" data-bind="attr: { 'data-step-id': Id, 'data-step-number': StepNumber, 'class': CssClass }">
                    <h2 data-bind="text: StepName"></h2>
                    <div data-bind="template: { 'name': 'progressBarTemplate', data: $root, 'if': $root.DisplayProgressBarBelowStep }"></div>
                    <p data-bind="html: HeaderText"></p>

                    <fieldset data-bind="foreach: RegistrationFields">
                        <div data-bind="template: ViewTemplateName"></div>
                    </fieldset>
                    <span class="legal-text legal-text-above-buttons" data-bind="html: LegalText"></span>
                    <div class="regButtonContainer">
                        <!-- ko foreach: RegistrationStepButtons -->
                        <button type="button" data-bind="html: DisplayText, click: $root.ButtonClick.bind($data, $parent), attr: { 'id': Id, 'data-action': Action, 'class': Action  }"></button>
                        <!-- /ko -->
                        <span class="legal-text legal-text-beside-buttons" data-bind="html: LegalText"></span>
                    </div>
                    <div class="loginFederatedCTA" data-bind="template: {'name': 'gigyaFederatedCTATemplate', 'if': $root.GigyaLoginIsEnabled && $root.CurrentView() === window.GeneralMills.Registration.Settings.registrationViewName && $root.CurrentStep() === 0}"></div>
                    <span class="legal-text legal-text-below-buttons" data-bind="html: LegalText"></span>
                    <span data-bind="html: FooterText"></span>
                </div>
            </div>
            <!-- /ko -->
            <div class="registrationStep thankYouStep" data-bind="visible: $root.ShowThankYouStep">
                <h2 data-bind="text: $root.ThankYouHeader"></h2>
                <div class="thankYouContent">
                    <span data-bind="html: $root.ThankYouContent"></span>
                </div>
                <div class="registrationRedirect">
                    <a data-bind="text: $root.ThankYouLinkText, click: $root.redirectAction"></a>
                </div>
            </div>
            <span class="alert alert-error" data-bind="text: $root.error(), visible: $root.error()"></span>
        </div>
        <div data-bind="visible: $root.CurrentStep() == 0">
            
        </div>
    </script>

    <script id="_registration.textbox.tmpl" type="text/html">
        <div data-bind="css: CssClass, attr: { 'data-field-id': Id }">
            <label data-bind="text: Label, attr: { 'for': FieldLabelLinkName }"></label>
            <span class="hint-text" data-bind="text: HintText"></span>            
            <input type="text" data-bind="value: Value, validationElement: Value, attr: { 'id': FieldLabelLinkName, 'name': FieldLabelLinkName, 'required': IsMandatory ? '' : null, 'placeholder': PlaceholderText }" />
            <span class="alert alert-error" data-bind="text: Error, visible: Error"></span>
        </div>
    </script>

    <script id="_registration.phone.tmpl" type="text/html">
