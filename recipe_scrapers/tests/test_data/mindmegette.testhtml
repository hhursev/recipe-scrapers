                    if ($("#user_recipes_folders_ul").length > 0) {
                        $("#user_recipes_folders_ul").html(usr_info.recipe_folders_html);
                    }
                    if ($("#footer_szakacskonyvem_h4").length > 0) {
                        $("#footer_szakacskonyvem_h4").removeClass("not-logged-in");
                        $("#footer_szakacskonyvem_h4_a").attr("href", "/profil/" + usr_info.login_name);
                    }
                    $(".recipe_rating_stars_li").show(); /* Receptértékelések */
                    $(".recipe_rating_stars_li_login").hide(); /* Receptértékelés login része */
                    /* Ha magazin cikkben van és be van jelentkezve, mentési státusz lekérdezése és gomb manipulálása  */
                    if ($("#save-remove-article-button-li").length > 0) {
                        get_article_user_save_status();
