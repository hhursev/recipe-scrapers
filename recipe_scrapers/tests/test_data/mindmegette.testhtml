        <script>
            var EVE = EVE || {};

            EVE.pageTime = 0;
            EVE.intervalTimerSpeed = 30;
            EVE.interactiveEventAfter = 30;
            EVE.onPageEventAfter = [300, 600, 900, 1800];

            EVE.intervalTimerFunc = function () {
                EVE.pageTime += EVE.intervalTimerSpeed;
                if (-1 !== EVE.onPageEventAfter.indexOf(EVE.pageTime)) {
                    trackThisEvent("Oldal", "Esemény", "Oldalon van " + EVE.pageTime + " másodperce");
                }
            }

            EVE.intervalTimer = setInterval(EVE.intervalTimerFunc, EVE.intervalTimerSpeed * 1000);
            EVE.stopIntervalTimer = function () {
                clearInterval(EVE.intervalTimer);
            }

            var trackThisEvent = function (category, type, label) {
                if (typeof (category) === 'undefined') {
                    category = 'Minden oldal';
                }
                if (typeof (type) === 'undefined') {
                    type = 'Esemény';
                }
                if (typeof (label) === 'undefined') {
                    label = 'pontatlan';
                }

                non_interaction_event = EVE.pageTime >= EVE.interactiveEventAfter ? true : false;

                gtag('event', type, {
                    'event_category': category,
                    'event_label': label,
                    'non_interaction': non_interaction_event
                });
            }

            /**
             * Event bejelentése GA-nak, majd ugrás az adott link-re.
             * 
             * @param str url - ugrás Link-je a bejelntés után
             * @param str category - Link kategóriája
             * @param str label - Link címkéje pl. title
             * @returns empty
             */
            var trackOutboundLink = function (url, category, type, label) {
                if (typeof (category) === 'undefined') {
                    category = 'outbound';
                }
                if (typeof (type) === 'undefined') {
                    type = 'click';
                }
                if (typeof (label) === 'undefined') {
                    label = url;
                }

                gtag('event', type, {
                    'event_category': category,
                    'event_label': label,
                    'transport_type': 'beacon',
                    'event_callback': function () {
                        document.location = url;
                    }
                });
            }

            /**
             * Összegyűjtjük az oldalon az összes eseménybeküldést
             */
            console.log("============== TRACKED click EVENTS GA =============");
            jQuery("a.ga-cet").each(function () {

                console.log(jQuery(this).data("ga-event-category") + " | " + jQuery(this).data("ga-event-type") + " | " + jQuery(this).data("ga-event-label"));

                jQuery(this).on("click", function () {

                    var cet_url = this.href;
                    var cet_category = typeof (jQuery(this).data("ga-event-category")) !== 'undefined' ? jQuery(this).data("ga-event-category") : "outbound";
                    var cet_type = typeof (jQuery(this).data("ga-event-type")) !== 'undefined' ? jQuery(this).data("ga-event-type") : "click";
                    var cet_label = typeof (jQuery(this).data("ga-event-label")) !== 'undefined' ? jQuery(this).data("ga-event-label") : cet_url;

                    trackOutboundLink(cet_url, cet_category, cet_type, cet_label);

                });

            });
            console.log("============== TRACKED scroll position EVENTS GA =============");

            jQuery(window).load(function () {

                var listScrollPos = [];
                var listCategoryPos = [];
                var listTypePos = [];
                var listLabelPos = [];
                var listFiredPos = [];

                var checkScrollPos = function () {

                    var currentPos = jQuery(window).scrollTop();

                    for (i = 0; i < listScrollPos.length; i++) {
                        if (!listFiredPos[i]) {
                            if (listScrollPos[i] < (currentPos + 200)) {

                                trackThisEvent(listCategoryPos[i], listTypePos[i], listLabelPos[i]);

                                listFiredPos[i] = true;

                                console.log("fired: " + listCategoryPos[i] + " | " + listTypePos[i] + " | " + listLabelPos[i]);
                            }
                        }
                    }
                }

                jQuery(".ga-set").each(function () {

                    var set_category = typeof (jQuery(this).data("ga-event-category")) !== 'undefined' ? jQuery(this).data("ga-event-category") : "Minden oldal";
                    var set_type = typeof (jQuery(this).data("ga-event-type")) !== 'undefined' ? jQuery(this).data("ga-event-type") : "Görgetési";
                    var set_label = typeof (jQuery(this).data("ga-event-label")) !== 'undefined' ? jQuery(this).data("ga-event-label") : "ID:" + jQuery(this).attr("id");

                    listScrollPos.push(jQuery(this).offset().top);
                    listFiredPos.push(false);
                    listCategoryPos.push(set_category);
                    listTypePos.push(set_type);
                    listLabelPos.push(set_label);

                    console.log(jQuery(this).attr("id") + " | " + set_category + " | " + set_type + " | " + set_label + " | " + jQuery(this).offset().top);

                });

                jQuery(document).on("scroll", checkScrollPos);

            });
        </script>

