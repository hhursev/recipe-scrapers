                } else {
                    /* Ha magazin cikkben van és nincs bejelentkezve */
                    if ($("#save-remove-article-button-li").length > 0) {
                        $("#save-remove-article-button-li").addClass("dropdown");                        
                    }
                }
            }
        });
    });
    
