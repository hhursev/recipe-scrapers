    function get_article_user_save_status(){
        $.ajax({
            type: "POST", cache: false,
            url: "http://www.mindmegette.hu/user_info_get_ajax/",
            data: {user_info_get_ajax: "1",get_article_saved_info:$('#articleID').val()},
