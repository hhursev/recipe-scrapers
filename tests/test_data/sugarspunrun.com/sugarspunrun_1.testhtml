	font-family:"Helvetica", sans-serif !important;
	-webkit-font-smoothing: antialiased;
}

ul > li span, ul > li strong {
	font-family:"Helvetica", sans-serif !important;
	-webkit-font-smoothing: antialiased;
}

strong{
	font-weight: 800 !important;
}

