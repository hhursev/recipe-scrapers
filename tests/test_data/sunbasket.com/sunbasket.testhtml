<script type="text/javascript">
  // ajax + infinite scroll for search-results.jsp and sidebar of recipes.jsp
  var scrollLoad = (function () {
    var content = {};
    var pageIndex = 0;
    var tileAmount = 20;
    var searchQuery;
    if (isLoadingTiles === undefined) {
      var isLoadingTiles = false;
    }

    $(window).scrollTop(0);

    function prepareJSON(searchQuery, searchType) {
      var jsonQuery;
      if (searchType === "SEARCH") {
        jsonQuery = {q: searchQuery}
      } else if (searchType === "RECIPE_DIET_TYPE") {
        jsonQuery = {dietType: searchQuery.replace(/-/g, "_")}
      } else if (searchType === "RECIPE_TAG") {
        jsonQuery = {recipeTag: searchQuery.replace(/-/g, "_")}
      } else if (searchType === "RECIPESEARCH") {
        jsonQuery = {q: searchQuery, limit: 1000, type: "recipe"}
      } else {
        jsonQuery = {q: searchQuery, limit: 1000, type: searchType}
      }

      if (searchQuery === "soups-and-stews") {
        jsonQuery.q = "soup stew chili broth";
      }
      if (searchQuery === "pasta-and-noodles") {
        jsonQuery.q = "pasta noodle soba udon ramen penne rigatoni pappardelle fettuccine orzo spaghetti orecchiette";
      }
      if (searchQuery === "salads-and-bowls") {
        jsonQuery.q = "salad bowl";
      }
      if (searchQuery === "tacos") {
        jsonQuery.q = "tacos tostadas";
      }
      if (searchQuery === "seafood") {
        jsonQuery.q = "fish salmon shrimp cod sole halibut tuna";
      }
      if (searchQuery === "burgers-and-sliders") {
        jsonQuery.q = "burgers sliders";
      }

      return jsonQuery;
    }

    function loadSomeTiles(response) {
      if (typeof response == 'undefined') {
        // exit function if scrolled on non-search page
        return false;
      } else {
        var results = response.results.slice(scrollLoad.pageIndex, scrollLoad.pageIndex + tileAmount);
      }

      results = replaceImgWithThumb(results);
      results = generateWebUrl(results);

      var template = $('#explore-tile-template').html();
      var html = Mustache.render(template, {results: results});

      isLoadingTiles = true;
      var endOfResults = response.results.length;
      var isDoneLoading = (scrollLoad.pageIndex >= endOfResults - tileAmount);

      if (scrollLoad.pageIndex === 0) {
        //new category load
        tileAmount = 20;
        $('#repopulate').html(html);
        isLoadingTiles = false;
        $(".spinner-lg-blue").hide();

      } else if (!isDoneLoading) {
        //scrolling down
        tileAmount = 10;
        setTimeout(function () {
          $("#repopulate .explore-tile-container").last().after(html);
          isLoadingTiles = false;
          $(".spinner-lg-blue").hide();
        }, 500);
      }


      if (isDoneLoading) {
        $("footer").show();
      } else {
        $(".spinner-lg-blue").show();
        $("footer").hide();
      }

      if (scrollLoad.searchQuery !== undefined) {
        $("#recipesByIngredientTitle").show();
      }
    }

    return {
      // public methods
      loadAjaxContent: function (searchQuery, searchType) {
        scrollLoad.searchQuery = searchQuery;

        $('#repopulate').html("");

        var jsonQuery = prepareJSON(searchQuery, searchType);
        // on first load, populate content object from json response
        if (typeof content[searchQuery] === 'undefined' || content[searchQuery].length === 0) {

          $.ajax({
            url: "/general-search/api",
            type: "get",
            data: jsonQuery,
            success: function (response) {
              loadSomeTiles(response);
              // populate content object
              content[searchQuery] = response;

              if (response.results.length === 0) {
                $(".generalSearchPage #repopulate").html("<div class='padding-md'>No results.</div>");
              }
            }
          });

        } else {
          // content object already populated
          loadSomeTiles(content[searchQuery]);
        }
      },
      loadTilesAtBottom: function () {
        var scrollBarPos = $(window).scrollTop();
        var pageBottom = ($(document).height() - $(window).height()) - 60;

        if (scrollBarPos >= pageBottom) {
          scrollLoad.pageIndex = scrollLoad.pageIndex + tileAmount;
          if (isLoadingTiles === false) {
            loadSomeTiles(content[scrollLoad.searchQuery])
          }

        }
      },
      searchQuery: searchQuery,
      pageIndex: pageIndex
    }

  }());

  $(window).on('scroll', function () {
    scrollLoad.loadTilesAtBottom();
  });

  function replaceImgWithThumb(arr) {
    for (var i = 0; i < arr.length; i++) {
      var thisImage = arr[i]["image"];
      if (thisImage.indexOf('tn_') === -1) {
        thisImage = 'tn_' + thisImage;
      }
      arr[i]["imageUrl"] = '//cdn.sunbasket.com/' + thisImage;
    }
    return arr;
  }

  function generateWebUrl(searchResults) {
      return searchResults.map(singleResult => ({ ...singleResult, webUrl: singleResult.type + "/" + singleResult.slug}));
  }

  function imgLoaded($this) {
    //load background-image of img-container, remove spinner
    var $imgContainer = $this.parent('a');
    $imgContainer.css('background-image', 'url(' + $this.attr('src') + ')');

    if (scrollLoad.pageIndex === 0) {
      $imgContainer.parent('.explore-tile').show();
    } else {
      $imgContainer.parent('.explore-tile').fadeIn("fast");
    }
    $this.remove();
  }
</script>


<script>
  $("#global-search-input").on("click", function () {
    $(this).trigger('select');
  });

  $(".generalSearchPage #search-form").on('submit', function (e) {
    e.preventDefault();
    var val = $('#global-search-input').val();
    scrollLoad.pageIndex = 0;
    scrollLoad.loadAjaxContent(val, "SEARCH");
    $("#search-results-title").text("Search Results: " + val);
  });

  //send mobile input val over to regular search input
  $('.mobile-search-input').on('keyup', function () {
    $('#global-search-input').val($(this).val());
  });

  $("#mobile-search-form").on('click', '.icon-search', function (e) {
    e.preventDefault();
    $("#search-form").trigger('submit');
    $('#mobile-global-search-modal .close').trigger('click');
  });

  $(".mobile-search-input").on('keypress', function (e) {
      scrollLoad.pageIndex = 0;
      scrollLoad.loadAjaxContent($('#global-search-input').val(), "SEARCH");
      $('#mobile-global-search-modal .close').trigger('click');
  });


  function isHeroColorLightOrDark(hex) {
    var stripedHex = hex.substring(1);      // strip #
    var rgb = parseInt(stripedHex, 16);   // convert rrggbb to decimal

    var r = (rgb >> 16) & 0xff;  // extract red
    var g = (rgb >> 8) & 0xff;  // extract green
    var b = (rgb >> 0) & 0xff;  // extract blue
    var luma = 0.2126 * r + 0.7152 * g + 0.0722 * b; // per ITU-R BT.709
    if (luma >= 150 && luma < 200) {
      $('#this-week-hero').addClass("hero-color-is-medium");
    } else if (luma < 150) {
      $('#this-week-hero').addClass("hero-color-is-dark");
    }
  }

  function selectExploreNavTab() {
    var currentExploreURL = document.URL;
    var urlExploreArray = currentExploreURL.split('/');
    var page;
    if (urlExploreArray.indexOf('the-sun-times') !== -1) {
      page = 'the-sun-times';
    }
    if (urlExploreArray.indexOf('recipes') !== -1 || urlExploreArray.indexOf('recipe') !== -1 || urlExploreArray.indexOf('snack') !== -1 || urlExploreArray.indexOf('salad') !== -1 || urlExploreArray.indexOf('sauce') !== -1) {
      page = 'recipes';
    }

    if (urlExploreArray.indexOf('ingredients') !== -1) {
      page = 'recipes';
    }

    var triangle = '<svg class="triangle" height="30" width="50"><polygon points="15,0 35,0 25,10" fill="white"></polygon></svg>';
    $('.explore-subnav ul #' + page + '-tab').addClass('active').after(triangle);
  }

  function selectRecipesFilterTab() {
    var pathElements = location.pathname.split('/');
    var categoryToSelect = pathElements.length > 2 ? pathElements[pathElements.length - 1] : "20-minute";

    try {
      var $link = $('#' + categoryToSelect + '-recipe-link');
    } catch(e) {
    }

    if ($link.length) {
      $link.trigger('click');
      $('#' + categoryToSelect + 'sm-recipe-link').addClass('selected');
    }
  }

  // In #video-hero, prevent page scroll while inside .video-roll-container

  $('.video-roll-container').on('mousewheel DOMMouseScroll', function (e) {
    var e0 = e.originalEvent;
    var delta = e0.wheelDelta || -e0.detail;

    this.scrollTop += (delta < 0 ? 1 : -1) * 10;
    e.preventDefault();
  });


  function attachStickySideNav() {
    var window_top = $(window).scrollTop();
    var footer_top = $('.page-wrapper').height() + $('.page-wrapper').offset().top;
    if ($('.sticky-anchor').offset()) {
      var div_top = $('.sticky-anchor').offset().top;
    }
    var div_height = $(".sticky").height();


    div_height = $(".sticky").height() + 80;


    var padding = 20;
    var margin_top = 20;

    margin_top = 100;



    if ($(window).width() > 991) {

      if (window_top + div_height > footer_top - padding)
        $('.sticky').css({top: (window_top + div_height - footer_top + padding) * -1})
      else if (window_top > div_top) {
        $('.sticky').addClass('stick');
        $('.sticky').css({top: margin_top})
      } else {
        $('.sticky').removeClass('stick');
      }

    } else {
      $('.sticky').removeClass('stick');
    }
  }

  $(window).on("resize scroll", function (e) {
    attachStickySideNav();
  });

  $(document).ready(function () {
    isHeroColorLightOrDark('');

    attachStickySideNav();
    setTimeout(function () {
      selectExploreNavTab();
    }, 500);

    $('a.img-container').on('click', function (e) {
      window.location = $(this).attr('href');
    });
  });

</script>
