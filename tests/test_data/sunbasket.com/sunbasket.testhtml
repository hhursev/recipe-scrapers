<script>
  $(document).ready(function() {
    // Ready query params in URL
    function getUrlParameter(name) {
      name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
      var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
      var results = regex.exec(location.search);
      return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
    }

    var fromJoinFlow = getUrlParameter('m');

    if (fromJoinFlow === "1"){
      $('.sticky-back-bar').fadeIn();
    } else {
      $('.sticky-back-bar').fadeOut();
    }

    var imagesViewed = [];
    $('.swiper-button-prev, .swiper-button-next').on('click', function() {
      var $wrapper = $('.swiper-wrapper');
      if (imagesViewed.indexOf($wrapper.find('.swiper-slide-active').data('swiperSlideIndex')) === -1) {
        imagesViewed.push($wrapper.find('.swiper-slide-active').data('swiperSlideIndex'));
      }

      var total = parseInt($('.swiper-pagination-total').text());

      window.sunbasket.analytics.track('Recipe Photo Viewed', {
        via: "product_page",
        path: window.location.pathname,
        sku: 'F296',
        recipe_type: "Meal Kits",
        user_platform: 'website',
        num_photos_avail: total,
        num_photos_viewed: Math.min(total, imagesViewed.length + 1) //because we see 1 image on page load
      });
    });
  });
</script>

