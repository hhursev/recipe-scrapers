<script>
  //# sourceURL=recipe_js.jsp
  var page_type = 5;
  $(document).ready(function () {
    if ($('body').hasClass('recipePage')) {
      calcSelectorWidth($('.main-protein-selector-container .protein-list-selector'), '0');
    }

    var ua = navigator.userAgent;
    var isAndroid = (ua.toLowerCase().indexOf("android") > -1) && (ua.indexOf("sunbasketAndroidApp") < 0);
    if (isAndroid) {
      $(".button-fixed").hide();
    }

    // replace brackets with (red text) for 4 person servings
    var regex = /\[(.*?)\]/g;

    $('.instruction-description').each(function () {
      var recipeText = $(this).html();
      var regexMatch = recipeText.match(/\[(.*?)\]/g);

      if (regexMatch) {
        $.each(regexMatch, function (key, value) {
          var replacedText = value.replace('[', '(');
          replacedText = replacedText.replace(']', ')');
          recipeText = recipeText.replace(value, "<span class='four-person-serving'>" + replacedText + "</span>");
        });

        $(this).html(recipeText);
      }
    });

    moveNutrInfoForMobile();

    $("#recipe-modal").on('loaded.bs.modal', function () {
      calcSelectorWidth($('.main-protein-selector-container .protein-list-selector'), '20');
      var selectedProteinDiets = $(this).find('.protein-list-selector option:selected').data('dietTypes');
      if (selectedProteinDiets != null) {
        $(this).find('.diet-types').empty().text(selectedProteinDiets);
      }
    });

  });

  $(window).on('resize', function () {
    moveNutrInfoForMobile()
  });

  function fromBinary(encoded) {
    var binary = atob(encoded)
    const bytes = new Uint8Array(binary.length);
    for (let i = 0; i < bytes.length; i++) {
      bytes[i] = binary.charCodeAt(i);
    }
    return (new TextDecoder()).decode(bytes);
  }

  function moveNutrInfoForMobile() {
    if ($(window).width() < 992) {
      $("#nutrition-container").prependTo($("#nutrition-wrapper-sm"));
    } else {
      $("#nutrition-container").appendTo($(".sidebar-extra-content-container"));
    }
  }


  $('.protein-list-selector').on({
    'focus': function() {
      triggerProteinOptionViewedRecipe($(this));
    },
    "keyup": function(e) {
      if (e.keyCode == 27) {
        triggerProteinOptionViewedRecipe($(this));
      }
    }
  });

  $('.protein-list-selector').on('change', function () {
    var isModal = ($(this).parents('.modal-body').length > 0);
    var $parentElement = isModal ? $(this).parents('.modal-body') : $('body');

    $('.protein-list-selector', $parentElement).val($(this).val());

    var recipeId = $(this).data('id');
    var $selectedOption = $('option:selected', $(this));

    var proteinSelectedIndex = $selectedOption.data('index');
    var proteinDietTypes = $selectedOption.data('dietTypes');
    var proteinImgSrc = $selectedOption.data('imageSrc');
    var proteinCaloriesPerServing = $selectedOption.data('caloriesPerServing');
    var proteinCookTimes = JSON.parse(fromBinary($selectedOption.data('cookTimes')));

    var cookTimesCopy = '';
    proteinCookTimes.forEach(function (cookTime) {
      if (cookTime.cookTimeLow > 0) {
        cookTimesCopy += '<p><span class="bold">' + cookTime.cookTimeLow;
        if (cookTime.cookTimeLow !== cookTime.cookTimeHigh) {
          cookTimesCopy += ' &ndash; ' + cookTime.cookTimeHigh;
        }
        cookTimesCopy += '</span><span> Minutes</span>';
        if (cookTime.cookingMethod !== 'DEFAULT') {
          cookTimesCopy += ' <span>(' + cookTime.cookingMethodDescription + ')</span>';
        }
        cookTimesCopy += '</p>';
      }
    });

    if (proteinCaloriesPerServing !== '') {
      $('.recipe-info .calories-per-serving').html(proteinCaloriesPerServing);
    }

    if (cookTimesCopy !== '') {
      $('.recipe-info .cook-times').html(cookTimesCopy);
    }

    if (proteinDietTypes !== '') {
      $('.recipe-info .diet-types').text(proteinDietTypes);
    }

    $('.nutrition-info', $parentElement).hide();
    $('#nutrition-' + proteinSelectedIndex, $parentElement).fadeIn('fast');

    var swiperContainer = document.querySelector('#swiper-recipe-' + recipeId);
    if (swiperContainer) {
      var currentRecipeSwiper = swiperContainer.swiper;
      var $proteinSlideImg = $('#swiper-recipe-' + recipeId + ' .swiper-slide .slide-img[data-upgrade-match="' + proteinImgSrc + '"');
      var $proteinSlideParent = $proteinSlideImg.parents('.swiper-slide');
      var $proteinSlideParentIndex = $proteinSlideParent.length ? $proteinSlideParent.data('swiper-slide-index') : 0;


      currentRecipeSwiper.slideToLoop($proteinSlideParentIndex);
    } else {
      $('.recipe-image-container .recipe-image', $parentElement).attr('src', proteinImgSrc);
    }

    calcSelectorWidth($('.main-protein-selector-container .protein-list-selector-' + recipeId), '0');


  });

  $('.main-protein-selector-container .protein-list-selector').on('change', function () {
    calcSelectorWidth($(this), '0');
  });

  function triggerProteinOptionViewedRecipe($thisSelectElement) {
    var joinVersion = '';
    var isModal = ($thisSelectElement.parents('.modal-body').length > 0);
    var $parentElement = isModal ? $thisSelectElement.parents('.modal-body') : $('body');
    var $selectedOption = $('option:selected', $thisSelectElement);
    var proteinDietTypes = $selectedOption.data('dietTypes');
    var previousDietTypes = $('.recipe-info .diet-types', $parentElement).text();

    var props = {
      via: isModal ? 'product_modal' : 'product_page',
      protein_selected: $selectedOption.text(),
      protein_sku: $selectedOption.data('internalId'),
      protein_pantry_flag: $thisSelectElement.data('proteinPantryFlag'),
      sku: $thisSelectElement.data('recipeInternalId'),
      protein_type: $selectedOption.data('proteinType'),
      diet_tags: proteinDietTypes.split(',').map(function (r) {
        return r.trim();
      }),
      previous_diet_tags: previousDietTypes.split(',').map(function (r) {
        return r.trim();
      }),
      meal_plan: (window.sunbasket.user && window.sunbasket.user.mealPlanWithPreferences) ? window.sunbasket.user.mealPlanWithPreferences : null,
      join_flow: (joinVersion.length ? joinVersion : null),
      default_protein_type: $thisSelectElement.data('defaultProteinType'),
      recipe_type: $thisSelectElement.data('analyticsRecipeType')
    };

    try {
      var upgradeId = $selectedOption.data('id');
      var itemTags = JSON.parse(atob($thisSelectElement.data('itemTags'), 'utf8'));

      if (itemTags) {
        props.item_tags = itemTags.hasOwnProperty(upgradeId) ? itemTags[upgradeId] : itemTags[0];
      }
    } catch (ignore) {
    }

    window.sunbasket.analytics.track('Protein Option Viewed', props);
  }

</script>
</div>
