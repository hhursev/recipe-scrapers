<script>
    $('document').ready(function(){
        var height = 0;
	$('.avisAvis').each(function() {
            if(height < $(this).outerHeight()){
                height = $(this).outerHeight();
            };
        });
	$('.avisAvis').css('min-height',height+'!important');
        $('#avisRedigeTextarea').keyup(function(){
            $('.avisMsgService').empty();
            $('#numberCaractere').html($('.avisRedigeTextarea').val().length);
            if($('.avisRedigeTextarea').val().length == 300){
                $('#numberCaractere').css('color','#ca4b58');
                $('#numberCaractere').css('font-weight','bold');
            } else {
                $('#numberCaractere').css('color','black');
                $('#numberCaractere').css('font-weight','normal');
            }
        });

        $('#avisAddNew').on('click', function() {
            $('.avisRedige').parent().show();
            $('.avisMsgService').parent().hide();
        });
        $('.avisMsgService').parent().hide();

        $('#avisButton').on('click', function() {
            var commentaire = $('#avisRedigeTextarea').val();
            var message_popop = "";
            if ($.trim(commentaire) != "") {
                $.ajax({
                    type: "POST",
                    url: "https://www.atelierdeschefs.fr/fr/commentaire/save",
                    data: {
                        el: 16689,
                        message: commentaire,
                        ty: 7,
                        url: 'https://www.atelierdeschefs.fr/fr/recette/16689-crepe-savoyarde.php'
                    },
                    success: function(result) {
//                            html = '';
//                            html += "<div class='lookh2' style='margin-top: 10px;'>Merci d'avoir donné votre avis sur cette vidéo</div>";
//                            $('#avisCourslive').empty().append(html);
//                            $('.sidebar .row-fluid:last-child').hide();
                        html = '';
                        html += "<p>Merci d'avoir donné votre avis sur cette recette.</p>";
                        //Message de service
                        $('.avisMsgService').empty().append(html);
                        //Champ commentaire
                        $('#avisRedigeTextarea').val('');
                        //Champ nombre de caractère
                        $('#numberCaractere').html($('.avisRedigeTextarea').val().length);
                        $('#numberCaractere').css('color','black');
                        $('#numberCaractere').css('font-weight','normal');
                        $('.avisRedige').parent().hide();
                        $('.avisMsgService').parent().show();
                    },
                    error: function (jqXHR, textStatus, errorThrown) {

                    }
                });
            }
        });

        $('#avisMoreavis').on('click', function() {
            $.ajax({
                type: "POST",
                url: "https://www.atelierdeschefs.fr/fr/commentaire/get",
                data: {
                    el: 16689,
                    ty: 7,
                    nb: $('.blocAvis').length
                },
                success: function(result) {
                    var data = jQuery.parseJSON(result);
                    var i=0;
                    var html = '';
                    if(!jQuery.isEmptyObject(data)){
                        $(data.commentaires).each(function(){
                            html += '<div class="span12 text-left blocAvis padding10 marginB10 marginL0">';
                            html += '   <div class="span12 marginL0 avisPrenomDate">';
                            html += '       <span class="bold pink">'+data.commentaires[i].prenom+'</span> - '+data.commentaires[i].date_commentaire;
                            html += '   </div>';
                            html += '   <div class="span12 marginL0 avisAvis">';
                            html += '       '+data.commentaires[i].commentaire+'';
                            html += '   </div>';
                            html += '</div>';
                            i++;
                        });
                        console.error(html);
                        $('#areaAvis').append(html);
                    } else {
                        $('#avisMoreavis').hide();
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {

                }
            });
        });
    });
</script>
