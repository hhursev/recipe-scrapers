<script>
function delegate(el, evt, sel, handler) {
  el.addEventListener(evt, function(event) {
    var t = event.target;
    while (t && t !== this) {
      if (t.matches(sel)) {
        handler.call(t, event);
      }
      t = t.parentNode;
    }
  });
}

function rLstClick(e) {
  let clst = e.target.closest('.rlisting')
  let _a = clst.getElementsByTagName('a')[0]


  if (typeof _a !== 'undefined') {
    let uri = _a.href
    if (typeof uri !== 'undefined') {
      console.log(uri)
      window.location = uri
    }
  }
}
if (document.getElementById('rs_content')) {
  delegate(document.getElementById('rs_content'), 'click', '.rlisting', rLstClick)
}

</script>
