                    <section class="recipe-ingredients-section">
                            <h2>Ingredients</h2>

                    <ul>
                            <li><div class="ingredient-description">1 onion finely chopped</div></li>
                            <li><div class="ingredient-description">500g beef mince</div></li>
                            <li><div class="ingredient-description">1 cup water</div></li>
                            <li><div class="ingredient-description">2 beef stock cubes</div></li>
                            <li><div class="ingredient-description">1/4 cup tomato sauce</div></li>
                            <li><div class="ingredient-description">2 tsp Worcestershire sauce</div></li>
                            <li><div class="ingredient-description">1 pinch salt and pepper *to taste</div></li>
                            <li><div class="ingredient-description">3 tbs plain flour</div></li>
                            <li><div class="ingredient-description">1 sheet shortcrust pastry</div></li>
                            <li><div class="ingredient-description">1 sheet puff pastry</div></li>
                            <li><div class="ingredient-description">1 egg to glaze</div></li>
                    </ul>
    
                            </section>
