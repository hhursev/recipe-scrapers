.wp-block-button__link.convertkit-product {
	color: #f9f9f9;
	font-size: .9rem !important;
	z-index: 1;
	position: relative;
	overflow: hidden;
	display: inline-flex;
	text-align: center;
	align-items: center;
	justify-content: center;
	padding: 15px 28px 14px;
	background-color: #225545;
	border: 1px solid transparent;
	font-size: .6875rem;
	line-height: 1.72727;
	font-weight: 600;
	text-transform: uppercase;
	text-decoration: none;
	text-align: center;
	letter-spacing: .46px;
	white-space: nowrap;

}

.wp-block-button__link.convertkit-product:hover {
	    background-color: rgba(34, 85, 69, .8);
	    color: #f9f9f9;
}


section.comment-form-sce-convertkit {
padding: 5px 0 15px 0;
}

section.comment-form-sce-converkit label {
font-weight: normal;
text-transform: capitalize;
}

.is-layout-flex {
  display: flex;
  gap: 0.5em;
  flex-wrap: wrap; 
}

figure.wp-block-gallery.has-nested-images {
	margin: 1em 0;
}

P.has-medium-font-size {
	font-size: 1.1rem;
}

.header__logo-image {
/* 	width: 130px;
	max-width: unset; */
	height: 100%;
}

@media (max-width: 600px) {
	.recipe-title__affiliate {
		max-width: 200px;
	}
}

@media (min-width: 1024px) {
    .footer__navigation-item-link {
        padding: 5px 10px;
    }
}


.gform-footer svg {
    display: none;
}

.main .formkit-form {
    margin: 15px auto;
}

/*wprm modal*/

body .wprm-popup-modal__btn {
    border-radius: 0;
}

body .wprm-popup-modal__container {
    border-radius: 0;
}

body .wprm-popup-modal__btn {
    background-color: #000;
}

body .wprm-popup-modal-user-rating textarea {
	border-radius: 0;
}



body .wprm-popup-modal-user-rating .wprm-user-rating-modal-field input {
	border-radius: 0;
}

.wprm-popup-modal-user-rating textarea::placeholder {
    color: rgb(0,0,0,.5)
}

.wprm-popup-modal-user-rating textarea {
    margin: 0;
}

.wprm-user-rating-modal-comment-suggestions-after {
    padding-top: 20px;
}

/*novoshare*/
body .ns-buttons.small .ns-total-share-count {
    font-size: 16px;
    width: 40px;
}
body .ns-columns.small.ns-has-total-share-count-before .ns-buttons-wrapper {
    margin-left: 55px;
}

body .ns-buttons.small .ns-total-share-count-text {
    font-size: 10px;
}

body .ns-total-share-count-amount {
	margin-top: -3px;
}

.recipe-sidebar .ns-buttons { display: none;
}

/*instagram*/

/*fix jumpbo play button*/
body .sbi_type_video .sbi_playbtn, body .sbi_carousel_vid_first .sbi_playbtn, body .sbi_link .svg-inline--fa.fa-play {
    display: none !important;
}

/*newsletter sidebar*/
.newsletter--sidebar {
	display: none;
}

/* forms */

.gform_required_legend {
	display: none;
}

.gfield {
    margin: 10px 0;
}

.ginput_container.ginput_container_textarea textarea {
    width: 100%;
}

/*kadence*/ 
:root {
	--kb-form-success-color: #141414;
	
	--kb-form-success-background: #f6f6f6;
}

.kb-adv-form-required {
	display: none;
}

.kb-adv-form-message {
	margin-top: 20px;
}

/* app */

body .c-app-features .second-col {
    margin-top: 40px;
}

body .c-app-features {

 flex-direction: column;
}

.c-app-features .second-col li {
	padding: 0 40px 10px 40px;
}

	body .c-app-features .second-col {
		padding-bottom: 5rem;
	}

@media (min-width: 960px) {
    body .c-app-features .col {
        width:100%;
        margin-top: 20px;
			 flex-direction: column;
    }
}

@media (max-width: 600px) {
	
	body .c-app-features .second-col {
		padding: 2rem 0 4rem 0;
	}
	body .c-app-features .second-col li {
	padding: 0 10px 10px 10px;
}
	
	body.home .hero__item--cta-second .hero__bg {
		margin-bottom: .5rem;
		width: 69%;
	}
	
	
}

/*eazy toc*/
#ez-toc-container {
    background-color: #f6f6f6;
    padding: 10px;
}

p.ez-toc-title {
    margin-top: 0;
    font-weight: bold;
}

body li.ez-toc-heading-level-2:before {
    content: '';
    display: none;
}

body li.ez-toc-heading-level-2 {
    padding-left: 0px !important;
}

body li.ez-toc-heading-level-2 a {
    text-decoration: none;
}

/*insta feed*/
.instagram-feed {
    margin-top:25px
}

@media (min-width: 1440px) {
    .instagram-feed {
        margin-top:70px
    }
}



/* helpers*/

.white-text {
	color: #ffffff;
}

.font-recoleta-medium {
	font-family: recoleta-medium !important;
}

.pad-the-heading {
	padding: 0 25px;
}


fieldset.wprm-comment-ratings-container {
    vertical-align: middle;
}

.page-template-page-appland .mv-ad-box {
	margin-top: 30px;
}

html.wf-loading{visibility:visible}.image .image__img{opacity:1}
.no-object-fit .image__img{display:unset}

/*paginate comments*/
.comments .navigation {
    padding-bottom: 30px;
}

.comments .navigation .current {
    text-decoration: none;
}

body li.kadence-blocks-gallery-item:before {
    display: none;
}

/* Mediavine CSS */

.newsletter--sidebar {
position: unset !important;
top: unset !important;
}

@media only screen and (max-width: 359px) {
.page-layout__container {
padding-left: 10px !important;
padding-right: 10px !important;
}
}

/* Mediavine mobile fix CSS */
@media only screen and (max-width: 359px) {
.wprm-recipe-template-chic-v1 {
    padding-left: 0px !important;
    padding-right: 0px !important;
}
.recipe-hero__image, .recipe-hero__recipe-info {
    margin-left: unset !important;
    margin-right: unset !important;
}
}
/*End Mediavine CSS */

/* NP Recipe Info Summary spacing fix */
.recipe-hero__recipe-info-summary {
    padding: 12px 24px 24px;
    padding-top: 12px;
    padding-right: 24px;
    padding-bottom: 24px;
    padding-left: 24px;
}

/* Fixes "Cook Mode" styling on recipe cards after WPRM 8.7.5 update */
.oc-recipe-container .wprm-prevent-sleep {
 padding-left: 0;
}
.oc-recipe-container .wprm-prevent-sleep .wprm-toggle-switch {
 width: auto !important;
 position: relative;
