<script>
var moengageRecipePayload = {
	'ID': '5880',
	'nome da receita': 'CREPIOCA COM BRÓCOLIS E FRANGO',
	'url da receita': '/receita/crepioca-com-brocolis-e-frango',
	'categoria da receita': 'Prato Principal',
	'rating da receita': '0',
};
function moengageTrackRecipeRatingEvent() {
	console.log('moengageTrackRecipeRatingEvent');
	Moengage.track_event('iniciou-avaliacao-receita', moengageRecipePayload);
}
function moengageTrackRecipeRatingCancelEvent() {
	console.log('moengageTrackRecipeRatingCancelEvent');
	Moengage.track_event('terminou-avaliacao-receita', moengageRecipePayload);
}
const elementIsVisibleInViewport = (el, partiallyVisible = false) => {
	const { top, left, bottom, right } = el.getBoundingClientRect();
	const { innerHeight, innerWidth } = window;
	return partiallyVisible
	? ((top > 0 && top < innerHeight) ||
		(bottom > 0 && bottom < innerHeight)) &&
	((left > 0 && left < innerWidth) || (right > 0 && right < innerWidth))
	: top >= 0 && left >= 0 && bottom <= innerHeight && right <= innerWidth;
};

$(document).ready(function() {
	Moengage.track_event('visualizou-receita', moengageRecipePayload);
	var vTimeout;
	$(window).on('scroll', function(){
		clearTimeout(vTimeout);
		vTimeout = setTimeout(function(){
			$('#content .products li:not(.moengage-tracked)').each(function(i, e){
				if(elementIsVisibleInViewport(e)) {
					var produtoPayload = $(e).data('moengage_payload');
					produtoPayload['ID da receita'] = '5880';
					produtoPayload['nome da receita'] = 'CREPIOCA COM BRÓCOLIS E FRANGO';
					Moengage.track_event('visualizou-produto', produtoPayload).then(function(){
						e.classList.add('moengage-tracked');
					});
				}
			});
		}, 50);
	});
	loadUserCollections()

	function loadUserCollections(){
		$.ajax({
			url: Routing.generate('ajax_colecao_list'),
			type: 'GET',
			dataType: 'json'
		}).done(function (response) {
			for (var x = 0; x < response.colecoes.length; x++) {
				$('.recipe-colletions').append('' +
					'<li class="collection collection-user grid-sys one-third" id="collection-' + response.colecoes[x].guid + '">' +
						'<div class="thumb">' +
							'<a>' +
								'<img src="/_dist/images/template/blank.gif" alt="' + response.colecoes[x].titulo + '" title="' + response.colecoes[x].titulo + '" style="background-image: url(' + response.colecoes[x].imagem + '), url(/_dist/images/template/thumb-default.jpg)" />' +
							'</a>' +
							'<ul class="options">' +

								'<li><a class="btn-default btn-action btn-white btn-add-collection align-center" data-guid="' + response.colecoes[x].guid + '" data-slug="crepioca-com-brocolis-e-frango" data-title="' + response.colecoes[x].titulo + '" data-description="' + response.colecoes[x].descricao + '"><i class="fa fa-plus"></i> Adicionar a coleção</a></li>' +
							'</ul>' +
						'</div>' +
						'<a><h4 class="text-red text-center text-upper collection-title">' + response.colecoes[x].titulo + '</h4></a>' +
						'<p><i class="fa fa-cutlery"></i> ' + response.colecoes[x].receitas + ' Receitas</p>' +
						'<p><i class="fa fa-user"></i> ' + response.colecoes[x].firstName + ' ' + response.colecoes[x].lastName + '</p>' +
					'</li>' +
				'');
			}

			// Paginação
			if($('.recipe-colletions > li').length >= 6){
				var collectionsList = new List('recipe-colletions', {
					page: 6,
					pagination: true
				});

				extraControlsListJS('#recipe-colletions', collectionsList, 5);
			}
		}).fail(function (response) {
			$.fancybox.open('modal-new-collection');
		});

		return;
	}

	$(document).on("click",".btn-add-collection", function() {
		var guid = $(this).data('guid'),
			slug = $(this).data('slug'),
			collectionTitle = $(this).data('title'),
			collectionDescription = $(this).data('description')

		var moengagePayload = {
			'ID': '5880',
			'nome da receita': 'CREPIOCA COM BRÓCOLIS E FRANGO',
			'url da receita': '/receita/crepioca-com-brocolis-e-frango',
			'categoria da receita': 'Prato Principal',
			'rating da receita': '0',
			'nome da coleção': collectionTitle,
			'descrição da coleção': collectionDescription,
		};
		$.ajax({
			url: Routing.generate('ajax_colecao_receita_add'),
			type: 'GET',
			data: {
				'guid': guid,
				'receita': slug
			},
			beforeSend: function() {
				// setting a timeout
			},
			success: function (response) {
				console.log('response', response)
				$.fancybox.close();
				if (response.status) {
					//$.fancybox.open('Receita adicionada à sua coleção com sucesso!');
					Moengage.track_event('adicionou-receita-coleção', moengagePayload);
					$.fancybox.open('<div class="message">' + response.message + '</div>');

					setTimeout(function(){
						location.reload();
					}, 3000);

				} else {
					$.fancybox.open('<div class="message">' + response.message + '</div>');
				}
			}
		});

	});

	$(".modal-new-collection").on("click", function() {
		$.fancybox.close();

		$.fancybox.open( $('#modal-new-collection'), {
			clickSlide : 'false',
			clickOutside : 'false',
			touch: false
		});
	});

	$(document).on("click", ".add-collection", function() {
		//$.fancybox.close();

		var title = $('#new-collection-title').val(),
			description = $('#new-collection-description').val();
			btn = $('.add-collection');

			if(btn.attr("disabled") == 'disabled') {
				return false;
			}

			$.ajax({
				url: Routing.generate('ajax_colecao_new'),
				type: 'GET',
				data: {
					'titulo': title,
					'descricao': description
			},
			beforeSend: function() {
				// setting a timeout
				btn.text('Carregando...');
				btn.attr('disabled', true);
			},
			success: function (response) {
				btn.text('Criar Coleção');
				btn.attr('disabled', false);

				$.fancybox.close();
				$.fancybox.open('' +
					'Sua coleção foi criada com sucesso!' +
					'<br /><br />' +
					'<a class="btn-default btn-action btn-add-collection align-center" data-guid="' + response + '" data-slug="crepioca-com-brocolis-e-frango">Adicionar receita</a>' +
					'<br /><br />' +
					'<small class="align-center">ou aguarde alguns segundos para carregar a sua coleção</small>' +
				'');

					setTimeout(function(){
						location.reload();
					}, 3000);
				}

			});
	});

	/** colecoes */
	$('.load-colletions').on('click', function() {
		$.fancybox.open( $('#modal-add-recipes'), {
			clickSlide : 'false',
			clickOutside : 'false',
			touch: false
		});

	});
	/* end */

	$(document).on('click', ".fancybox-close-small", function(){
		$.fancybox.close();
	})
});


// Verificar status de Favorito da Receita
	$('.favorite-recipe').on('click', function(e) {
		var slug = $(this).data('slug');

		$.ajax({
			url: Routing.generate('ajax_colecao_isfavorita'),
			data: {
				receita: slug
			},
			type: 'GET'
		}).done(function (response) {
			if(response == false) {
				// Adicionar Receita nos Favoritos
				favRecipe(slug);
			}
			else {
				// Remover Receita dos Favoritos
				unfavRecipe(slug);
			}
			return response;
		}).fail(function (response) {
			$.fancybox.open(response);
		});

		return;
	});

	$('#weekly-recipes-email').on('focus', function(){
		Moengage.track_event('iniciou-inscrição-newsletter', {
			'tipo de newsletter': 'Cardápio semanal',
		});
	});
	$("#weekly-recipes-form").on("submit", function(e){
		e.preventDefault();
		var email = $("#weekly-recipes-email").val();
		if(email == '' || !isValid(email)){
			$.fancybox.open("Digite um e-mail válido");
			$("#weekly-recipes-email").css('border-color', 'red');
			return false;
		} else {
