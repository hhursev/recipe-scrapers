	//requisição via ajax
	function avalia(icon, data){
		$.ajax({
			url: Routing.generate('ajax_avaliacao'),
			data: data,
			type: 'POST',
			dataType: 'json',
			beforeSend: function() {
				icon.addClass('icon-disabled');
			},

		}).done(function(response){
			icon.removeClass('icon-disabled');
			//console.log(response);
			if(response.status == true){
				var avaliacao = response.avaliacao;
				var media = response.media;

				$('.content-type-evaluation').data('media', media);
				$('.content-type-evaluation').data('avaliacao',avaliacao);

				preencheAvaliacao(media, avaliacao);

				if(response.action == 'remove'){
					$.fancybox.close();
                    moengageTrackRecipeRatingCancelEvent();
				} else {
                    moengageTrackRecipeRatingEvent();
                }

				$.fancybox.open(response.msg);

			}else if(response.status == 'modal_remove'){
				$(".triggerClick").trigger('click');
				$('.btn-confirm-remove-avaliacao').data('avaliacao',response.avaliacao);
			}

		}).fail(function(response){
			$.fancybox.open(response);
		});
	}

	//preenchendo avaliação
	function percorreAvaliacao(avaliacao){
		$('.content-type-evaluation .icon-star').each(function(index, element){
			var i = index + 1;

			if(i <= avaliacao){
				$(this).removeClass('fa-star-o');
				$(this).removeClass('text-red');
				$(this).addClass('fa-star');
				$(this).addClass('text-yellow');

				if($(this).hasClass('fa-star-half-o')){
					$(this).removeClass('fa-star-half-o');
					$(this).addClass('fa-star');
				}
			}else{
				if($(this).hasClass('fa-star-half-o')){
					$(this).removeClass('fa-star-half-o');
					$(this).addClass('fa-star');
				}
				$(this).removeClass('text-yellow');
				$(this).removeClass('fa-star');
				$(this).addClass('fa-star-o');
				$(this).addClass('text-red');
			}
		});
		$('[data-toggle="tooltip"]').tooltip();
	}

	function preencheAvaliacao(media, avaliacao){
		number_format = parseFloat(media).toFixed(2);
		var m = number_format.split('.');
		var meio = parseInt(m[0]) + 1;

		$('.content-type-evaluation .icon-star').each(function(index, element){
			var i = index + 1;

			if(i <= avaliacao){
				//set icon_star  = 'fa-star'
			 	//set star_color = 'text-yellow'
				$(this).removeClass('fa-star-half-o');
			 	$(this).removeClass('fa-star-o');
			 	$(this).removeClass('text-red');
				$(this).addClass('text-yellow');
				$(this).addClass('fa-star');
			}else if(i > avaliacao && i <= media){
				//set icon_star  = 'fa-star'
				//set star_color = 'text-red'
				$(this).removeClass('fa-star-half-o');
				$(this).removeClass('fa-star-o');
				$(this).removeClass('text-yellow');
				$(this).addClass('text-red');
				$(this).addClass('fa-star');
			}else if(i == meio){
				if(parseInt(m[1]) >= 34 && parseInt(m[1]) <= 66){
					//set icon_star = 'fa-star-half-o'
					//set star_color = 'text-red'
					$(this).removeClass('fa-star');
					$(this).removeClass('fa-star-o');  //igual 2
					$(this).removeClass('text-yellow');
					$(this).addClass('fa-star-half-o');
					$(this).addClass('text-red');
				}else{
					//set star_color = 'text-red'
					//set icon_star = 'fa-star-o'
					$(this).removeClass('fa-star');
					$(this).removeClass('fa-star-half-o');
					$(this).removeClass('text-yellow');
					$(this).addClass('text-red');
					$(this).addClass('fa-star-o');
				}

			}else{
				//set star_color = 'text-red' %}
				//set icon_star = 'fa-star-o' %}
				$(this).removeClass('fa-star');
				$(this).removeClass('fa-star-half-o');
				$(this).removeClass('text-yellow');
				$(this).addClass('text-red');
				$(this).addClass('fa-star-o');
			}
		});
	}

