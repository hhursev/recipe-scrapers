	height: 0;
}
.schema-faq > div p.schema-faq-answer {
	padding-left: 16px !important;
	padding-right: 16px !important;
}
.schema-faq {
	margin-bottom: 28px;
}
.schema-faq-question.active + p.schema-faq-answer:not(.block-editor-rich-text__editable) {
	height: inherit;
	padding-top: 7px;
}.feast-remove-top-padding {
