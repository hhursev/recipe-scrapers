.wprm-recipe-template-rg-2020 h5,
.wprm-recipe-template-rg-2020 h6 {
    font-family: 'raleway', Arial, sans-serif; /*wprm_header_font_family type=font*/
    color: #212121; /*wprm_header_text type=color*/
    line-height: 1.3em; /*wprm_header_line_height type=font_size*/
    margin: 0 !important;
    padding: 0 !important;
}
.wprm-recipe-template-rg-2020 .wprm-recipe-header {
    margin-top: 1.2em !important;
}
.wprm-recipe-template-rg-2020 h1 {
    font-size: 40px; /*wprm_h1_size type=font_size*/
}
.wprm-recipe-template-rg-2020 h2 {
    font-size: 32px; /*wprm_h2_size type=font_size*/
}
.wprm-recipe-template-rg-2020 h3 {
    font-size: 24px; /*wprm_h3_size type=font_size*/
}
.wprm-recipe-template-rg-2020 h4 {
    font-size: 20px; /*wprm_h4_size type=font_size*/
}
.wprm-recipe-template-rg-2020 h5 {
    font-size: 18px; /*wprm_h5_size type=font_size*/
}
.wprm-recipe-template-rg-2020 h6 {
    font-size: 18px; /*wprm_h6_size type=font_size*/
}

.smart-points {
margin: 10px 0;
}
.smart-points span {
    background: #006cd5;
    width: 30px;
    height: 30px;
    line-height: 30px;
    border-radius: 30px;
    color: #fff;
    text-align: center;
    margin: 0;
    display: inline-block;
    font-size: 12px!important;
    letter-spacing: 0;
    font-weight: 700
}
.smart-points span.wprm-recipe-green_smartpoints {
background: #00776b; 
}
.smart-points span.wprm-recipe-purple_smartpoints {
background: #960e68; 
}
a.wprm-recipe-print, a.wprm-recipe-pin, a.wprm-recipe-link-button {
    display: block;
    text-transform: uppercase;
    font-size: 12px;
    font-weight: 700;
    letter-spacing: 1px;
    cursor: pointer;
    margin-top: 12px;
    padding: 10px 15px;
    text-decoration: none;
    color: #fff!important;
    background: #3a3a3a;
}
.entry-content .wprm-recipe a.wprm-recipe-print, .entry-content .wprm-recipe a.wprm-recipe-pin  {
    color: #fff!important;
}

.wprm-recipe-print svg path, .wprm-recipe-pin-icon svg path {
    fill: #fff!important;
}
a.wprm-recipe-print:hover, a.wprm-recipe-pin:hover, a.wprm-recipe-link-button:hover {
	background: #307C88!important;
    -webkit-transition: all .2s ease-in-out;
    -moz-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
}
.recipe-card-save {
    display: block;
    text-transform: uppercase;
    font-size: 12px;
    font-weight: 700;
    letter-spacing: 1px;
    cursor: pointer;
    margin-top: 5px;
    padding: 5px 15px;
    text-decoration: none;
    color: #3a3a3a;
    background: transparent;
    border: 2px solid #3a3a3a;
}
.recipe-card-save:hover {
color: #307C88;
border: 2px solid #307C88;
    -webkit-transition: all .2s ease-in-out;
    -moz-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
