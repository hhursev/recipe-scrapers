                        }

                        let suggestion_item = document.createElement("li");
                        suggestion_item.appendChild(suggestion_item_link);

                        suggestions_list.appendChild(suggestion_item);
                    });

                    suggestions_container.appendChild(suggestions_list);
                }
                else
                {
                    let suggestions_no_results = document.createElement("p");

