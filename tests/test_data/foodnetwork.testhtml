                id = site[0];
                title = site[1];
                icon = site[2];
                url = site[3];

                siteObject = {id: id, title: title, icon: icon, postUrl: url};

                for (var i=0; i<me.sites.length; i++)
                {
                    if (me.sites[i].id == id)
                    {
                        me.sites.splice(i, 1, siteObject);
                        spliced = true;
                        break;
