                    suggestions_container.appendChild(suggestions_no_results);
                }

                search_field_container.appendChild(suggestions_container);
                suggestions_container.style.display = "block";
            }
        }
    }

    function getSuggestions()
    {
        if (this.value.length > 2)
        {
            let url = "https://uk-api.loma-cms.com//feloma/search/page/?q=" + this.value + "&environment=foodnetwork&page_size=10";

            if(!http_suggestions_request)
            {
                console.error("Error: Cannot create an http request!");
                return false;
            }

            http_suggestions_request.onreadystatechange = printSuggestions;
            http_suggestions_request.open("GET", url);
            http_suggestions_request.send();
        }
        else
        {
            suggestions_container.style.display = "none";
        }
    }

