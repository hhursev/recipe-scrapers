    function printSuggestions()
    {
        if(http_suggestions_request.readyState === XMLHttpRequest.DONE)
        {
            if(http_suggestions_request.status === 200)
            {
                let response_json = http_suggestions_request.responseText;
                let response_parsed = JSON.parse(response_json);
                let suggestions_results = response_parsed.data;

                let suggestions_container_title = document.createElement("h5");
                suggestions_container_title.textContent = translations.SEARCH_RESULTS;
                suggestions_container.innerHTML = "";
                suggestions_container.appendChild(suggestions_container_title);

                if(suggestions_results.length > 0)
                {
                    let suggestions_list = document.createElement("ul");

                    suggestions_results.forEach(function(item)
                    {
                        let suggestion_item_link = document.createElement("a");
                        suggestion_item_link.textContent = item.title;

                        suggestion_item_link.href = "/" + item.slug;
                        if(item.hasOwnProperty("parentSlug") && item.parentSlug != "")
                        {
