<script>
  let input = document.querySelector('#mainValue');
  let res = document.querySelector('#outResult');
  let from = document.querySelector('#convertFrom');
  let to = document.querySelector('#convertTo');
  let converter_group_volumes_weights = document.querySelectorAll("#convertTo .converter_group_volumes_weights");
  let converter_group_temperatures = document.querySelector("#convertTo .converter_group_temperatures");
  let conversionsTables = {
    temperatures: {
      c: {
        toK: (value) => { return value + 273.15; },
        fromK: (value) => { return value - 273.15; }
      },
      f: {
        toK: (value) => { return (value - 32) * 5/9 + 273.15; },
        fromK: (value) => { return (value - 273.15) * 9/5 + 32; }
      }
    },
    others: {
      kg: 1,
      gr: 1000,
      mg: 1000000,
      lbs: 2.20462,
      oz: 35.274,
      lt: 1,
      usg: 0.264172,
      usq: 1.05669,
      usp: 2.11338,
      usc: 4.16667,
      uson: 33.814,
      ust: 67.628,
      uste: 202.884,
      mc: 0.001,
      ml: 1000,
      iga: 0.219969,
      ipi: 1.75975,
      icu: 3.51951,
      fou: 35.1951,
      tsp: 67.628,
      tea: 56.3121,
      sob: 8.45351
      }
  };

  let handleInput = (e) => {
    let str_from = from.options[from.selectedIndex].text;
    let str_to = to.options[to.selectedIndex].text;
    let final_result;
    let temperaturesUnits = ['f','c'];


    if (e.target.name === 'convertFrom') {
      if (temperaturesUnits.includes(e.target.value)) {
        converter_group_volumes_weights.forEach(item => {
          item.disabled = true;
        });
        converter_group_temperatures.disabled = false;
      } else {
        converter_group_volumes_weights.forEach(item => {
          item.disabled = false;
        });
        converter_group_temperatures.disabled = true;
      }
    }
    if (!input.value) {
      input.value = 1;
    }
    if (from.value !== '' && to.value !== '') {
      if (temperaturesUnits.includes(from.value)) {
        final_result = conversionsTables.temperatures[to.value].fromK(
          conversionsTables.temperatures[from.value].toK(parseFloat(input.value))
        );
      } else {
        final_result = parseFloat(input.value) / conversionsTables.others[from.value] * conversionsTables.others[to.value];
      }
      if (input.value != null || input.value !== '') {
        final_result = Number.parseFloat(final_result).toFixed(3);
      }
      res.innerHTML = `${input.value} ${str_from}<strong>= ${final_result} ${str_to}</strong>`;
    }
  };
  from.onchange = handleInput;
  to.onchange = handleInput;
  input.oninput = handleInput;
</script>
