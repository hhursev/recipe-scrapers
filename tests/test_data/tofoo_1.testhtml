    <section class="recipe-detail block-t-center">
      <h1 class="recipe-detail__title h3 blue">Banh Mi</h1>
                  <div class="recipe-detail__desc">Serves 1 • Prep 30 min • Cooking 5 min</div>
                    <div class="recipe-detail__ins h6"> Vegan</div>
      
      

    </section>
