        <script type="text/javascript">
    ((() => {
        const darkModeCheckbox = document.querySelector('#darkmode');

        if (!darkModeCheckbox) {
            return;
        }

        const CODE = 'iddqd';

        let buffer = '';

        window.addEventListener( 'keypress', ((event) => {

            buffer += event.key;

            if (CODE.indexOf(buffer) === -1) {
                buffer = '';
            }

            if (buffer === CODE) {
                darkModeCheckbox.checked = !darkModeCheckbox.checked;
                buffer = '';
            }
        }));

    })());
</script>
