body .userway.userway_p5 {
	margin-bottom: 100px;
}

.comment-form p input#wpgdprc {
	  width: 15px;
	  margin-left:0px;
}

.comment-form-cookies-consent { display:none;}

.single-post > p:first-of-type {
  font-size: 1.2em;
	font-style: italic;
	line-height: 1.1em;
}

.af-form-wrapper .required.error {
	box-shadow: 0px 0px 2px #cc0000;
}
.af-form-wrapper .error_msg {
	text-align: center;
	color: #cc0000;
}

.newbox h3.widget-title { text-align:center;}

.newbox p {text-align:center; color: #858685;}

.newbox {border:1px solid #eaeaea; padding:20px;}

/* Books sidebar widget */
.book .cover-image {
	display: table-cell;
	vertical-align: middle;
	min-width: 125px;
}
.book .order-now {
	display: table-cell;
	vertical-align: middle;
	padding: 0 15px;
}

/* Cookbook Index */

.page-id-42708 h1.pagetitle {
	display: none;
}
.cookbook-index-print,
.cookbook-index-download {
	letter-spacing: normal;
	margin-left: 7px;
	padding: 3px;
	vertical-align: bottom;
}
.cookbook-index-print .fa,
.cookbook-index-download .fa {
	border-radius: 50%;
	color: #FFF;
	font-size: 17px;
	text-align: center;
	line-height: 32px;
	height: 32px;
	width: 32px;
}
.cookbook-index-print .fa {
	background-color: #319584;
}
.cookbook-index-download .fa {
	background-color: #A97FA3;
}
.cookbook-index-search {
	margin: 25px 0;
	display: inline-block;
}
.cookbook-index-icon-container {
	display: inline-block;
	margin-left: 15px;
}
.cookbook-index-legend {
	text-align: right;
}
.ci_cookbook_filter, .ci_smartpoints_filter,
.ci_spv_container {
	display: inline-block;
	font-size: 14px;
	margin-right: 5px;
	text-transform: uppercase;
}
.ci_cookbook_filter_label,
.ci_smartpoints_filter_label {
	display: inline-block;
	font-size: 22px;
	font-weight: bold;
	margin-right: 10px;
	min-width: 150px;
}
.ci_cookbook_filter_desc {
	font-size: 15px;
	font-style: italic;
	margin-left: 160px;
	padding: 5px 0;
	line-height: 1em;
}
a.ci_cb,
a.ci_cb_all {
	background-color: #999;
	color: #FFF;
	display: block;
	padding: 3px 10px;
	text-decoration: none;
}
a.ci_cb.active,
a.ci_cb_all.active {
	background-color: #f06f13;
}
.ci_smartpoints_filters {
	display: inline-block;
}
a.ci_sp,
a.ci_sp_all {
	color: #FFF;
	display: block;
	padding: 3px 30px;
	text-decoration: none;
	opacity: 0.2;
}
a.ci_sp.sp-blue {
	background-color: #006cd5;
}
a.ci_sp.sp-green {
	background-color: #00776b;
}
a.ci_sp.sp-purple {
	background-color: #960e68;
}
a.ci_sp_all {
	background-color: #f06f13;
}
a.ci_sp.active,
a.ci_sp_all.active {
	opacity: 1;
}
.ci_spv_container {
	display: inline-block;
	position: relative;
}
.ci_spv_filter_label {
	display: inline-block;
	margin-right: 0;
}
a.ci_spv_anchor {
	display: block;
	background-color: #999;
	padding: 3px 20px;
	color: #FFF;
	text-decoration: none;
}
.ci_smartpoint_value_filters {
	position: absolute;
	text-align: center;
	width: 100%;
	background-color: #999;
	display: none;
	z-index: 9;
}
.ci_smartpoint_value_filters a.ci_spv {
	display: block;
	color: #FFF;
	padding: 3px;
	text-decoration: none;
}
.ci_smartpoint_value_filters a.ci_spv:hover, .ci_smartpoint_value_filters a.ci_spv.active {
	background-color: #AAA;
}
@media screen AND (max-width:768px){
	.ci_cookbook_filter_label,
.ci_smartpoints_filter_label{
		display: block;
	}
	.ci_cookbook_filter_desc{
		margin-left: 0;
	}
}
@media screen AND (max-width:399px){
	a.ci_sp, a.ci_sp_all {
		padding: 3px 10px;
	}
	a.ci_spv_anchor {
		padding: 3px 8px;
	}
}
.cookbook-index-search {
	position: relative;
	width: 70%;
}
.cookbook-index-search input {
	background-color: #f5f5f5;
	border: 0;
	font-size: 16px;
	padding: 9px 10% 9px 9px;
	width: 100%;
	-webkit-appearance: none;
}
#ci_search_icon {
	color: #999;
	display: block;
	line-height: 1em;
	padding: 8px 12px;
	position: absolute;
	text-align: center;
	top: 0;
	right: 0;
	z-index: 1;
}
#ci_search_icon.fa-times {
	cursor: pointer;
}
.category-index-section h3 {
	margin-top: 10px;
}
.category-index-table {
	border: none;
	font-size: 15px;
	line-height: 1.3em;
	margin-bottom: 35px;
	text-align: left;
}
.category-index-table th {
	background-color: #F9F9F9;
	font-size: 16px;
	text-transform: uppercase;
}
.category-index-table td,
.category-index-table th {
	border: none;
	padding: 5px;
}
.category-index-table th.sp-title {
	text-align: center;
}
.category-icons {
	display: inline-block;
	vertical-align: middle;
}
.category-icon {
	margin-left: 4px;
}
.category-index-table td.cell-recipe {
	width: 50%;
}
.category-index-table td.cell-cookbook {
	width: 20%;
}
.category-index-table td.cell-type {
	width: 18%;
}
.category-index-table td.cell-page-number {
	width: 5%;
}
.category-index-table td.cell-smart-points {
	width: 5%;
	white-space: nowrap;
}
.category-index-table .myww-smartpoints {
	display: inline-block;
	color: #FFF;
	font-size: 0.85em;
	line-height: 1em;
	margin: 0 1px;
	padding: 5px 5px 4px;
	width: 22px;
	text-align: center;
}
.category-index-table .sp-blue {
	background-color: #006cd5;
}
.category-index-table .sp-green {
	background-color: #00776b;
}
.category-index-table .sp-purple {
	background-color: #960e68;
}
.category-index-table .ci_cb,
.category-index-table .ci_cb_all {
  color: #FFF;
  background-color: #999;
  margin-right: 3px;
  padding: 3px;
}
.category-index-table .ci_cb.active,
.category-index-table .ci_cb_all.active {
  background-color: red;
}
.category-index-table .row-recipe.inactive {
  display: none;
}
/* Cookbook Index PDFs */
#cookbook_pdf_wrap {
	background-color: #F2F2F2;
	margin-bottom: 10px;
	padding: 3px 35px;
	position: relative;
}
.cookbook-pdf-container {
	display: flex;
	flex-wrap: nowrap;
	justify-content: center;
}
.cookbook-pdf-column {
	padding: 15px 5px 0;
}
.cookbook-pdf-title {
	border-bottom: 1px dashed #ddd;
	font-size: 14px;
	line-height: 1.3em;
	margin: 0 5px 5px;
	text-align: center;
}
.cookbook-pdf-column ul {
	margin: 0;
	padding: 0;
	list-style-type: none;
}
.cookbook-pdf-column ul li {
	font-size: 15px;
	text-align: center;
}
.cookbook-pdf-column ul li a {
	color: #666;
	display: block;
	padding: 8px 5px;
	text-decoration: none;
	line-height: 1.2em;
}
.cookbook-pdf-column ul li a:hover {
	background-color: #CCC;
}
#cookbook-pdf-index-close {
	position: absolute;
	cursor: pointer;
	top: 5px;
