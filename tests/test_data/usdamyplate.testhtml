    <script>
      (function () {
        const username = localStorage.getItem('mp_username');
        var mpWelcomeAuth = document.querySelectorAll(".welcome-auth"),
          mpWelcomeAnon = document.querySelectorAll(".welcome-anon"),
          i;
        if (username) {
          var mpUsername = document.querySelectorAll(".mp-username");
          for (i = 0; i < mpUsername.length; i++) {
            mpUsername[i].innerHTML = decodeURIComponent(username);
          }
        }
        for (i = 0; i < mpWelcomeAuth.length; i++) {
          mpWelcomeAuth[i].style.display = username ? "block" : "none";
        }
        for (i = 0; i < mpWelcomeAnon.length; i++) {
          mpWelcomeAnon[i].style.display = username ? "none" : "block";
        }
      })();
    </script>

