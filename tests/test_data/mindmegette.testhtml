            });
        };

        me.all_to_cart = function (button) {
            $(button).parent().toggleClass('all-in-cart');

            var isAllToCart = $(button).parent().hasClass('all-in-cart');
            if (isAllToCart) {
                $('.add-to-cart').parent().addClass('in-cart');
            } else {
                $('.in-cart').removeClass('in-cart');
            }
            me.state_refresh();

            var cartRow = $(button).attr('data-info');
            var cartRecipeID = $(button).attr('data-recipe_id');
