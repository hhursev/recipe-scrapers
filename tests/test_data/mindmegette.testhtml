		<script type="application/ld+json">{"@context":"http:\/\/schema.org","@type":"Recipe","author":"MME","cookTime":"PT45M","description":"Egy finom Tepsis krumpli c\u00e9kl\u00e1val \u00e9s r\u00e9p\u00e1val eb\u00e9dre vagy vacsor\u00e1ra? Tepsis krumpli c\u00e9kl\u00e1val \u00e9s r\u00e9p\u00e1val Receptek a Mindmegette.hu Recept gy\u0171jtem\u00e9ny\u00e9ben!","image":"https:\/\/www.mindmegette.hu\/images\/283\/O\/tepsis-ceklas.jpg","recipeIngredient":["6 db burgonya","2 db c\u00e9kla","8 db r\u00e9pa","4 db lilahagyma","6 gerezd fokhagyma","friss rozmaring","s\u00f3","bors"],"name":"Tepsis krumpli c\u00e9kl\u00e1val \u00e9s r\u00e9p\u00e1val Recept k\u00e9ppel - Mindmegette.hu - Receptek","recipeCategory":"egyt\u00e1l\u00e9tel,k\u00f6ret","keywords":["Tepsis krumpli c\u00e9kl\u00e1val \u00e9s r\u00e9p\u00e1val","veget\u00e1ri\u00e1nus","klasszikus","eb\u00e9d-vacsora otthonra","\u00e1tlagos","egyszer\u0171","45 perc","egyt\u00e1l\u00e9tel","k\u00f6ret","krumplis","tepsis","c\u00e9kl\u00e1s","burgonya","rozmaringos - MME"]}</script><script>


/* INGR: Hozzávalók dobozát pakolja át képernyőméretnek megfelelő szükséges helyre
 *
 * Használat: az INGR.init();-et hívjuk meg a recept alatt. A többit intézi saját maga.
 */

var INGR = INGR || {};

INGR.splitWidthAt = 992;
INGR.SmallPlaceId = "ingradientsPlaceOriginal";
INGR.LargePlaceId = "ingradientsPlaceSide";
INGR.MovedChildId = "ingradientsAndBanners";

INGR.boxRepositioned = false;
INGR.Recipe = {};

INGR.init = function() {
	var me = this;
	this.checkBoxPosition();
	window.addEventListener("resize", function(){ me.checkBoxPosition();});

	this.Recipe = new Recipe(); // init add-to-cart functions
};

INGR.isLargerWidth = function( width ) {
	return window.innerWidth > width;
};

INGR.checkBoxPosition = function() {
	var isLargerWidth = this.isLargerWidth( this.splitWidthAt );

	if (this.boxRepositioned !== isLargerWidth ) {
		this.moveChild(	isLargerWidth ? this.LargePlaceId : this.SmallPlaceId , this.MovedChildId );
		this.boxRepositioned = isLargerWidth;
	}
};

INGR.moveChild = function( destinationParentId, childId ) {
	document.getElementById( destinationParentId ).appendChild(
		document.getElementById( childId )
	);
};


var Recipe = function(){

	var me = this;

	me.init = function(){
		me.init_buttons();
		me.state_refresh();
		return me;
	};

	me.all_to_cart_button_state_refresh = function(){
		var itemsInCart = $('.in-cart').length;
		var itemsInList = $('.add-to-cart').length;

		if ( itemsInList === itemsInCart ) {
			$(".all-to-cart").parent().addClass('all-in-cart');
		} else {
			$(".all-to-cart").parent().removeClass('all-in-cart');
		}
	};

	me.cart_item_num_refresh = function(){
		$('.cart-items-num').text($('.in-cart').length);
	};

	/**
	 * Frissíti az összes elem állípotát
	 *
	 * @returns nothing
	 */
	me.state_refresh = function(){
		me.cart_item_num_refresh();
		me.all_to_cart_button_state_refresh();
	};

	me.init_buttons = function(){
		console.log("Recipe_obj.init_buttons");
        $('.add-to-cart').off('click.recipe').on('click.recipe', function(){
            me.add_to_cart(this);
        });

		$('.all-to-cart').click(function(){
			me.all_to_cart(this);
		});

		$('.add-to-folder').click(function(){
			me.add_to_folder($(this).attr('data-folder_id'));
		});

		$('#add_selection').click(function(){
			$("#add_selection").hide();
			$("#new_folder").removeClass('hide');
		});

		$('#new_folder_name').keyup(function (e) {
			if (e.keyCode === 13) {
				var foldername = e.target.value;
				me.create_folder(foldername);
			}
		});

		$('.stars li').click(function(){
			me.update_rating($(this).attr('data-star_value'));
		});

		$("#cartBtn").click(function(){
			window.location.href='/bevasarlolista';
		});
	};

	me.add_to_folder = function(folder_id){

		post_data = {
			'folder_id' : folder_id,
			'recipe_id' : $('#recipe_id').val(),
                        'usri':$("#user_nav").attr("data-usri")
		}

		$.ajax({
			type: "POST",
			url: "/receptek/recipeToFolder",
			data: post_data,
			success: function(msg) {
				me.after_folder_add(msg);
			},
			dataType:'JSON'
		});
	};

	me.create_folder = function(folder_name){

		var post_data = {
			'folder_name' : folder_name,
			'recipe_id' : $('#recipe_id').val(),
        'usri':$("#user_nav").attr("data-usri"),
		};

		$.ajax({
			type: "POST",
			url: "/receptek/createFolder",
			data: post_data,
			success: function(msg) {
				me.after_folder_add(msg);
			},
			dataType:'JSON'
		});

	};

	me.update_rating = function(value, recipe_id){

		post_data = {
			'rating_value' 	: value,
			'recipe_id'		: $('#recipe_id').val(),
			'modal_id'		: 'update_rating',
			'ok_id'		: 'update_rating_ok'
		}

		$.ajax({
			type: "POST",
			url: "/receptek/ratingRecipe",
			data: post_data,
			success: function(msg) {
				me.after_update_rating(msg);

				$('#update_rating_ok').click(function(){
					$('#update_rating').foundation('reveal', 'close');
				});
			},
			dataType:'JSON'
		});
	};

	me.after_folder_add = function(msg){

		var post_data = {
			'template' : 'folder_added',
			'tplVars' : {
				'modal_id' : 'folder_added',
				'ok_id': 'folder_added_ok'
			}
		};

      $.ajax({
          type: "POST",
          url: "/modal",
          data: post_data,
          success: function (html) {
              getUserInfo();
              $('#folder_added').remove();
              $('body').append(html);
              $('#folder_added').addClass('show');// foundation('reveal', 'open');

              $('#folder_added_ok').click(function () {
                  $('#folder_added').removeClass('show');// foundation('reveal', 'close');
              });
          },
          dataType: 'HTML'
      });

		$("#add_selection").show();
		$("#new_folder").addClass('hide');
	};


    me.after_update_rating = function (msg) {
        var post_data = {
            'template': 'rating_updated',
            'tplVars': {
                'modal_id': 'rating_updated',
                'modal_id': 'update_rating',
                'ok_id': 'update_rating_ok'
            }
        };

        $.ajax({
            type: "POST",
            url: "/modal",
            data: post_data,
            success: function (html) {
                $('#rating_updated').remove();
                $('body').append(html);
                $('#rating_updated').addClass('show');

                $('#update_rating_ok').click(function () {
                    $('#update_rating').removeClass('show');
                });

                if (typeof getRecipeRating !== 'undefined') {
                    getRecipeRating();
                }
            },
            dataType: 'HTML'
        });
    };

	me.add_to_cart = function(button){
		$(button).parent().toggleClass('in-cart');
		me.state_refresh();
        var cartRow = $(button).attr('data-info');
        var cartRecipeID = $(button).attr('data-recipe_id');
        $.ajax({
			type: "POST",
			url: "/receptek/ajaxAddToCart",
			data: {
                recipeID: cartRecipeID,
                cartRow: cartRow
            },
			success: function(data) {
			},
			dataType:'HTML'
		});
	};

	me.all_to_cart = function(button){
		$(button).parent().toggleClass('all-in-cart');

		var isAllToCart = $(button).parent().hasClass('all-in-cart');
		if( isAllToCart ) {
			$('.add-to-cart').parent().addClass('in-cart');
		} else {
			$('.in-cart').removeClass('in-cart');
		}
		me.state_refresh();

        var cartRow = $(button).attr('data-info');
        var cartRecipeID = $(button).attr('data-recipe_id');
        $.ajax({
			type: "POST",
			url: "/receptek/ajaxAddToCart",
			data: {
                recipeID: cartRecipeID,
                cartRow: cartRow,
				isAllToCart: isAllToCart ? 1 : 0
            },
			success: function(data) {
				console.log(data);
			},
			dataType:'HTML'
		});
		console.log("all_to_cart(): r_id:"+ cartRecipeID+" isAllToCart:"+isAllToCart);
	};

	me.init();
};

/**
 * @author gergely.racz
 *
 * A recept értékeléseinek html-jét frissíti, aktuális átlag értékelés jelzésével egyben.
 */
var getRecipeRating = function () {
    $.ajax({
        type: "POST", cache: false,
        url: "https://www.mindmegette.hu/get_recipe_stars/",
        data: {'recipe_id': $('#recipe_id').val()},
        success: function (returned) {
            var parsedData = JSON.parse(returned);
            if (typeof parsedData.ratingHtml !== 'undefined' && $(".dropdown-menu.stars").length > 0) {
                $(".dropdown-menu.stars").html(parsedData.ratingHtml);
            }
        }
    });
}

</script>

