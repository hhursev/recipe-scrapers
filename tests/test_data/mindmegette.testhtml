                goa_blocks.push({
                    node: $(this),
                    id: $(this).attr("id").substr(block_type.length),
                    p: typeof($(this).data("priority")) !== "undefined" ? $(this).data("priority") : 100
                });
                blocks.push({
                    elementId: $(this).attr("id"),
