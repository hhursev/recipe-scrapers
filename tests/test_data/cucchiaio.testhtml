        <div id="cercainput_comp"><form action="/ricerca.html" method="GET" id="searchForm"><input name="full" type="hidden"><input name="_charset_" type="hidden" value="UTF-8"><input name="key" type="text" id="key" autocomplete="off"></form><div id="autocomplete" style="display:none; background-color: white;"><ul></ul></div></div>

        <script type="text/javascript">

            var isOldSearch = false;

            var timer=null;

            var isCtrl = false;

             $("#key").keydown(function (event) {
                 event = event||window.event // IE support
                    var c = event.keyCode
                    var ctrlDown = event.ctrlKey||event.metaKey // Mac support

                    if (ctrlDown && c==86)
                        isCtrl = true;
                    else
                        isCtrl = false;
            });

             $("#key").keyup(function(event){
                if (!isCtrl) {
                    clearTimeout(timer);

                    if ($(this).val().length > 2) {
                        timer = setTimeout(function(){doSearch()}, 1000);
                    }
                }
             });

            $("#key").on('paste', function () {
                  var element = this;
                  setTimeout(function () {doSearch()}, 100);
                });

            $("#autocomplete").focusout(function() {
                $("#autocomplete").css("display","none");
              });

            var mouse_is_inside = false;

            $(document).ready(function()
            {
                $("#autocomplete").hover(function(){
                    mouse_is_inside=true;
                }, function(){
                    mouse_is_inside=false;
                });

                $("body").mouseup(function(){
                    if(!mouse_is_inside) $("#autocomplete").hide();
                });
            });

            function doSearch(){
                if(isOldSearch){
                	doOldSearch();
                }else{
                    doNewSearch();
                }
            }

            function doNewSearch(){

                var init = new Date().getMilliseconds();
                 $("#autocomplete").css("display","none")
                 $("#autocomplete ul li").remove();

                var op = "suggest"; //operation
    			var num = 10; //number of results
    			var param = $("#key").val();
                var f = "rich"; //format
    			var t = "cda"; //search type
   				var charset = "UTF-8";

                $.ajax({
                        type: "POST",
            			dataType: "json",
            			async: true,
            			cache: false,
            			data: {op:op, param:param, f:f, num:num, t:t, _charset_:charset},
