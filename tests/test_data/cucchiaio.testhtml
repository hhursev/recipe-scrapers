    /*LISTNER EVENTO noAdvMpopoverlay for desktop and mobile*/
    if(overlayFooterActive=="1"){
    	document.addEventListener('noAdvMpopoverlay', callAdFloor,false);

    }

    function callAdFloor(e) {
        /* LISTNER SU EVENTO SCROLL PER INTERCETTARE IL PRIMO SCROLLING UP */
        previous = window.scrollY;
        document.addEventListener('scroll', scrollUpFloorAdAdagio, false);
    }

    function scrollUpFloorAdAdagio(){

        var element = isMobileFlag ? "MFLOOR" : "FLOOR";

		   if(window.scrollY < previous){
		        // SCROLL UP
		        _edidomus.B.setFormat(formatAdagio);
		        _edidomus.B.setBaseLocation(locationAdagio);
		        _edidomus.B.setPositionId(['@BODY']);
		        _edidomus.B.setElementId([element]);
		        _edidomus.B.setKeyword(keywordAdagio);
		        _edidomus.B.makeCall();

		        // LA CHIAMATA VA EFFETTUATA SOLO AL PRIMO SCROLL UP, RIMUOVO LISTNER
		        document.removeEventListener('noAdvMpopoverlay',callAdFloor,false);
		        document.removeEventListener('scroll',scrollUpFloorAdAdagio,false);
		   }
			 previous = window.scrollY;
	}

