type":"TEXT","id":"","nodes":[],"textData":{"text":"Duchess Potatoes","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"BULLETED_LIST","id":"zgtk78p4oorax576987","nodes":[{"type":"LIST_ITEM","id":"yge188p4oorax576977","nodes":[{"type":"PARAGRAPH","id":"1blea142474","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"2 pounds yukon gold potatoes, peeled, cut into 1 inch cubes","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"8c9bq8p4oorax576979","nodes":[{"type":"PARAGRAPH","id":"3swgi142477","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"6 tablespoons (180g) unsalted butter","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"vyt5a8p4oorax576981","nodes":[{"type":"PARAGRAPH","id":"xl8kn142480","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":" cup (60g) heavy cream","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"zo5gk8p4oorax576983","nodes":[{"type":"PARAGRAPH","id":"hvz7k142483","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"4 egg yolks","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"z38jy8p4oorax576985","nodes":[{"type":"PARAGRAPH","id":"ipita142486","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Salt and pepper to taste","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"t4m4v142488","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"ORDERED_LIST","id":"bffi28p4oorax576999","nodes":[{"type":"LIST_ITEM","id":"fffwm8p4oorax576989","nodes":[{"type":"PARAGRAPH","id":"iqan4142491","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Begin by placing potatoes into a medium saucepan filled with enough water to cover the potatoes. Season water generously with salt and bring the water to a boil. Once boiling, reduce heat to a simmer. Cook for about 20-22 minutes, or until the potatoes fall apart when poked by a fork.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"o5e7u8p4oorax576991","nodes":[{"type":"PARAGRAPH","id":"j88ym142494","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"While the potatoes are cooking , preheat the oven to 425F (218C) and line a half baking sheet with parchment paper.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"gnux98p4oorax576993","nodes":[{"type":"PARAGRAPH","id":"05x8a142497","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Drain the cooked potatoes and pass through a potato ricer. Add the riced potatoes back into the pot they were cooked in along with heavy cream, and 4 tablespoons of butter. Using a spatula, fold to combine.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"3x36s8p4oorax576995","nodes":[{"type":"PARAGRAPH","id":"nrnj1142500","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Once the potatoes have slightly cooled, begin to fold in egg yolks, working one at a time. Once all of the egg yolks have been added and the mixture is completely homogenous, add mixture to a piping bag with star shaped tip.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"rktou8p4oorax576997","nodes":[{"type":"PARAGRAPH","id":"atnw3142503","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Pipe the potatoes onto the parchment paper lined baking tray into 3 in diameter swirls. Place the baking tray into the oven and bake until golden brown, about 15-20 minutes.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"orderedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"pjpjl142505","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"dbja6142506","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Potato Casserole","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"BULLETED_LIST","id":"5tguc8p4oorax577018","nodes":[{"type":"LIST_ITEM","id":"amgp78p4oorax577002","nodes":[{"type":"PARAGRAPH","id":"2or88142510","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"2.5 lbs (1.2kg) russet potatoes","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"ydtia8p4oorax577004","nodes":[{"type":"PARAGRAPH","id":"9dwe3142513","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"2  cup (550ml) milk","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"ejb9g8p4oorax577006","nodes":[{"type":"PARAGRAPH","id":"5wiou142516","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"2  cup (550ml) heavy whipping cream","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"x4yiy8p4oorax577008","nodes":[{"type":"PARAGRAPH","id":"i7h6f142519","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"4 cloves garlic","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"kzpye8p4oorax577010","nodes":[{"type":"PARAGRAPH","id":"jdtgw142522","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"3 sprigs thyme","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"7bq908p4oorax577012","nodes":[{"type":"PARAGRAPH","id":"c9ivl142525","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"4 oz (114g) gruyere cheese, grated","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"nnp2d8p4oorax577014","nodes":[{"type":"PARAGRAPH","id":"wxurb142528","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Salt and pepper to taste","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"o1n5i8p4oorax577016","nodes":[{"type":"PARAGRAPH","id":"by3r1142531","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Baking spray to grease","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"4menw142533","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"ORDERED_LIST","id":"svtkz8p4oorax577032","nodes":[{"type":"LIST_ITEM","id":"vtyix8p4oorax577020","nodes":[{"type":"PARAGRAPH","id":"3mb46142536","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Preheat your oven to 375F (245C). Coat a 9x13 baking dish with baking spray.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"870rd8p4oorax577022","nodes":[{"type":"PARAGRAPH","id":"5v089142539","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Add your milk and heavy cream to a large saucepan and bring to a boil over medium high. As soon as it starts to boil, immediately reduce to medium low. Season your cream mixture with salt and pepper to taste. Its important to taste at this step as its the basis of flavor for your casserole. Just barely underseason it.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"c9t5p8p4oorax577024","nodes":[{"type":"PARAGRAPH","id":"8qo35142542","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Peel your potatoes and slice into rounds about the thickness of a coin. A mandoline for slicing is highly recommended in order to keep the slices the same thickness. Add potatoes immediately to your simmering cream along with the thyme. Simmer for about 5 minutes or just barely until fork tender. Adjust salt levels at this point if it needs more.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"06ooz8p4oorax577026","nodes":[{"type":"PARAGRAPH","id":"n1ncu142545","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Cut off the heat, remove your thyme, add in your garlic and lightly swirl the pan to distribute the garlic.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"svb6s8p4oorax577028","nodes":[{"type":"PARAGRAPH","id":"v473o142548","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Using a slotted spoon, carefully transfer the potatoes to your baking dish. Be sure not to break the potatoes up when doing this. Pour your infused cream over your potatoes, justenough to coat each layer and leave a little bit of moisture inside, about 2 cups. Top with remaining cheese and bake in the oven for 45-60 minutes. Or until the top is browned and the potatoes are soft. Broil if you need the top more browned.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"vrran8p4oorax577030","nodes":[{"type":"PARAGRAPH","id":"vlfou142551","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Allow to rest for 5 minutes at room temp and serve immediately.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"orderedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"fitmy142553","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"589e8142554","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Smashed Potato","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"BULLETED_LIST","id":"vfufx8p4oorax577041","nodes":[{"type":"LIST_ITEM","id":"elzji8p4oorax577035","nodes":[{"type":"PARAGRAPH","id":"gh6eg142558","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"2 pounds (1.8kg) Yukon gold potatoes (small)","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"hjfxw8p4oorax577037","nodes":[{"type":"PARAGRAPH","id":"u9o8d142561","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":" cup (60mL) canola oil","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"wa9f98p4oorax577039","nodes":[{"type":"PARAGRAPH","id":"0dczr142564","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Salt and pepper to taste","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"368c9142566","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"ORDERED_LIST","id":"q2iq58p4oorax577055","nodes":[{"type":"LIST_ITEM","id":"y124i8p4oorax577043","nodes":[{"type":"PARAGRAPH","id":"b3dlh142569","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"In a large pot, fill with water and place over high heat. Wash potatoes and place in a pot of boiling water. Generously salt the water. It should be as salty as a potato chip.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"avl068p4oorax577045","nodes":[{"type":"PARAGRAPH","id":"yoafr142572","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Once potato is added, reduce to a simmer over medium heat and simmer for 20 minutes or until they are fork tender.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"geaxr8p4oorax577047","nodes":[{"type":"PARAGRAPH","id":"hxvzl142575","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Once cooked, remove from heat and strain through a colander","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"0h4z58p4oorax577049","nodes":[{"type":"PARAGRAPH","id":"e3dfp142578","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Place potatoes on a baking sheet in a single layer. Using the flat bottom of a cup, squish the potatoes down until they are about  inch thick. Place the potatoes in the refrigerator overnight, uncovered.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"ixs4a8p4oorax577051","nodes":[{"type":"PARAGRAPH","id":"92al0142581","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Place potatoes from the refrigerator onto a sheet tray and pour over the canola oil making sure it is evenly dispersed.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"mqq358p4oorax577053","nodes":[{"type":"PARAGRAPH","id":"mohpz142584","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Place a sheet tray in an oven preheated to 425F (218C) and cook for 30-35 minutes or until the flat sides are crispy and brown.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"orderedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"3aly2142586","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"eapxj142587","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Smoked Potato","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"BULLETED_LIST","id":"4kb9p8p4oorax577064","nodes":[{"type":"LIST_ITEM","id":"d6nd28p4oorax577058","nodes":[{"type":"PARAGRAPH","id":"56sro142591","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"1 large russet potato","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"ci60h8p4oorax577060","nodes":[{"type":"PARAGRAPH","id":"sn5lc142594","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"1 tablespoon olive oil","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"qdtc08p4oorax577062","nodes":[{"type":"PARAGRAPH","id":"qlld2142597","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Salt and pepper to taste","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"zse34142599","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"ORDERED_LIST","id":"xh8578p4oorax577070","nodes":[{"type":"LIST_ITEM","id":"kidfc8p4oorax577066","nodes":[{"type":"PARAGRAPH","id":"p3bbh142602","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Preheat the smoker to 300F (150C). While the oven is preheating, poke potatoes all over using a fork.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"egizr8p4oorax577068","nodes":[{"type":"PARAGRAPH","id":"dvht2142607","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Brush the potato with olive oil and season with salt and pepper. Once hot, place the potato into the smoker and cook for around 2 hours. The potato should be fork tender.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"orderedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"djw8a142609","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"1vyqq142610","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Salt Baked Potatoes","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"BULLETED_LIST","id":"lyhaj8p4oorax577079","nodes":[{"type":"LIST_ITEM","id":"w5s6z8p4oorax577073","nodes":[{"type":"PARAGRAPH","id":"gb30q142614","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"2 pounds (900g) small new potatoes, rinsed","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"o4mld8p4oorax577075","nodes":[{"type":"PARAGRAPH","id":"d4epg142617","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"700g salt","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"typ