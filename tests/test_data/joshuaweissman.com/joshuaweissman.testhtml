71","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Salt and pepper to taste","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"zmafi210373","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"ORDERED_LIST","id":"o8qqs8p4oorax577508","nodes":[{"type":"LIST_ITEM","id":"5k3858p4oorax577506","nodes":[{"type":"PARAGRAPH","id":"5uo5c210376","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Add all ingredients to the slow cooker and toss to combine. Cover and cook the potatoes on high for 3-4 hours. The potatoes should be fork tender.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"orderedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"ogibv210378","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"83r6v210379","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Boxed Mashed Potatoes","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"BULLETED_LIST","id":"mbzaa8p4oorax577513","nodes":[{"type":"LIST_ITEM","id":"ta4wd8p4oorax577511","nodes":[{"type":"PARAGRAPH","id":"jclxu210383","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"1 box instant mashed potatoes","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"2x08x210385","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},