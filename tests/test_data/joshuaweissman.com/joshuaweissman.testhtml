UTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"87s1d8p4oorax577526","nodes":[{"type":"PARAGRAPH","id":"r9r7t210404","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"1 cup (240ml) whole milk","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"vfihi8p4oorax577528","nodes":[{"type":"PARAGRAPH","id":"y602b210407","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"4 cloves garlic, crushed lightly left in pods","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"baq618p4oorax577530","nodes":[{"type":"PARAGRAPH","id":"g6gqy210410","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Salt and pepper to taste","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"fw0t9210412","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"ORDERED_LIST","id":"s2qy58p4oorax577544","nodes":[{"type":"LIST_ITEM","id":"me5318p4oorax577534","nodes":[{"type":"PARAGRAPH","id":"xnhxo210415","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Take your yukon gold potatoes and peel them over a piece of parchment paper. Cut them into 1x1 inch pieces and place them into a cold pot of water. Season the water heavily with salt and bring to a rapid boil over high heat. Boil the potatoes for 15-20 minutes or until fork tender.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"swnza8p4oorax577536","nodes":[{"type":"PARAGRAPH","id":"najjt210418","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Once the potatoes are done cooking, strain through a colander and allow them to drain of their water. Using a potato ricer, and while your potatoes are still hot; press all of them through on the finest setting into a separate bowl. Place to the side and keep warm.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"z5tdv8p4oorax577538","nodes":[{"type":"PARAGRAPH","id":"vb6gg210421","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"To make the flavor liquid, melt unsalted butter in a pot set to medium-low heat. Once the butter is half-way melted, add in 4 cloves of lightly crushed skin-on garlic. From there, add in whole milk and heavy cream. Season with salt and whisk together","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"zelcv8p4oorax577540","nodes":[{"type":"PARAGRAPH","id":"f950j210424","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Bring up to a steamy heat and let steep for 10 minutes.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"vcsmm8p4oorax577542","nodes":[{"type":"PARAGRAPH","id":"i9n81210427","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Strain out any aromatics you added and pour into the bowl with the pressed potatoes. Stir until everything is evenly incorporated, season with salt and pepper to you liking and serve.","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"orderedListData":{"indentation":0}},{"type":"PARAGRAPH","id":"yfuno210429","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"94z3h210430","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"FRIED","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"FONT_SIZE","fontSizeData":{"unit":"PX","value":24}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"qdfph210432","nodes":[],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"PARAGRAPH","id":"d582s210433","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"Home Fries","decorations":[{"type":"BOLD","fontWeightValue":700},{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}},{"type":"UNDERLINE","underlineData":true}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}},{"type":"BULLETED_LIST","id":"uuvx38p4oorax577555","nodes":[{"type":"LIST_ITEM","id":"fd32l8p4oorax577549","nodes":[{"type":"PARAGRAPH","id":"75mma210437","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"3 red potatoes, medium dice","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"3dagp8p4oorax577551","nodes":[{"type":"PARAGRAPH","id":"qsn7p210440","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"3 tablespoons unsalted butter","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]},{"type":"LIST_ITEM","id":"8s3948p4oorax577553","nodes":[{"type":"PARAGRAPH","id":"vuzpw210443","nodes":[{"type":"TEXT","id":"","nodes":[],"textData":{"text":"1 tablespoon Montreal Steak Seasoning","decorations":[{"type":"COLOR","colorData":{"foreground":"rgb(0, 0, 0)"}}]}}],"paragraphData":{"textStyle":{"textAlignment":"AUTO"},"indentation":0}}]}],"bulletedListD