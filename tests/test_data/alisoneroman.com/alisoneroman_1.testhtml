        <script defer>
        document.addEventListener('DOMContentLoaded', function () {
          const main = document.querySelector('main.gh-main');
          if (!main) {
            return;
          }

          // Add fade-in class after DOM is loaded
          main.classList.add('page-loaded');
        });
        </script>

        <script defer>
        // Handle external links: add target="_blank" and remove ref parameter
        document.addEventListener('DOMContentLoaded', function() {
          const links = document.querySelectorAll('a[href]');
          const currentDomain = window.location.hostname;
          
          links.forEach(link => {
            const href = link.getAttribute('href');
            
            // Skip if it's not a full URL or is relative/internal
            if (!href || href.startsWith('/') || href.startsWith('#') || href.startsWith('mailto:') || href.startsWith('tel:')) {
              return;
            }
            
            try {
              const url = new URL(href);
              
              // Check if it's an external link
              if (url.hostname !== currentDomain) {
                // Add target="_blank" and rel attributes for external links
                link.setAttribute('target', '_blank');
                link.setAttribute('rel', 'noopener noreferrer');
                
                // Remove the ref parameter
                url.searchParams.delete('ref');
                
                // Update the href with the cleaned URL
                link.setAttribute('href', url.toString());
              }
            } catch (e) {
              // Invalid URL, skip
            }
          });
        });
        </script>

