    function fillInbox(data, callback) {
        if (data.length === 0) {
            var startingInboxItem = {
                MessageType: "System",
                Actor: { PhotoUrl: "" },
                ActionText: "Welcome! This is your notifications center. For instance, BigOven will let you know if someone's rated a recipe you've posted."
            };

            data = new Array();
            data.push(startingInboxItem);

            if (callback != null) {
                callback('', data.length);
            }
        }

        // var cacheBuster = new Date().getTime();
        var resultHtml = '';

        $.each(
            data,
            function (index, val) {
                var html = buildInboxItem(val);
                resultHtml += html;
            });
        if (callback != null) {
            callback(resultHtml, data.length);
        }
    }

