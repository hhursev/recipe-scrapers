<script defer type="text/javascript">
    var ads = ads || {};
    ads.utils = {
        init: function () {
            this.waitForIt('#skm-ad-flexrec-1', '#sidebar', ads.utils.adsPlacement);
        },
        waitForIt: function (el, container, action) {
            $el = (typeof (el) == 'object' && el instanceof jQuery) ? el : $(el);
            $container = (typeof (container) == 'object' && container instanceof jQuery)
                ? container
                : $(container);

            if ($container.length > 0) {
                var timeout = null;

                if (!timeout) {

                    if ($el.length <= 0) {
                        timeout = setTimeout(function () {
                                clearTimeout(timeout);
                                timeout = null;
                                ads.utils.waitForIt($el, $container, action);
                            },
                            250);

                    } else {
                        action();

                    }
                }
            }
        },

        adsPlacement: function () {
            //                    if (!!$('#skm-ad-flexrec-1').length) { // make sure "#sticky" element exists
            var el = $('#skm-ad-flexrec-1');
            var stickyTop = $('#skm-ad-flexrec-1').offset().top;
            var footerTop = $('#footer').offset().top;
            var bannerHeight = $('#banner_bottom').outerHeight();


            $(window).scroll(function () { // scroll event

                var stickyHeight = $('#skm-ad-flexrec-1').height();
                var limit = (footerTop - bannerHeight) - stickyHeight - 20;
                var windowTop = $(window).scrollTop();

                if (stickyTop < windowTop) {
                    el.css({ position: 'fixed', top: 80 });
                } else {
                    el.css('position', 'static');
                }

                if (limit < windowTop) {
                    var diff = limit - windowTop;
                    el.css({ top: diff });
                }
            });
            // }
        }
    };

    if (document.readyState === 'complete') {
        ads.utils.init();
    } else {
        document.addEventListener("DOMContentLoaded", function () {
            ads.utils.init();
        });
    }
</script>

