.rank-math-question {
  cursor: pointer;
  position: relative;
  display: block;
  padding-right: 1em;
  margin-right: 1em;
  font-weight: 300;
  margin-top: 30px;
}

.rank-math-question:after {
  position: absolute;
  right: 5px;
  top: 0;
  content: "\2715";
  font-style: none;
  font-size:17px;
  transform: rotate(-52deg);
  transition: all 150ms ease-in-out;
}

.rank-math-question.collapse:after {
