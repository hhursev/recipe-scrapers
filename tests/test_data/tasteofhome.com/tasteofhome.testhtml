			<fieldset>
				<legend>Select a Recipe Folder:</legend>
				<div class="folders-list">
					<# _.each( data.Folders, function( folder ) { #>
						<div class="folders-list__item">
							<input type="radio" id="{{ folder.Id }}-checkbox" name="folder" value="{{ folder.Id }}">
							<label for="{{ folder.Id }}-checkbox">
								{{ folder.Name }}
							</label>
						</div>
					<# } ); #>
				</div>
				<span class="validation-error">
					Please select a folder				</span>
			</fieldset>
		<# } else { #>
			<p>No folders yet</p>
		<# } #>

				<a href="#" role="button" class="recipe-create-folder" data-name="create a new recipe folder" data-module="recipe engagement" data-position="recipe box">
			+ Create a New Recipe Folder		</a>

		<# if ( data.Folders.length ) { #>
			<div class="popup-actions">
				<button class="popup-btn-confirm" data-action-text="Saving...">
					Save				</button>
				<button class="popup-btn-cancel popup-btn-link">
					Cancel				</button>
			</div>
		<# } #>
	</div>
</script>
