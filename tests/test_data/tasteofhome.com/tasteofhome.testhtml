	<script type="text/html" id="tmpl-save-recipe-popup">
	<div id="save-recipe-popup" class="recipe-box-popup">
		<p class="popup-title">
			<strong>
				Save {{ data.ContentTitle }} to My Recipe Box			</strong>
		</p>

