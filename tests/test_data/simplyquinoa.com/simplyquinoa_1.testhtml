<style type="text/css">.wprm-recipe-template-sq-card {
    margin: 1.5em auto;
    background-color: #ffffff; /*wprm_background type=color*/
    font-size: 1em; /*wprm_main_font_size type=font_size*/
    line-height: 1.7em; /*wprm_main_line_height type=font_size*/
    color: #333333; /*wprm_main_text type=color*/
    max-width: 800px; /*wprm_max_width type=size*/
}
.wprm-recipe-template-sq-card a {
    color: #000000; /*wprm_link type=color*/
}
.wprm-recipe-template-sq-card p, .wprm-recipe-template-sq-card li {
    font-size: 1em !important;
    line-height: 1.7em; /*wprm_main_line_height type=font_size*/
}
.wprm-recipe-template-sq-card li {
    margin: 0 0 0 32px !important;
    padding: 0 !important;
}
.rtl .wprm-recipe-template-sq-card li {
    margin: 0 32px 0 0 !important;
}
.wprm-recipe-template-sq-card ol, .wprm-recipe-template-sq-card ul {
    margin: 0 !important;
    padding: 0 !important;
}
.wprm-recipe-template-sq-card br {
    display: none;
}
.wprm-recipe-template-sq-card .wprm-recipe-name,
.wprm-recipe-template-sq-card .wprm-recipe-header {
    color: #212121; /*wprm_header_text type=color*/
    line-height: 1.3em; /*wprm_header_line_height type=font_size*/
}
.wprm-recipe-template-sq-card h1,
.wprm-recipe-template-sq-card h2,
.wprm-recipe-template-sq-card h3,
.wprm-recipe-template-sq-card h4,
.wprm-recipe-template-sq-card h5,
.wprm-recipe-template-sq-card h6 {
    color: #212121; /*wprm_header_text type=color*/
    line-height: 1.3em; /*wprm_header_line_height type=font_size*/
    margin: 0;
    padding: 0;
}
.wprm-recipe-template-sq-card h1 {
    font-size: 1.6em; /*wprm_h1_size type=font_size*/
}
.wprm-recipe-template-sq-card h2 {
    font-size: 1.6em; /*wprm_h2_size type=font_size*/
}
.wprm-recipe-template-sq-card h3 {
    font-size: 1.2em; /*wprm_h3_size type=font_size*/
}
.wprm-recipe-template-sq-card h4 {
    font-size: 1em; /*wprm_h4_size type=font_size*/
}
.wprm-recipe-template-sq-card h5 {
    font-size: 1em; /*wprm_h5_size type=font_size*/
}
.wprm-recipe-template-sq-card h6 {
    font-size: 1em; /*wprm_h6_size type=font_size*/
}
.wprm-recipe-template-sq-card .wprm-recipe-header {
  margin-bottom: 0.5em;
}
.wprm-recipe-template-sq-card {
    font-size: 1em; /*wprm_main_font_size type=font_size*/
	border-style: solid; /*wprm_border_style type=border*/
	border-width: 4px; /*wprm_border_width type=size*/
	border-color: #dfeaea; /*wprm_border type=color*/
    background-color: #ffffff; /*wprm_background type=color*/
    max-width: 800px; /*wprm_max_width type=size*/
}


.wprm-recipe-template-sq-card .wprm-sq-header-container {
	background-color: #FFFDFC;
	padding: 20px 30px;
}
.wprm-recipe-template-sq-card .wprm-sq-header-top {
	display: grid;
	grid-template-columns: 1fr auto;
	justify-content: space-between;
	grid-column-gap: 1em;
	margin-bottom: 1em;
}
.wprm-recipe-template-sq-card .wprm-header-meta {
	font-size: 0.9em;
}

.wprm-sq-times-container .wprm-recipe-times-container {
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	grid-column-gap: 1em;
	grid-row-gap: 0.5em;
	font-size: 0.9em;
	padding: 10px 20px;
	background-color: #dfeaea;
}
.wprm-sq-times-container .wprm-recipe-times-container::before {
	content: '\e825';
	font-family: Webicons;
}
.wprm-sq-times-container .wprm-recipe-details-label {
	font-size: 0.9em;
	font-weight: 500;
}

.wprm-sq-button-container {
	display: flex;
	flex-wrap: wrap;
	grid-column-gap: 0.5em;
}
.wprm-sq-button-container a.wprm-recipe-link {
	flex: 1;
  	font-size: 0.9em;
	text-decoration: none !important;
}
.wprm-sq-button-container a.wprm-recipe-link:hover {
	background-color: #507a7a !important;
}
.wprm-recipe-template-sq-card .wprm-recipe-summary {
	line-height: 1.5em;
}
.wprm-recipe-template-sq-card .wprm-sq-content {
  padding: 20px;
}

.wprm-recipe-template-sq-card .wprm-recipe-equipment-container,
.wprm-recipe-template-sq-card .wprm-recipe-ingredients-container,
.wprm-recipe-template-sq-card .wprm-recipe-instructions-container,
.wprm-recipe-template-sq-card .wprm-recipe-notes-container {
	margin-bottom: 1.3em;
}
.wprm-recipe-template-sq-card .wprm-nutrition-label-container {
	font-size: 0.9em;
	line-height: 1.5em;
}

.wprm-sq-footer-container {
	background-color: #f9fbfb;
	border-top: 2px solid #dfeaea;
	display: flex;
	align-items: center;
}
.wprm-sq-footer-left {
	flex-grow: 1;
	text-align: center;
	padding: 1em 1em 1.5em;
}
.wprm-sq-footer-left .icon-container {
	/* color: #f5a623; */
  	color: #c4851c;
	font-size: 24px;
	margin-bottom: 0.5em;
}
.wprm-sq-footer-left h3 {
	margin-bottom: .4em;
}
.wprm-sq-footer-left p {
	font-size: 15px;
	font-weight: 500;
	margin-bottom: .4em;
}
.wprm-sq-footer-image {
	flex-shrink: 0;
	flex-grow: 1;
	padding: 1.5em;
	max-width: 100%;
}
.wprm-sq-footer-image .wprm-recipe-image {
	border: 8px solid #FFF;
	transform: rotate(10deg);
	width: 130px;
	max-width: 100%;
	height: auto;
	margin: 0 auto;
}
@media (max-width:544px){
  .wprm-recipe-template-sq-card .wprm-sq-header-top {
	display: flex;
	flex-wrap: wrap-reverse;
  }
  .wprm-header-image {
	flex-grow: 1;
    margin: -10px -20px 1em;
  }
  .wprm-header-image .wprm-recipe-image img {
	width: 100%;
	height: 250px;
	object-fit: cover;
	object-position: center;
	border-radius: 0 !important;
  }
  .wprm-recipe-template-sq-card .wprm-sq-content {
	padding: 20px 10px;
  }
  .wprm-sq-footer-container {
		flex-wrap: wrap-reverse;
  }
  .wprm-sq-footer-image {
		padding-bottom: 0.5em;
  }
  .wprm-sq-footer-image .wprm-recipe-image {
		width: 150px;
  }
