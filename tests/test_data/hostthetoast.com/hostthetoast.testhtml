			custom-bullet-list {
  list-style-type: none !important;
}

.custom-bullet-list li::before {
  content: "";
  display: inline-block;
  width: 20px;
  height: 20px;
  background-image: url('https://hostthetoast.com/wp-content/uploads/2023/07/Custom-Bullet.png');
  background-size: contain;
  margin-right: 10px;
  vertical-align: middle;
}

.custom-bullet-list li::marker {
  content: none;
  display: none;
}

.custom-bullet-list li {
	padding-bottom:15px;
}

.entry-content ol li {
  list-style-type: decimal;
}

.entry-content ul li {
  list-style-type: disc;
  padding-left: 0px !important;
}

/* Custom Numbered List */

.custom-numbered-list {
  list-style-type: none !important;
	counter-reset: li;
}

.custom-numbered-list li {
	padding-bottom: 15px;
}

.custom-numbered-list li::marker {
  content: none;
  display: none;
}

.custom-numbered-list li::before {
  counter-increment: li;
	content: counter(li);
  font-size: 56px;
  font-weight: 700;
  font-family: Montserrat,sans-serif;
	color: #0f6179;
  margin-right: 15px;
  margin-top: -10px;
  min-width: 35px;
  text-align: center;
	float: left;
}

.custom-numbered-list li::after {
  content: '';
  display: block;
  clear: both;
}

.custom-numbered-list .list-item p {
  margin-left: 10px;
	float: left;
}


/* Tip Section*/

.tip-section {
  box-sizing: inherit;
  border: 3px solid #000;
  padding: 50px 20px 0px 20px;
  margin: 40px 0;
  position: relative;
}

.tip-section h2 {
  font-size: 32px;
  font-weight: 800;
	color: #fff;
  letter-spacing: 0.025em;
  background-color: #0f6179;
  padding: 2px 10px;
  position: absolute;
	margin: 0;
  top: -20px;
  left: 20px;
}

.tip-section p {
  font-size: 18px;
  line-height: 1.5;
}

@media (max-width: 768px) {
  .tip-section h2 {
    font-size: 24px;
  }

  .tip-section p {
    font-size: 16px;
	}

/* Recipe Summary */

.wp-block-group.recipe-summary {
  background: #f4f6f6;
  padding: 20px 0;
	border: 1px solid red;
}

.recipe-title {
  text-align: center;
  font-size: 32px;
}		</style>
