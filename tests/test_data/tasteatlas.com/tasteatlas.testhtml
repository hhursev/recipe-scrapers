<script>
    // Side scroll Desktop Slider
    function scrollSlider(elementClass, elementNumber = 0, direction, scrollMove = 15, totalScroll = 315) {
        var containers = document.querySelectorAll(elementClass);
        this.horizontalScroll(containers[elementNumber], direction, scrollMove, totalScroll);
    };
    // Side scroll Desktop Slider
    function horizontalScroll(element, direction, scrollMove, totalScroll) {
        var scrollAmount = 0;
        var slideTimer = setInterval(function () {
            if (direction == 'left') {
                element.scrollLeft -= scrollMove;
            } else {
                element.scrollLeft += scrollMove;
            }
            scrollAmount += scrollMove;
            if (scrollAmount >= totalScroll) {
                window.clearInterval(slideTimer);
            }
        }, 15);
    };
    // Smooth scroll to Variations slider
    function scrollToSection() {
        var matchUrl = window.location.href.split("/").pop();
        var recipeUrl = window.location.href;
        if (matchUrl !== "recipe" && recipeUrl.match('/recipe')) {
            const scrollToVariation = document.getElementById('selected-variation').offsetTop;
            window.scrollTo({ top: scrollToVariation, behavior: 'smooth' });
        }
    };
    (function () {
        setTimeout(function () {
            scrollToSection();
        }
        , 1000)
    })();

    // Smooth scroll to Recipe section
    this.scrollToRecipe = function () {
        const scrollToRecipe = document.getElementById('recipe-section').offsetTop
        window.scrollTo({ top: scrollToRecipe, behavior: 'smooth' });
    }
</script>
