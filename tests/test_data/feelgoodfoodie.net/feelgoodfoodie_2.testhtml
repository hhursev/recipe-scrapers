        margin-top: 6px;
    }

    .cwp-food-cta .cwp-food-cta__actions .wp-element-button {
        width: auto;
        padding: 12px 14px;
        font-size: 0.875rem;
    }

}

@media only screen and (min-width: 768px) {
    .cwp-food-cta {
        padding: 24px 24px;
        display: flex;
        gap: 16px;
        align-items: center;
    }

    .cwp-food-cta .cwp-food-cta__heading {
        flex-grow: 1;
    }
    
    .cwp-food-cta__heading span {
        font-size: 1.375rem;
    }

    .cwp-food-cta .cwp-food-cta__actions {
        flex-shrink: 0;
    }
