    <script>
        var uuid = "vwpr9gab-d4f55-857620-cfcd2-f55fe74i";
        var currentUserId = 0;
        var canEdit = false;
        var dailyRecipeEdit;

        var moveToComment = function(requestCommentId) {
            if (requestCommentId) {
                var $requestedComment = $(requestCommentId);
                if ($requestedComment.length) {
                    goTop = $requestedComment.offset().top;
                } else {
                    goTop = $('.comments.media-list').offset().top;
                }
                $('body, html').animate({
                    scrollTop : goTop
                }, 500);
            }
        };

        TMRC_ready(function() {
            // initRecommendation($('.recommendations-box'));
            getDynamicInfo(uuid)
                .done(function (results) {
                    initTinyMCE();
                    initRecipeRatingStars('.recipe-rating');
                    $('input.custom-checkbox', '.comments-container').ezMark();

                    dailyRecipeEdit = results.dailyRecipe;

                    Comment.init();
                    Comment.startup().then(function(data) {
                        setTimeout(function() {
                            moveToComment(window.location.hash);
                        }, 200);
                        var commentCount = 0;
                        if (data) {
                            commentCount = data.count;
                        }
                        increaseViewCount(uuid, {comment_count: commentCount});
                    });
                                        if (results.isEditable) {
                        $('.btn-edit-recipe').removeClass('d-none');
                        $('button.recipe-action__edit').removeClass('d-none');
                        $('.step-container').addClass('active').find('button.recipe-action__edit').on('click', function(e){
                            var $target = $(e.target);
                            if($target.hasClass('preventDefault') || $target.parents('.preventDefault').length > 0) {
                                return;
                            }

                            if (!dailyRecipeEdit.editable) {
                                Toaster.error(dailyRecipeEdit.errorMessage)
                            } else {
                                $(document).trigger('come_to_edit_recipe');
                                window.location.href = Routing.generate('tmrc_recipe_edit', {uuid: uuid});
                            }
                        });

                        if ($('.images-allow-others-link').length) {
                            results.isEditable ? $('.images-allow-others-link').remove()
                                                       : $('.images-allow-others-link').removeClass('d-none');
                        }
                    } else {
                        // show add other images link
                        $('.images-allow-others-link').length && $('.images-allow-others-link').removeClass('d-none');
                    }
                    fixOpenWindowAccessories();
                });
        });
    </script>
