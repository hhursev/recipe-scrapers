  function isHeroColorLightOrDark(hex) {
    var hex = hex.substring(1);      // strip #
    var rgb = parseInt(hex, 16);   // convert rrggbb to decimal

    var r = (rgb >> 16) & 0xff;  // extract red
    var g = (rgb >>  8) & 0xff;  // extract green
    var b = (rgb >>  0) & 0xff;  // extract blue
    var luma = 0.2126 * r + 0.7152 * g + 0.0722 * b; // per ITU-R BT.709
    // console.log("luma " + luma);
    if (luma >= 150 && luma < 200) {
        $('#this-week-hero').addClass("hero-color-is-medium");
    }
    else if  (luma < 150) {
      $('#this-week-hero').addClass("hero-color-is-dark");
    }
  }

  function selectExploreNavTab() {
    var currentExploreURL = document.URL;
    var urlExploreArray = currentExploreURL.split('/');
    var page;
    var prettyPage;

    if (urlExploreArray.indexOf('the-sun-times') !== -1) {
        page = 'the-sun-times';
    }
    if (urlExploreArray.indexOf('recipes') !== -1 || urlExploreArray.indexOf('recipe') !== -1 || urlExploreArray.indexOf('snack') !== -1 || urlExploreArray.indexOf('salad') !== -1 || urlExploreArray.indexOf('sauce') !== -1) {
        page = 'recipes';
    }

    if (urlExploreArray.indexOf('ingredients') !== -1) {
        page = 'recipes';
    }

    $('.explore-subnav ul #' + page + '-tab').addClass('active');
    var triangle = '<svg class="triangle" height="30" width="50"><polygon points="15,0 35,0 25,10" fill="white"></polygon></svg>';
    $('.explore-subnav ul #' + page + '-tab').after(triangle);

  }

  function selectRecipesFilterTab() {
    $('#' + '' + '-recipe-link').addClass('selected');
    $('#' + '' + 'sm-recipe-link').addClass('selected');
  }

  // In #video-hero, prevent page scroll while inside .video-roll-container

  $('.video-roll-container').on( 'mousewheel DOMMouseScroll', function (e) {
    var e0 = e.originalEvent;
    var delta = e0.wheelDelta || -e0.detail;

    this.scrollTop += ( delta < 0 ? 1 : -1 ) * 10;
    e.preventDefault();
  });


  function attachStickySideNav() {
      var window_top = $(window).scrollTop();
      var footer_top = $('.page-wrapper').height() + $('.page-wrapper').offset().top;
      if ($('.sticky-anchor').offset()) {
        var div_top = $('.sticky-anchor').offset().top;
      }
      var div_height = $(".sticky").height();


        div_height = $(".sticky").height() + 80;


      var padding = 20;
      var margin_top = 20;

        margin_top = 100;



      if ($(window).width() > 991) {

        if (window_top + div_height > footer_top - padding)
            $('.sticky').css({top: (window_top + div_height - footer_top + padding) * -1 })
        else if (window_top > div_top) {
            $('.sticky').addClass('stick');
            $('.sticky').css({top: margin_top})
        } else {
            $('.sticky').removeClass('stick');
        }

      } else {
        $('.sticky').removeClass('stick');
      }
  }

  $(window).on("resize scroll",function(e){
    attachStickySideNav();
  });

  $(document).ready(function () {

    isHeroColorLightOrDark('');
    selectRecipesFilterTab();
    attachStickySideNav();
    setTimeout(function(){
        selectExploreNavTab();
    }, 500);


      showSimilarRecipes(1530);


    $('a.img-container').on('click', function(e) {
        var el = $(this);
        var link = el.attr('href');
        window.location = link;
    });

  });

</script>
