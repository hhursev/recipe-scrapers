        if (searchQuery == "soups-and-stews") { jsonQuery.minScore = 3.885844; jsonQuery.q = "soup stew chili broth"; }
        if (searchQuery == "pasta-and-noodles") { jsonQuery.minScore = 3.01; jsonQuery.q = "pasta noodle soba udon ramen penne rigatoni pappardelle fettuccine orzo spaghetti orecchiette"; }
        if (searchQuery == "salads-and-bowls") { jsonQuery.minScore = 3.00; jsonQuery.q = "salad bowl"; }
        if (searchQuery == "eggs") { jsonQuery.minScore = 5.96; }
        if (searchQuery == "pork") { jsonQuery.minScore = 2.2643218; }
        if (searchQuery == "tacos") { jsonQuery.minScore = 1.0; jsonQuery.q = "tacos tostadas"; }
        if (searchQuery == "seafood") { jsonQuery.minScore = 2.0; jsonQuery.q = "fish salmon shrimp cod sole halibut tuna"; }
        if (searchQuery == "burgers-and-sliders") { jsonQuery.minScore = 3.5121577; jsonQuery.q = "burgers sliders"; }

        return jsonQuery;
      }

      function loadSomeTiles(response) {
        if (typeof response == 'undefined') {
          // exit function if scrolled on non-search page
            return false;
        } else {
            var results = response.results.slice(scrollLoad.pageIndex, scrollLoad.pageIndex + tileAmount);
        }
        results = replaceImgWithThumb(results);
        results = {results: results};
        var template = $('#explore-tile-template').html();

        var html = Mustache.render(template, results);

        isLoadingTiles = true;
        var endOfResults = response.results.length;
        var isDoneLoading = (scrollLoad.pageIndex >= endOfResults - tileAmount);

        if (scrollLoad.pageIndex == 0) {
          //new category load
          tileAmount = 20;
          $('#repopulate').html(html);
          isLoadingTiles = false;
          $(".spinner-lg-blue").hide();

        } else if (!isDoneLoading) {
          //scrolling down
          tileAmount = 10;
          setTimeout(function(){
              $("#repopulate .explore-tile-container").last().after(html);
              isLoadingTiles = false;
              $(".spinner-lg-blue").hide();
          }, 500);
        }


        if (isDoneLoading) {
          $("footer").show();
        } else {
          $(".spinner-lg-blue").show();
          $("footer").hide();
        }

        if (scrollLoad.searchQuery !== undefined) {
          $("#recipesByIngredientTitle").show();
        }
      }

      return {
          // public methods

          loadAjaxContent: function (searchQuery, searchType) {

              scrollLoad.searchQuery = searchQuery;

              $('#repopulate').html("");

              var jsonQuery = prepareJSON(searchQuery, searchType);
              // on first load, populate content object from json response
              if (typeof content[searchQuery] === 'undefined' || content[searchQuery].length == 0 ) {

                  $.ajax({
                      url: "https://rest.sunbasket.com/search",
                      type: "get",
                      data: jsonQuery,
                      success: function (response) {

                          loadSomeTiles(response);
                          // populate content object
                          content[searchQuery] = response;

                          if (response.results.length == 0) {
                              $(".generalSearchPage #repopulate").html("<div class='padding-md'>No results.</div>");
                          }
                      }
                  });

              } else {
                  // content object already populated
                  loadSomeTiles(content[searchQuery]);
              }
          },
          loadTilesAtBottom: function() {
              var scrollBarPos = $(window).scrollTop();
              var pageBottom = ($(document).height() - $(window).height()) - 60;

              if (scrollBarPos >= pageBottom) {

                  scrollLoad.pageIndex = scrollLoad.pageIndex + tileAmount;

                  if (isLoadingTiles == false) {
                      loadSomeTiles(content[scrollLoad.searchQuery])
                  }

              }
          },
          searchQuery : searchQuery,
          pageIndex : pageIndex
      }

  }());

  $(window).scroll(function() {
    scrollLoad.loadTilesAtBottom();
  });

  function replaceImgWithThumb(arr) {
    for(var i=0; i < arr.length; i++) {
      var thisImage = arr[i]["image"];
      if (thisImage.indexOf('tn_') == -1) {
          thisImage = 'tn_' + thisImage;
      }
      arr[i]["imageUrl"] = '//cdn.sunbasket.com/' + thisImage;
    }
    return localPathUrls(arr);
  }

  function localPathUrls(arr) {
      const TYPE_TO_URI = {
          'recipe': '/recipe/',
          'article': '/content/article/',
          'video': '/video/'
      }

      var prefix = '';
      if (document.domain == 'localhost') {
          prefix = '/' + window.location.pathname.split('/')[1];
      }
      for(var i=0; i < arr.length; i++) {
          arr[i]["webUrl"] = prefix + TYPE_TO_URI[arr[i]["type"]] + arr[i]["slug"];
      }
      return arr;
  }

  function imgLoaded($this) {

    //load background-image of img-container, remove spinner
    var $imgContainer = $this.parent('a');

    $imgContainer.css('background-image','url('+ $this.attr('src') +')');

    if (scrollLoad.pageIndex == 0) {
      $imgContainer.parent('.explore-tile').show();
    } else {
      $imgContainer.parent('.explore-tile').fadeIn("fast");
    }
    $this.remove();
  }
</script>


