			.single .content-sidebar-wrap .slick-film-strip {display:none!important;}
@media only screen and (max-width: 600px) {
.site-header {height:62px!important;min-height:62px!important;max-height:62px!important;}
.featured-pages-block .featured-first {height:522px!important;max-height:522px!important;min-height:522px!important;}
}
.single .outline-box {min-height:133px!important;margin: 0 auto 25px!important;padding: 20px 4% 15px!important;}
.single .wp-block-image {margin-bottom: 1em!important;}
.entry-category a{
color: #242424;
background: #B4D66F;
padding: 5px 10px;
font-weight: 700;
letter-spacing: .5px;
	font-size: 12px;
}
/* Recipe Save */
.inpost-save {
background-color: #f2f2f2;
padding: 10px 15px;
margin: 25px 0px;
box-shadow: 2px 2px 12px 0 rgba(0, 0, 0, 0.3);
border: 2px solid #038577;
border-radius: 5px;
}

.save-title {
	font-size: 25px;
	font-family: TimesNewRoman, Times New Roman, Times, Baskerville, Georgia, serif;
	font-weight: bold;
	margin-bottom: 0 !important; 
	line-height: 1.3;
}

p.save-description {
	margin-bottom: 15px !important;
	line-height: 1.3;
}

.inpost-save .save-disclaimer {
	margin-bottom: 5px;
}

.inpost-save form {
margin-bottom: 5px !important
}

.inpost-save .ff_submit_btn_wrapper_custom {
	margin-top: 0 !important;
}


.inpost-save .ff-message-success p {
	margin-bottom: 10px !important;
}

.inpost-save .ff_btn_style {
	padding: 12px 20px !important;
}

.inpost-save .ff-default input {
	min-height: 50px !important;
	font-size: 14px;
}

.inpost-save .ff-default .ff-el-form-control, .inpost-save .ff-default .ff_btn_style {
	border-radius: 0px;
}

.inpost-save .ff-default .ff_btn_style {
	font-weight: bold;
}

.tasty-recipes-print-view .im-container, .tasty-recipes-print-view .inpost-save {
	display: none !important;
}

@media print {
	.inpost-save {
		display: none !important;
	}
}

/* End Recipe Save */

/* Mediavine Sidebar + Side Scroll CSS */
.single-post .site-inner,
.single-post .site-inner>.wrap {
    overflow-x: clip !important;
}

@media only screen and (min-width: 1140px) and (max-width: 1171px) {
    .content-sidebar .content {
        max-width: calc(100% - 340px);
    }

    .content-sidebar .sidebar {
        min-width: 300px;
    }
}
@media only screen and (max-width: 359px) {
    #h-facebook-pinterest-instagram-twitter-youtube {
        font-size: 23px !important;

    }

    .wprm-recipe {
        border: unset !important;
    }

    .wprm-col,
    .wprm-summary,
    .wprm-padding,
    .wprm-callout-custom {
        padding: unset !important;
    }

    .wprm-recipe-image:before {
        right: 42px !important;
    }
}
