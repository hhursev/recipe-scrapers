<style type="text/css">.wprm-recipe-the-woks-of-life {
  background-color: #fff; /* wprm_background type=color */
  border-width: 0px; /* wprm_border_width type=size */
  border-radius: 0px; /* wprm_border_radius type=size */
  padding: 16px;
}

.wprm-recipe-instructions-container .wprm-recipe-instruction-text,
.wprm-recipe li {
  font-size: 20px;
  font-weight: 400;
}

.wprm-recipe-rating .wprm-rating-star.wprm-rating-star-full svg * {
  fill: #f75b2b !important;
  stroke: #f75b2b !important;
}

.wprm-call-to-action-icon {
  display: none;
}
/* -------- [ OC Code ] -------- */

/* [ Containers ] */

.wprm-print {
  width: 650px;
  max-width: calc(100% - 20px);
  margin: 0 auto;
  padding: 0;
}

.wprm-recipe-container {
  font-size: 16px;
}

.wprm-recipe {
  overflow: visible;
}

.wprm-recipe-container {
  /* only in non-print displays */
  clear: both;
  margin: 8px 0 20px;
}

.wprm-recipe-container:focus {
  outline: 0; /* remove, as it shows when using the "jump" button */
}

/* [ CTA ] */

.wprm-recipe-container .wprm-recipe-cta {
  line-height: 1.8;
  text-align: center;
}

@media screen and (min-width: 768px) {
  .wprm-recipe-container .wprm-recipe-cta {
    font-size: 20px;
    text-align: left;
  }
}

.wprm-print .wprm-recipe-cta {
  display: none;
}

.wprm-recipe-cta-header {
  display: block;
  font-size: 17px;
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: 0.05em;
  color: #1f4463;
}

/* [ Card ] */

.wprm-recipe.wprm-recipe-template-the-woks-of-life-card-by-once-coupled {
  overflow: visible;
}

.wprm-recipe-container .wprm-recipe-the-woks-of-life {
  padding: 20px;
  margin: 100px auto 12px;
  border: 2px solid #222;
  overflow: initial;
  position: relative;
}

@media screen and (min-width: 768px) {
  .wprm-recipe-container .wprm-recipe-the-woks-of-life {
    margin-top: 15px;
    position: relative;
  }
}

.wprm-recipe-container .wprm-recipe-the-woks-of-life::before {
  content: 'RECIPE';
  font-family: 'Barlow';
  font-weight: 500;
  font-size: 14px;
  text-align: left;
  letter-spacing: 0.1em;
  background: #343434;
  color: #fff;
  padding: 10.5px 12px;
  color: #fff;
  position: absolute;
  top: 42px;
  left: -22px;
  border-radius: 0 0 5px 5px;
  line-height: 1;
  transform: rotate(-90deg);
}

@media screen and (min-width: 375px) {
  .wprm-recipe-container .wprm-recipe-the-woks-of-life::before {
    left: -26px;
    font-size: 17px;
  }
}

@media screen and (min-width: 768px) {
  .wprm-recipe-container .wprm-recipe-the-woks-of-life::before {
    transform: rotate(-90deg);
    left: -65px;
    top: 24px;
    border-radius: 5px 5px 0 0;
  }
}

/* [ Header ] */

.wprm-recipe-container .wprm-entry-header {
  position: relative;
  margin-top: 75px;
}

@media screen and (min-width: 768px) {
  .wprm-recipe-container .wprm-entry-header {
    margin-top: 0;
    min-height: 230px;
  }

  .wprm-recipe-container .wprm-entry-header > * {
    text-align: left !important;
  }

  .wprm-recipe-container .wprm-entry-header .description-wrap .wprm-recipe-name,
  .wprm-recipe-container .wprm-entry-header .description-wrap .wprm-recipe-summary {
    margin-right: 200px;
  }
}

/* Image */

.wprm-recipe-container .wprm-recipe-image {
  margin: -40px auto 16px;
  border-radius: 50%;
  border: 2px solid #222;
  width: 175px;
  height: 175px;
  overflow: hidden;
  display: flex;
  position: absolute;
  top: -145px;
  left: calc(50%);
  transform: translateX(-50%);
}

@media (min-width: 768px) {
  .wprm-recipe-container .wprm-recipe-image {
    margin: 0;
    max-width: 100%;
    width: 168px;
    height: 164px;
    border-width: 3px;
    border-radius: 0;
    display: flex;
    border: 0;
    right: 0;
    left: unset;
    top: 0;
    transform: unset;
  }
}

/* Ratings*/

.wprm-recipe-container .wprm-entry-header .wprm-recipe-rating {
  margin: 12px 0 0px;
  text-align: center;
}

@media screen and (min-width: 768px) {
  .wprm-recipe-container .wprm-entry-header .wprm-recipe-rating {
    margin: 0;
    position: static;
    top: unset;
    right: unset;
    display: flex;
    align-items: flex-end;
    justify-content: flex-start;
    line-height: 1;
  }
}

.wprm-recipe-container .wprm-rating-star {
  margin: 0 1px;
}

.wprm-recipe-container .wprm-rating-star polygon {
  stroke: #f75b2b;
}

.wprm-recipe-container .wprm-rating-star-full polygon {
  fill: #f75b2b !important;
}

.wprm-recipe-container .wprm-user-rating {
  width: 100px;
  text-align: center;
}

@media screen and (min-width: 768px) {
  .wprm-recipe-container .wprm-user-rating {
