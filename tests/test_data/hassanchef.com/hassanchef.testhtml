    } else if (hc < n) {
      text += '<ul class="ps-3 pt-2 fs-8"><li>';
      l++;
    } else if (hc > n && l > 1) {
      for (var m = 0; m < hc - n; m++) {
        text += "</li></ul>";
      }
      l--;
    }
    hc = n;
    text += i > 0 ? "</li>" : "";
    text += '<li class="mb-2">';
    text += '<a class="text-reset hover-text-primary" href="#' + data.id + '">' + data.title + "</a>";
    if (i == e.length - 1) {
      for (var j = 1; j < hc; j++) {
        text += "</li></ul>";
      }
    }
    return text;
  }).join("").replace(/<li>\s*<\/li>/gi, "") + "</div>";
}

