	max-width: 100%;
	position: fixed;
	left: 0;
	width: 300px;
	top: 0;
	height: 100%;
	overflow-y: scroll;
	transform: translateX(-300px);
	transition: transform 300ms ease-in-out;
	z-index: 10000003;
}
#sidebarSearch {
