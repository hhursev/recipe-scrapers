	text-transform: capitalize;
	font-size: 14px;
}
.breadcrumbs a {
	position: relative;
	display: inline-block;
	margin-right: 20px;
	color: #22656f;
}
.breadcrumbs a::after {
  content: '';
  display: block;
  position: absolute;
  top: 50%;
  left: 100%;
	border: 4px solid transparent;
