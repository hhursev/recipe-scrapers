/* global layout, mostly header bar */

body {
	margin: 0;
	padding: 50px 0 0;
}
.header-container {
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	z-index: 10000002;
	height: 50px;
	overflow: hidden;
	backdrop-filter: blur(2px);	
	border-bottom: 1px solid rgba(0, 145, 85, 0.9);
}
.header-bar .logo {
	height: 50px;
	max-width: 167px;
	margin: 1px 0;
	display: block;
}
.header-bar {
	position: fixed;
	top: 0;
	left: 15px;
	right: 15px;
	z-index: 10000002;
	height: 50px;
	overflow: hidden;
	display: grid;
	grid-template-columns: 50px 255px 1fr 90px;
	grid-template-rows: 1fr;
	grid-column-gap: 0px;
	grid-row-gap: 0px;
	max-width: 1250px;
	margin: 0 auto;
}
