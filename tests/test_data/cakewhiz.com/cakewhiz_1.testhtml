			/* Custom CSS by iMark Interactive */
.page-id-61265 .entry-content h2:not(.home-about) {
	background-color: #612779;
	color: #fff;
	padding-top: 17px;
}

/* Recipe Save */
.wprm-recipe .inpost-save, .inpost-save {
	background-color: #f2f2f2;
	padding: 10px 15px;
	margin-bottom: 25px;
	border: 2px solid #000;
}

.inpost-save :where(.wp-block-columns) {
	margin-bottom: 5px;
}

.wprm-recipe .save-title, .save-title {
	font-size: 25px;
	font-weight: bold;
	margin-bottom: 0 !important;
	line-height: 1.3;
}

.wprm-recipe p.save-description, p.save-description {
	margin-bottom: 0px !important;
	line-height: 1.3;
	font-size: 16px !important;
}

.wprm-recipe .inpost-save .save-disclaimer {
	font-size: 14px !important;
}

.wprm-recipe .inpost-save .save-disclaimer,.inpost-save .save-disclaimer {
	margin-bottom: 0px;
}

.wprm-recipe .inpost-save div.wpforms-container-full, .inpost-save div.wpforms-container-full {
	margin-bottom: 5px;
	margin-top: 0px;
}

.wprm-recipe .inpost-save button[type=submit],.inpost-save button[type=submit] {
	background-color: #612779 !important;
	color: #fff !important;
}

.wprm-recipe .inpost-save div.wpforms-container-full .wpforms-form input[type=email], .inpost-save div.wpforms-container-full .wpforms-form input[type=email] {
	height: 43px;
	border: 1px solid #000;
}

.wprm-recipe .inpost-save div.wpforms-container-full.inline-fields .wpforms-form .wpforms-field, .inpost-save div.wpforms-container-full.inline-fields .wpforms-form .wpforms-field {
	display: table-cell;
	padding-right: 2%;
	width: 100%;
}

.wprm-recipe .inpost-save div.wpforms-container-full .wpforms-confirmation-container-full, .inpost-save div.wpforms-container-full .wpforms-confirmation-container-full {
	background: #612779;
	border: 1px solid #612779;
	box-sizing: border-box;
	color: #fff;
	font-size: 15px !important;
}

.wprm-recipe .inpost-save .wp-block-columns.is-layout-flex {
	gap: 15px !important;
	margin-bottom: 5px;
}

.wprm-recipe .inpost-save .wpforms-container.inline-fields .wpforms-field-container {
	width: 100% !important;
}

.wprm-recipe .inpost-save div.wpforms-container-full .wpforms-confirmation-container-full p {
	color: #fff;
}

body:not(.wprm-print) .wprm-recipe .inpost-save p:first-letter {
	padding: 0px;
}

aside, #breadcrumbs, .entry-meta {
	font-size: 14px;
}

@media ( max-width: 580px ) {
	header.entry-header {
		margin-bottom: 10px;
	}
	nav#breadcrumbs {
		margin-bottom: 0px;
	}

	.swp_social_panel {
		margin-top: 5px !important;
		margin-bottom: 5px !important;
	}

	h1.entry-title {
		margin-bottom: 10px;
	}

	.wprm-recipe-template-custom-snippet {
		text-align: left;
	}

}

@media only screen and (max-width: 650px) {
	.wprm-recipe .wpforms-container.inline-fields .wpforms-form,.wpforms-container.inline-fields .wpforms-form, .wpforms-container.inline-fields .wpforms-field-container { 
		display: inline-block !important;
		width: 100% !important;
	}
	.wprm-recipe .wpforms-container.inline-fields .wpforms-submit-container, .wpforms-container.inline-fields .wpforms-submit-container {
		width: 100% !important;
	}
	.inpost-save div.wpforms-container-full.inline-fields .wpforms-form .wpforms-field {
		padding-right: 0px;
		display: block;
		margin-bottom: -20px;
	}
	.save-title {
		font-size: 16px;
	}
	p.save-description {
		font-size: 14px !important;
	}
	.inpost-save .save-disclaimer {
		font-size: 12px !important;
		margin-top: -10px;
	}
}

@media print {
	.inpost-save {
		display: none !important;
	}
}
.print-button-test .wprm-recipe-jump {
