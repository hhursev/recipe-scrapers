    "recipeIngredient": [
        "butter (ghee, or shortening for greasing the pan)",
        "3 cups almond flour",
        "1 1\/2 cups tapioca starch (plus more for flouring pan)",
        "1\/2 cup coconut flour",
        "1 1\/2 teaspoons baking soda",
        "2 teaspoons ground ginger",
        "1 teaspoons ground cinnamon",
        "1\/4 teaspoon ground allspice",
        "1\/4 teaspoon ground cardamom",
        "1\/2 teaspoon finely ground sea salt",
        "1 cup coconut sugar",
        "1 cup molasses (use true molasses if you can find it)",
        "2 teaspoons fresh ginger",
        "1 cup boiling water",
        "4 eggs"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 350\u00baF. Grease a 9&#215;13-inch cake pan.",
            "url": "https:\/\/www.zenbelly.com\/gingerbread\/#instruction-step-1"
        },
        {
            "@type": "HowToStep",
            "text": "In a large bowl, whisk together the almond flour, tapioca starch, coconut flour, baking soda, ground ginger, cinnamon, allspice, cardamom, and salt.",
            "url": "https:\/\/www.zenbelly.com\/gingerbread\/#instruction-step-2"
        },
        {
            "@type": "HowToStep",
            "text": "In a medium heat proof bowl, whisk together the coconut sugar, molasses, fresh ginger, and boiling water. Once it&#8217;s lukewarm (the molasses and coconut sugar should take the temperature down enough), whisk in the eggs.",
            "url": "https:\/\/www.zenbelly.com\/gingerbread\/#instruction-step-3"
        },
        {
            "@type": "HowToStep",
            "text": "Pour the wet ingredients into the dry ingredients and whisk until there are no lumps.",
            "url": "https:\/\/www.zenbelly.com\/gingerbread\/#instruction-step-4"
        },
        {
            "@type": "HowToStep",
            "text": "Pour into the prepared pan and bake for 28-35 minutes*&nbsp;",
            "url": "https:\/\/www.zenbelly.com\/gingerbread\/#instruction-step-5"
        }
    ],
