<script id="recipe-notes-tmpl" type="template">

  <div class="notes-header">Cooking Notes</div>

  <div class="form-container"></div>

  <div class="notes-threads">

    <div class="notes-tabs-container"></div>

    <div class="notes-threads-container">
      <div class="all-thread-container">
        <div class="no-notes-message">
          There aren&#8217;t any notes yet. Be the first to <span class="leave-note-shortcut">leave one</span>.
        </div>
      </div>
      <div class="helpful-thread-container"></div>
      <div class="private-thread-container">
        <div class="no-notes-message thread">
          <p>Private Notes are notes that only you can see.  <span class="leave-private-note-shortcut">Leave a Private Note</span>.</p>
          <p><em>For example: "Made for Alex's birthday. Next time double the recipe."</em></p>
        </div>
      </div>
    </div>

  </div>

  <div class="btn-std btn-default show-all-btn">Show More Notes</div>

</script>

<script id="note-form-tmpl" type="template">

    <div class="notes-user-controls">

      <div class="profile-user-img <%= hasImg ? 'has-image' : '' %>">
        <img class="user-img" src="<%= userImg %>">
        <i class="icon-user-anon-profile"></i>
      </div>

      <div class="notes-user-input">
        <input class="notes-user-name" type="text" placeholder="Your name (required)" value="<%= name %>">
      </div>
    </div>

    <div class="notes-form-img">
      <img class="user-img" src="<%= userImg %>">
    </div>

    <div class="notes-body-container">
      <div class="notes-body-count"><%= bodyLimit %></div>
      <textarea class="notes-body" readonly placeholder="Share your notes with other cooks or leave a private note."></textarea>
      <div class="notes-form-controls">
        <% if (!isReply) { %>
          <div class="notes-form-options">
            <span>Who can see this note?</span>
            <input id="notes-public-radio" name="note-type" type="radio" checked>
            <label class="notes-public-label" for="notes-public-radio">Everyone</label>
            <input id="notes-private-radio" name="note-type" type="radio">
            <label class="notes-private-label" for="notes-private-radio">Only You</label>
          </div>
        <% } %>
        <div class="notes-form-actions">
          <button class="cancel-btn btn-std">Cancel</button>
          <button class="add-btn btn-std disabled ladda-button grey-spinner" data-style="normal" data-spinner-size="20"><%= (!isReply) ? 'Add Note' : 'Reply' %></button>
        </div>
      </div>
    </div>

</script>

<script id="note-tabs-tmpl" type="template">

  <% _.each(tabs, function(tab, index) { %>
    <% if (tab.displayWhenNone && counts[index] === 0) return; %>
    <li class="<%= activeName === tab.name ? 'active' : '' %>">
      <a href="#" data-name="<%= tab.name %>">
        <span class="desktop"><%= tab.display %></span><span class="mobile"><%= tab.mobileDisplay %></span><span class="notes-count"><%= counts[index] %></span>
      </a>
    </li>
  <% }); %>

</script>

<script id="note-tmpl" type="template">

  <div class="thread-note
      <%= isSameUserNote ? 'user-note' : '' %>
      <%= isPrivate ? 'private-note' : '' %>
      <%= canHaveReplies && isReply ? 'is-reply' : '' %>
      <%= isApproved ? '' : 'is-new' %>
      <%= wasJustAdded ? 'brand-new' : '' %>
      <%= isEditor ? 'editor-note' : '' %>"
      data-id="<%= id %>" data-user-id="<%= userId %>">
    <% if (!isApproved) { %>
      <div class="pending-note">
        Thank you for your submission. Your note will appear once it has been approved.
      </div>
    <% } %>
    <span class="note-img">
      <img class="user-img <%= imgSrc ? '' : 'hidden' %>" src="<%= imgSrc %>">
    </span>
    <div class="note-base">
      <div class="note-header">
        <span class="note-name"><%= userName %></span>
        <i class="icon-editor-badge <%= isEditor ? '' : 'hidden' %>"></i>
        <time class="note-time note-header-additional"><%= createTime %></time>
        <span class="note-private-msg note-header-additional">only you can see this</span>
      </div>
      <div class="note-body"><%= body %></div>
      <div class="note-footer">
        <% if (!isReply && canHaveReplies) { %>
          <div class="reply-btn">
            <i class="icon icon-note-reply"></i><i class="icon icon-note-reply-active"></i>Reply
          </div>
        <% } %>
        <div class="helpful-btn <%= isHelpful ? 'is-helpful' : '' %> ladda-button grey-spinner" data-style="normal" data-spinner-size="24">
          <i class="icon icon-note-helpful"></i><i class="icon icon-note-helpful-active"></i><span class="helpful-count"><%= recommendationsCount %></span>This is helpful
        </div>
        <div class="flag-btn ladda-button grey-spinner hidden" data-style="normal" data-spinner-size="24">
          <i class="icon icon-note-flag"></i><i class="icon icon-note-flag-active"></i></span>Flag
        </div>
      </div>
    </div>
  </div>
</script>

<script id="recipe-ratings-tmpl" type="template">

  <div class="ratings-rating">
    <span class="ratings-header ratings-content"><%= header %></span>

    <div class="ratings-stars-wrap">
      <div class="ratings-stars ratings-content <%= ratingClass %> <%= state %>">
        <span class="icon-star star-one"></span><span class="icon-star star-two"></span><span class="icon-star star-three"></span><span class="icon-star star-four"></span><span class="icon-star star-five"></span>

        <div class="ratings-tooltip ladda-button grey-spinner" data-style="normal" data-spinner-size="24">
          <p class="ratings-tooltip-header"><%= tooltipHeader %></p>
          <div class="tooltip-stars <%= tooltipRatingClass %> <%= tooltipState %>">
            <span class="icon-star star-one"></span><span class="icon-star star-two"></span><span class="icon-star star-three"></span><span class="icon-star star-four"></span><span class="icon-star star-five"></span>
          </div>
        </div>

      </div>
    </div>
  </div>
</script>





