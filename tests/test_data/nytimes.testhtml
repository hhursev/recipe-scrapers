    <div class="icon-close-grey <%= showCloseBtn ? '' : 'hidden' %>"></div>
    <h3 class="popover-title"></h3>
    <% if (body) { %>
      <div class="popover-body"><%= body %></div>
    <% } %>
    <div class="popover-content"></div>
  </div>
</script>

<script id="parse-msg-tmpl" type="template">
  <div class="parse-message"><%= parse_msg %></div>
</script>

<script id="show-more-btn-tmpl" type="template">
