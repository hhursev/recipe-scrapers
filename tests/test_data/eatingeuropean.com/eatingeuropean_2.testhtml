	outline: 2px solid #613c37;
	outline-offset: -2px;
}

.home-popular-categories figcaption:hover {
	outline-offset: 2px;
	transition: 0.3s all ease;
}

.home-popular-categories figcaption a {
	text-decoration: none;
	color: #FFF;
	font-weight: 500;
}

.home-cta {
	background: #375C61;
	width: 100vw;
  position: relative;
  left: 50%;
  right: 50%;
  margin: 10px -50vw 20px;
