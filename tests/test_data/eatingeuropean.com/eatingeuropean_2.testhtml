.recipe-page-cat-list a, .recipe-page-cat-list h2 {
	color: #FFF !important;
}

.recipe-page-cat-list a {
	text-decoration-color: #FFF !important;
}



/* Archives */
.archive {
	background: #FFF
}

.archive .loop-entry {
	box-shadow: none;
	border-radius: 0;
	background: #375C61 !important;
}

.archive .loop-entry:hover {
	outline: 4px solid #375C61;
	outline-offset: 1px;
	transition: 0.2s all ease;
}

.archive .loop-entry a {
	color: #FFF !important;
	text-decoration: none !important;
	font-size: 22px;
	font-weight: 600 !important;
}

.archive .loop-entry:hover a {
	color: #FFF !important;
	text-decoration: none !important;
}

.navigation.pagination {
	text-align: center;
	font-weight: 500;
}

.navigation.pagination a {
	font-weight: 500;
	color: #000;
}

.pagination .page-numbers {
  border-radius: 0;
}

.pagination .current {
	background: #613c37;
	border: 2px solid #613c37;
	color: #FFF;
}

.pagination a:hover {
	border: 2px solid #613c37;
}




/* Contact page */
.contact-page-group .ninja-forms-req-symbol, .contact-page-group label {
	color: #FFF !important;
}

.nf-error .nf-error-msg, .nf-form-fields-required, .nf-error-msg {
	color: #CCC !important;
	font-weight: 500;
	font-size: 14px;
}

.contact-page-group .contact-form-column {
	background: #613c37;
	padding: 40px;
}

.contact-page-group .nf-error.field-wrap .nf-field-element:after {
    background: #375C61;
}

.contact-page-group .nf-error .ninja-forms-field {
  border-color: #375C61!important;
}



/* Singles */
.comment-reply-link {
	background: #613c37 !important;
	border: 2px solid #613c37 !important;
	border-radius: 0 !important;
	font-weight: 500;
	font-size: 14px;
	padding: 7px 10px 8px !important;
	box-shadow: none;
	color: #FFF;
}

.comment-reply-link:hover {
	background: #FFF !important;
	color: #613c37;
	transition: 0.3s all ease;
	box-shadow: none;
}

.comment-body {
	border-bottom: 1px solid #613c37;
}

.comment.bypostauthor {
	border-left: 10px solid #613c37
}

.comment.bypostauthor .comment-body {
	padding-left: 20px;
}

.entry-author-style-center {
	border-bottom: 2px solid #613c37;
	font-weight: 500;
}

.entry-author-style-center .entry-author-avatar:before, .entry-author-style-center .entry-author-avatar:after {
  background: #613c37;
	height: 2px;
}

.comment-navigation .nav-previous:after, .post-navigation .nav-previous:after {
    width: 2px;
    height: 50%;
    background: #613c37;
}

.splide .splide__arrow:hover, .splide .splide__arrow:focus, .splide .splide__arrow {
	background: #613c37 !important;
	color: #FFF !important;
	opacity: 1 !important;
}

.splide .splide__arrow svg {
	fill: #FFF !important;
}

.entry-related .entry-title a {
	color: #000 !important;
	text-decoration: none !important;
}



/* WP Recipe Maker */
/* Snippets */
.wprm-recipe-snippet a {
	background: #613c37 !important;
	border-radius: 0 !important;
	text-decoration: none !important;
	border: 2px solid #613c37 !important;
	font-size: 14px;
}

.wprm-recipe-snippet a:hover {
	background: #FFF !important;
	color: #613c37 !important;
}

.wprm-recipe-snippet a:hover path {
	fill: #000 !important;
}

.wprm-recipe-snippet {
	text-align: center !important;
}

/* Rec card */
.wprm-recipe-template-not-sure-header {
	background-color: #375C61 !important;
	color: #ffffff;
}

a.wprm-recipe-link-inline-button {
	text-decoration: none !important;
	background: #613c37 !important;
	border: 2px solid #613c37 !important;
	color: #FFF !important;
}

a.wprm-recipe-link-inline-button path {
	fill: #FFF !important;
}

a.wprm-recipe-link-inline-button polygon {
	fill: #FFF !important;
	stroke: #FFF !important;
}

.wprm-recipe-template-not-sure {
	border: 2px solid #375C61 !important;
	border-radius: 0 !important;
	font-family: "Times New Roman", Times, serif !important;
}

.wprm-recipe-template-not-sure li, .wprm-recipe-template-not-sure .wprm-recipe-name, .wprm-recipe-template-not-sure .wprm-recipe-header {
	font-family: "Times New Roman", Times, serif !important;
}

.wprm-recipe-template-not-sure-container {
	border-radius: 0 !important;
}

.wprm-call-to-action {
	background: #613c37 !important;
	color: #FFF !important;
	padding: 30px 10px !important;
}

.wprm-call-to-action-header {
	color: #FFF !important;
}

.wprm-call-to-action a {
	color: #FFF !important;
	font-weight: 600 !important;
	text-decoration-color: #FFF !important;
}

.wprm-call-to-action path, .wprm-call-to-action circle {
	fill: #FFF;
}

/* Sidebar */
.sidebar-bio {
	font-size: 16px;
	font-weight: 500;
	margin-bottom: 40px;
	line-height: 24px
}

.sidebar-bio img {
	border-top-left-radius: 100px;
	border-bottom-right-radius: 100px;
	box-shadow: 12px 12px 14px rgba(0,0,0,0.2)
}

.sidebar-featured h4 {
	font-size: 18px;
}

.sidebar-featured .uagb-post__title a:hover {
	color: #FFF !important;
}



/* Signup bars */
.top-signup-bar .flavour-text {
	float: left;
	color: #FFF;
	font-weight: 600;
	margin: 0;
	margin-top: 10px;
	font-size: 18px;
}

.top-signup-bar {
	background: #375C61;
	padding: 10px;
}

.top-signup-bar .signup-container {
	max-width: 618px;
	display: block;
	margin: 0 auto;
}

.top-signup-bar #mc_embed_signup {
	max-width: 336px;
	clear: none !important;
	float: left;
}

.top-signup-bar #mc_embed_signup .email {
	width: 200px;
	clear: none !important;
	border-radius: 0;
	margin-left: 20px;
	border: 2px solid #613c37;
}

.top-signup-bar #mc_embed_signup .button {
	padding: 9px 20px 10px !important;
}

.top-signup-bar .mc-field-group {
	float: left;
}

.top-signup-bar label {
	display: none;
}


.after-entry-signup-bar .flavour-text {
	color: #FFF;
	font-weight: 500;
	margin: 0;
	margin-top: 10px;
	text-align: center;
}

.after-entry-signup-bar {
	background: #375C61;
	padding: 20px 10px;
	overflow: auto;
}

.after-entry-signup-bar .signup-container {
	max-width: 100%;
	display: block;
	margin: 0 auto;
}

.after-entry-signup-bar #mc_embed_signup {
	max-width: 400px;
	display: block;
	margin: 10px auto;
}

.after-entry-signup-bar #mc_embed_signup .email {
	width: 100%;
	border-radius: 0;
}

.after-entry-signup-bar #mc_embed_signup .button {
	padding: 7px 20px !important;
	width: 100%;
	margin-top: 10px;
}

.top-signup-bar .mc-field-group {
	float: left;
}

.after-entry-signup-bar label {
	display: none;
