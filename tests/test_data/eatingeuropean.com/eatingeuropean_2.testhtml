	margin-top: 10px;
}

.home-popular-categories {
	background: #613c3710;
	width: 100vw;
  position: relative;
  left: 50%;
  right: 50%;
  margin: 0 -50vw;
  padding: 32px 0 30px;
}

.home-popular-categories .wp-block-uagb-image__figure {
	position: relative;
}

.home-popular-categories figcaption {
	position: absolute;
	background: #613c37;
	margin: auto;
	left: 0;
	right: 0;
	bottom: -20px;
	font-size: 20px;
	text-transform: uppercase;
	max-width: 180px;
	padding: 10px 20px;
