}

.home-trending-recipes .right-column .uagb-post-grid-byline {
	display: none !important;
}

.uagb-post__inner-wrap {
	background: #375C61 !important;
	transition: 0.2s all ease;
}

.uagb-post__inner-wrap .uagb-post__title {
	color: #FFF !important;
}


.uagb-post__inner-wrap:hover {
	outline: 4px solid #375C61;
	outline-offset: 1px;
	transition: 0.2s all ease;
