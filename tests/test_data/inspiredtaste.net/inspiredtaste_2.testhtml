            const mediaId = style.split('/media/')[1]?.split('/poster.jpg')[0]; 
            if (mediaId) { 
                console.log('Media Id:', mediaId); 
                const mediaLink = await fetchMediaLink(mediaId); 
                let adthriveVideoTitle; 
