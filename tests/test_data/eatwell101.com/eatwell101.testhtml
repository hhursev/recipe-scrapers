      var servingsInit = parseFloat(servbisInit);
      var resultInit = servingsInit * 1;
      var finalInit = jQuery(this).html().replace(servingsInit, init);
      jQuery(this).html(finalInit);







      // operation
      var serv = $(this).html();
      var servbis = serv.match(/(\d*)(\.\d+|(\s+\d*\s*)?\s*\/\s*\d+|(\-\d*\s*)?\s*\/\s*\d+|(\&\d*\s*)?\s*\/\s*\d+|\s*[\u00BC-\u00BE\u2150-\u215E]+|(\-[\u00BC-\u00BE\u2150-\u215E])?)?/);


      var servings = parseFloat(servbis);

