        <script>
            var namesForNutrients = units = {
                "ENERGY": "Energy",
                "FAT": "Fat",
                "PROTEIN": "Protein",
                "CARB": "Carbs",
                "FAT_SAT": "Saturated Fat",
                "FAT_POLY": "Polyunsat Fat",
                "FAT_MONO": "Monounsat Fat",
                "SUGAR": "Sugar",
                "CHOLE": "Cholesterol",
                "SODIUM": "Sodium",
                "POTAS": "Potassium",
                "FIBER": "Fiber",
            }

            function postData() {

                // reset values in case user changed the scale for the ingredients
                jQuery("#rco ul li").not("p").each(function(){
                var init = jQuery(this).attr("data-amount");
                var servInit = jQuery(this).html();
                var servbisInit = servInit.match(/(\d*)(\.\d+|(\s+\d*\s*)?\s*\/\s*\d+|(\-\d*\s*)?\s*\/\s*\d+|(\&\d*\s*)?\s*\/\s*\d+|\s*[\u00BC-\u00BE\u2150-\u215E]+|(\-[\u00BC-\u00BE\u2150-\u215E])?)?/);
                var servingsInit = parseFloat(servbisInit);
                var resultInit = servingsInit * 1;
                var finalInit = jQuery(this).html().replace(servingsInit, init);
                jQuery(this).html(finalInit);
                });








                // OK Let's go
                var xhr = new XMLHttpRequest();
                var url = "nutri/api/calculate/text";
                xhr.open("POST", url, true);
                xhr.setRequestHeader("Content-Type", "application/json");
                xhr.onreadystatechange = function () {
                    if (xhr.readyState === 4 && xhr.status === 200) {
                        var json = JSON.parse(xhr.responseText);
                        // Show modal before creating chart
                        document.getElementById("result-modal").style.display = "block";
                        //createChart(json);
                        fillNutritionTable(json);
                    }
                };
                //var ings = document.getElementById("rco").innerText.split("\n");

                var geting = document.getElementById("rco").innerText;
                //var sub = geting.substr(0,geting.length-1);
                var ings = geting.split("\n");

                var serv = document.getElementsByClassName("servings")[0].innerText;
                var servbis = serv.match(/(\d+)/);
                var servings = parseInt(servbis);

                var databrut = JSON.stringify({ "ingredients": ings, "servings": servings });
                var databrutbis = databrut.replace(/,\"\",/g, ',');
                var data = databrutbis.replace(/,\"\"/g, '');
                xhr.send(data);
                console.log(data);
            }
            function fillNutritionTable(data) {
                data = Object.entries(data.nutrition);
                var table = document.getElementById("nutrition-table-body");
                for (var i = 0; i < data.length; i++) {
                    // Prepare data
                    var name = namesForNutrients[data[i][0]];
                    var unit = data[i][1][2];
                    var summary = `${data[i][1][0]} ${unit}`;
                    var serving = `${data[i][1][1]} ${unit}`;
                    // Create a row
                    var row = document.createElement("div");
                    row.className = "siimple-table-row"
                    // Fill in row
                    var name_div = document.createElement("div");
                    name_div.className = "siimple-table-cell";
                    name_div.innerHTML = name;
                    var summary_div = document.createElement("div");
                    summary_div.className = "siimple-table-cell";
                    summary_div.innerHTML = summary;
                    var serving_div = document.createElement("div");
                    serving_div.className = "siimple-table-cell";
                    serving_div.innerHTML = serving;
                    row.appendChild(name_div);
                    row.appendChild(summary_div);
                    row.appendChild(serving_div);
                    // Add row to table
                    table.appendChild(row);
                }
                // Replace Kcal by Cal
                var str1 = document.getElementsByClassName("siimple-table-cell")[4].innerText;
                cal = str1.replace("kcal","cal");
                document.getElementsByClassName("siimple-table-cell")[4].innerText = cal;
                var str2 = document.getElementsByClassName("siimple-table-cell")[5].innerText;
                cal = str2.replace("kcal","cal");
                document.getElementsByClassName("siimple-table-cell")[5].innerText = cal;





                //Update calorie in post metadata










            }
            function clearNutritionTable() {
                var table = document.getElementById("nutrition-table-body");
                table.innerHTML = '';
            }
            document.getElementById("toggle_nutrigo").addEventListener("click", function () {
                input = document.getElementById("rco").textContent.trim();
                if (!input) {
                    alert("Add some ingredients first");
                } else {
                    postData();
                    clicky.log('#nutrition','Nutrition');
                }
            });
            document.getElementById("modal-close").addEventListener("click", function () {
                document.getElementById("result-modal").style.display = "none";

                clearNutritionTable();
            });
        </script>

