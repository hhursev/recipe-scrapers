<style id='sitecare-block-core-button-inline-css' type='text/css'>
header .wp-block-button {
	.wp-block-button__link {
		vertical-align: middle;
		
		&:hover,
		&:focus {
			.has-text-color, & {
				color: var(--wp--preset--color--base) !important;
			}
			background-color: var(--wp--preset--color--primary) !important;
		}
	}


	.icon-user-profile {
		mask-image: url(/wp-content/themes/gfss_rebrand/src/block-styles/../icons/user-profile.svg);
	}
	.icon-user-plus {
		mask-image: url(/wp-content/themes/gfss_rebrand/src/block-styles/../icons/user-plus.svg);
	}

	.icon-user-profile,
	.icon-user-plus {
		--icon--size: 1.25rem;
		display: block;
		background-color: currentColor;
		mask-position: center;
		mask-size: contain;
		mask-repeat: no-repeat;
		height: var(--icon--size);
		width: var(--icon--size);
		margin-right: 8px;
		position: relative;
		top: 1px;
	}
}
</style>
