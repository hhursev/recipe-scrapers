function leukeu_track_save(elm, $pid){

	if(!$pid) return;

	//if(jQuery(elm).hasClass('is-liked')) return;

	jQuery.post('/wp-admin/admin-ajax.php', {
		action: 'leukeu_save_recept',
		data: $pid,
		wuid: window.wuid,
		dataType: "json"
	}, function(json) {
		if(json.status == 'success'){
			if(json.action == 'saved'){
				dataLayer.push({
				  	'event': 'save_recipe',
				  	'eventLabel': json.post_title,
				  	'recepten': {
				    	'items': [{
					      	'item_name': json.post_title,
					      	'item_id': $pid,
					      	'item_moeilijkheid': json.moeilijkheid,
					      	'item_zuivel': json.zuivel,
					      	'item_diverse': json.diverse,
					      	'item_extra_thema': json.extra_thema,
					      	'item_dieet': json.dieet,
					      	'item_course': json.course,
					      	'item_cuisine': json.cuisine,
					      	'item_tijd': json.tijd,
					      	'item_soort': json.soort,
					      	'item_thema': json.thema,
					      	'item_groenten_fruit': json.groenten_fruit,
					      	'item_vlees_vis': json.vlees_vis,
					      	'item_category': json.categories
					    }]
					}
				});
			}



		}
	});

}

