function onst_getCookie(name) {
	const value = `; ${document.cookie}`;
	const parts = value.split(`; ${name}=`);
	if (parts.length === 2) return parts.pop().split(';').shift();
	return false;
};

var inDebounce = false;
window['leukeu_track_onstuimig'] = function(eventname){
	if (inDebounce) clearTimeout(inDebounce);

	inDebounce = setTimeout(() => {
		dataLayer.push({
			'event': eventname,
			'eventLabel': 'Pasta met paddenstoelen en champignons',
			'eventLabody_classel': 'Pasta met paddenstoelen en champignons',
			'logged_in': (onst_getCookie('leukeu_user_logged_in') ? '1':'0'),
