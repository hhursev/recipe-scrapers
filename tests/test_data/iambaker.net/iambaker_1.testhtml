h id="a" d="M17.535 20.719H21C21 10.954 12.922 3 3 3v3.397c8.01 0 14.535 6.424 14.535 14.322zm-6.12 0h3.465c0-6.447-5.333-11.7-11.88-11.7v3.397c2.25 0 4.354.867 5.951 2.43a8.203 8.203 0 012.464 5.873zm-6.019-.023c1.317 0 2.397-1.046 2.397-2.351 0-1.294-1.08-2.351-2.397-2.351C4.07 15.994 3 17.05 3 18.345c0 1.305 1.069 2.351 2.396 2.351z"/></defs><use fill-rule="evenodd" xlink:href="#a"/></symbol></defs></svg><style type="text/css">.wprm-template-iab-header {
background: var(--wp--preset--color--quinary);
padding-left: 32px;
padding-right: 32px;
position: relative;
text-align: center;
}

.wprm-template-iab-header::before {
content: "";
display: block;
width: 100%;
height: 171px;
background: var(--wp--preset--color--background);
position: absolute;
top: 0;
left: 0;
z-index: 1;
}

.wprm-template-iab-header > * {
position: relative;
z-index: 2;
}

.wprm-recipe-image img {
box-shadow: var(--wp--custom--box-shadow--1);
margin-bottom: 16px;
}

h2.wprm-recipe-name {
margin: 32px 0;
}


.wprm-recipe-details-unit {
font-size: inherit;
}

.wprm-template-iab-header .wprm-recipe-details-label {
font-weight: 900;
display: block;
}

.wprm-template-iab-header .wprm-recipe-meta-container {
border-left-width: 0 !important;
font-size: var(--wp--preset--font-size--tiny);
margin-bottom: 32px;
display: flex;
align-items: center;
flex-wrap: nowrap;
}

.wprm-template-iab-header .wprm-recipe-meta-container > * {
flex-basis: 0;
flex-grow: 1;
}

.wprm-template-iab-header .wprm-recipe-meta-container > * + * {
border-left: 1px solid var(--wp--preset--color--foreground);
}

.wprm-recipe-summary {
margin-bottom: 32px;
}

.wprm-template-iab-buttons {
display: flex;
align-items: center;
justify-content: center;
gap: 12px;
padding-bottom: 32px;
}

.wprm-template-iab-buttons a {
background: #fff;
border: 1px solid var(--wp--preset--color--foreground);
color: var(--wp--preset--color--foreground);
font-weight: 600;
line-height: var(--wp--custom--line-height--medium);
display: block;
padding: 12px;
text-decoration: none;
}

.wprm-template-iab-buttons a:hover {
background: var(--wp--preset--color--primary);
}

.wprm-recipe-equipment-container,
.wprm-recipe-ingredients-container,
.wprm-recipe-instructions-container,
.wprm-recipe-video-container,
.wprm-recipe-notes-container,
.wprm-recipe-footer-container {
background: var(--wp--preset--color--background);
border: 8px solid var(--wp--preset--color--quinary);
border-top-width: 0;
padding: 24px;
}

.wprm-recipe h3 {
font-size: var(--wp--preset--font-size--big);
margin-bottom: 16px;
}

.wprm-recipe h4 {
font-size: var(--wp--preset--font-size--normal);
margin-bottom: 16px;
}

.wprm-recipe-equipment li .wprm-checkbox-container,
.wprm-recipe-ingredients li .wprm-checkbox-container,
.wprm-recipe-instructions li .wprm-checkbox-container {
top: 8px;
}

ul.wprm-recipe-ingredients,
ul.wprm-recipe-instructions {
margin-bottom: 32px;
}

.wprm-recipe-footer-container {
font-size: var(--wp--preset--font-size--tiny);
}

.wprm-nutrition-toggle {
background: #fff;
border: 1px solid var(--wp--preset--color--foreground);
display: inline-block;
padding: 12px;
font-size: var(--wp--preset--font-size--normal);
font-weight: 600;
line-height: var(--wp--preset--font-size--medium);
margin-bottom: 16px;
}

.wprm-nutrition-toggle:hover {
background: var(--wp--preset--color--primary);
}

.wprm-nutrition-label-container {
margin-bottom: 16px;
}

.wprm-recipe-iab-cta {
background: var(--wp--preset--color--quinary) url(/wp-content/themes/iambaker-2022/assets/icons/logo/default.jpg) no-repeat right 16px center;
background-size: auto 82%;
background-blend-mode: darken;
padding: 32px;
line-height: var(--wp--custom--line-height--medium);
}

.wprm-recipe-iab-cta p {
max-width: 543px;
}

.wprm-recipe p.wprm-recipe-iab-cta__title {
font-family: var(--wp--preset--font-family--display);
font-weight: 900;
font-size: var(--wp--preset--font-size--big);
line-height: var(--wp--custom--line-height--tiny);
margin: 0 0 8px;
}


@media only screen and (max-width: 767px) {
.wprm-recipe-container {
margin-left: calc( -1 * var(--wp--custom--layout--padding) );
margin-right: calc( -1 * var(--wp--custom--layout--padding) );
}

.wprm-recipe-equipment-container,
.wprm-recipe-ingredients-container,
.wprm-recipe-instructions-container,
.wprm-recipe-video-container,
.wprm-recipe-notes-container,
.wprm-recipe-footer-container {
padding-left: var(--wp--custom--layout--padding);
padding-right: var(--wp--custom--layout--padding);

}

.wprm-template-iab-header .wprm-recipe-meta-container {
flex-wrap: wrap;
}

.wprm-template-iab-header .wprm-recipe-meta-container > * {
flex-basis: 50%;
margin-bottom: 16px;
}

.wprm-template-iab-header .wprm-recipe-meta-container > *:nth-child(3) {
border-left: 0;
}

.wprm-recipe-iab-cta {
background-position: right -40% center;
}


}
