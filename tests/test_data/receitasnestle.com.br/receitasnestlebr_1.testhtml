<script>
const currentTime = new Date().getTime();
const timeFloatButtom = localStorage.getItem('timeFloatButtom');

const btnFloatBottom = document.getElementById("button-float-bottom");
const btnFloatClose = document.querySelector(".btn-close-float");

const timeFloat = currentTime + (30 * 60 * 1000); // 30 minutes in milliseconds
if(!timeFloatButtom){
  localStorage.setItem('timeFloatButtom', timeFloat);
}else{
  if (currentTime >= timeFloatButtom) {
    localStorage.setItem('timeFloatButtom', timeFloat);
    floatBottom()
  }else{
    btnFloatClose.classList.add('hidden');
    btnFloatBottom.classList.add('hidden');

  }
}

function floatBottom(){
  btnFloatBottom.classList.remove('hidden');
  btnFloatClose.classList.remove('hidden');
}

btnFloatClose.addEventListener("click", function (e) {
  e.preventDefault();
  this.classList.add('hidden');
  btnFloatBottom.classList.add('hidden');
});
</script>
