                <script type="text/javascript">
	var userAgent = window.navigator.userAgent.search('Node');
	if(userAgent <= -1){
		(function(){
			var container = $(".receitas-relacionadas .lista-receitas"), palavraschave = $("#palavraschave_para_empilhamentos");

			if(container.length == 0){
				return false;
			}else if(palavraschave.length == 0){
				container.hide();
			}

			palavraschave = palavraschave.val().replace(", ", ",").split(",");
			criteria = [],
			limit = "4";
			var url = "https://" + window.location.host + "/bancos_editoriais_search/4e4ec4241a327a457c000007/_find";

			$(palavraschave).each(function(i, e){
				criteria.push('{"palavraschave_para_empilhamentos":{"$regex":"' + e + '", "$options":"ig"}}');
			})

			for(var tt = palavraschave.length -1; tt > 0; tt--){
				criteria.push('{"palavraschave_para_empilhamentos":{"$regex":"' + palavraschave[tt] + '", "$options":"ig"}}');
			}

			criteriaDesc = [];
			for(cCriteria = criteria.length; cCriteria > 0; cCriteria--){
				criteriaDesc.push(criteria[cCriteria - 1]);
			}

			criteria = '{"titulo_da_receita" : {"$nin" : ["' + $('.receita .header h2').html() + '",""]}, "$or":[' + criteriaDesc.join(',') + ']}';
			$.ajax({
				url: url,
				dataType: 'json',
				data: {'criteria' : criteria, 'limit' : limit},
				success: function(data){
					if(data.results.length == 0){
						container.parent().remove();
					}

					var html = "";
					//trata url para https
					for (var k in data.results){
						for (var i in data.results[k]){
							if(data.results[k][i].indexOf("http:") != -1) {
								data.results[k][i] = data.results[k][i].replace("http:", "https:");
							}
						}
					}
					$(data.results).each(function(i, e){
						var tempoPreparo = e.tempo_de_preparo.split(" ", 1);
						var porcoes = e.pores;

						html += '<li class="item">';

						if(typeof e.imagem_da_busca_da_receita == "undefined" || e.imagem_da_busca_da_receita.length <= 0){
							imagem = 'https://i0.statig.com.br/canais/receitas/images/CHE12612-default_chef-316x198.jpg';
						}else{
							imagem = e.imagem_da_busca_da_receita;
						}

						imagem = imagem.replace("http://", "https://");

						html += '<div class="col-sm-5 col-xs-12 rr-imagem">'+
									'<a class="a-link box-imagem" href="' + e.url +  '" >' +
										'<img src="' + imagem + '" >'+
									'</a>'+
								'</div>';

						html += '<div class="col-sm-7 col-xs-12 rr-descricao">'+
									'<div class="box-info receitanamesa rnm-recipe-info--inline">'+
										'<cite class="citacao-imagem">' + e.categorias.split(",")[0] + '</cite>' +
										'<h5>' +
											'<a class="a-link titulo" href="' + e.url + '" >' + e.titulo_da_receita + '</a>' +
										'</h5>' +
										'<div class="box-tempo">' +
											'<div class="box-icon">' +
												'<div class="rnm-icn rnm-icn-tempo icn-minutos"></div>' +
											'</div>' +
											'<div class="box-desc">' +
												'<p class="txt-tempo">Min</p>' +
												'<p class="txt-number-tempo">'+ tempoPreparo +'</p>' +
											'</div>' +
										'</div>' +
										'<div class="box-porcoes">' +
											'<div class="box-icon">' +
												'<div class="rnm-icn rnm-icn-porcoes icn-porcoes"></div>' +
											'</div>' +
											'<div class="box-desc">' +
												'<p class="txt-porcoes">Porcões</p>' +
												'<p class="txt-number-porcoes">'+ porcoes +'</p>' +
											'</div>' +
										'</div>' +
									'</div>'+
								'</div>'+
							'</li>';
					})
					container.html(html);
				},
				error: function(){
					container.parent().remove();
				}
			});
		})()
	}
</script>
