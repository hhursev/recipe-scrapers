    /* http://js.statig.com.br/noticias/bottom_14.js*/
    function defineAjax(url, poststr, func) {
        /*
            alert('url: ' + url);
            alert(poststr);
            alert(func);
        */
        $.ajax({
            url: url,
            type: 'GET',
            data: poststr,
            contentType: "application/x-www-form-urlencoded; charset=utf-8",
            timeout: 100000,
            error: function(){
                alert('Houve um problema no envio. Tente novamente mais tarde.');
            },
