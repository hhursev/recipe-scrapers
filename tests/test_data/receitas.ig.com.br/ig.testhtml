                <script type="text/javascript">
    /* inicio - funcionalidade imprimir receita personalizado.  */
    var btnImprimir = document.querySelector(".btn-imprimir");
    btnImprimir.addEventListener("click", function(){
        tela_impressao = window.open();
        /* base64 imagem da receita para print*/
        function getBase64Image(el, callback) {
            var img = new Image();
            img.crossOrigin = 'Anonymous';
            img.onload = function() {
                var canvas = document.createElement('canvas');
                var ctx = canvas.getContext('2d');
                var dataURL;
                canvas.height = this.naturalHeight;
                canvas.width = this.naturalWidth;
                ctx.drawImage(this, 0, 0);
                dataURL = canvas.toDataURL();
                callback(dataURL);
            };
            img.src = el.src;
            if (img.complete || img.complete === undefined) {
                img.src = "data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///ywAAAAAAQABAAACAUwAOw==";
                img.src = el.src;
            }
        }

        getBase64Image(document.querySelector("#galleria img"), readyPrintPage);

        function readyPrintPage(base64){
            var http;
            var el = document.querySelector(".header-receita");
            var header = el.cloneNode(true);
            header.querySelector("#galleria img").src = base64;
            var headerReceita = header.innerHTML;

            if (window.XMLHttpRequest) {
                http = new XMLHttpRequest();
            } else {
                /* code for older browsers*/
                http = new ActiveXObject("Microsoft.XMLHTTP");
            }
            http.onreadystatechange = function() {
                if (this.readyState == 4 && this.status == 200) {
                    var css = http.responseText;
                    var textoReceita = document.querySelector("#texto-receita").innerHTML;
                    var header =  '<html>' +
                                    '<head>' +
                                        '<meta charset="UTF-8">' +
                                        '<title>Receita</title>' +
                                        '<style>'+  css +'</style>' +
                                    '</head>'+
                                    '<body id="print">' +
                                        '<div class="container">';

                    var footer =        '</div>'+
                                        '<script type="text/javascript">' +
                                            'window.print()' +
                                        '<'+'/script>' +
                                    '</body>' +
                                    '</html>';
                    var html = header + headerReceita + textoReceita + footer;
                    tela_impressao.document.write(html);
                }
            };
            http.open('GET', '//statig0.akamaized.net/sass-canais/canaisiGv04/receitas/css/print.min.css', true);
            http.send();
        };
    })
    /* fim - funcionalidade imprimir receita personalizado.*/

    /**** inicio funcionalidade: aumentar e diminuir fontes da receita ****/
    /* função para clique do aumentar fonte*/
    function aumentarFonte(){
        var itens = pegaTextoReceita();
        for (var k = 0; k < itens.length; k++){
            var fontSize = validaTamanhoFonte(itens[k], "aumentar");
            if(fontSize != false){
                itens[k].style.fontSize = somaFonte(fontSize) + "px";
            }
        }
    }
    /* função para clique do diminuir fonte*/
    function diminuirFonte(){
        var itens = pegaTextoReceita();
        for (var k = 0; k < itens.length; k++){
            var fontSize = validaTamanhoFonte(itens[k], "diminuir");
            if(fontSize != false){
                itens[k].style.fontSize = subtraiFonte(fontSize) + "px";
            }
        }
    }
    /* função que monta o array de itens que devem ser aplicado o aumentar/diminuir texto.*/
    function pegaTextoReceita(){
        var elementosAlvo = [];
        var liIngredientes = document.querySelectorAll(".lista-ingredientes .ingredientes");
        var pDescReceita = document.querySelectorAll(".box-descricao-receita p");
        var pInfoNutricao = document.querySelectorAll(".info-nutrition p");
        var pModoPreparo = document.querySelector(".box-preparo p");

        for (var k = 0; k < liIngredientes.length; k++){
            elementosAlvo.push(liIngredientes[k]);
        }

        for (var i = 0; i < pDescReceita.length; i++){
            elementosAlvo.push(pDescReceita[i]);
        }

        for (var j = 0; j < pInfoNutricao.length; j++){
            elementosAlvo.push(pInfoNutricao[j]);
        }

        elementosAlvo.push(pModoPreparo);
        return elementosAlvo;
    }
    /* função que valida valida tamanho atual da fonte para não permitir aumentar mais que 24px*/
    function validaTamanhoFonte(item, acao){
        var itemStyle = window.getComputedStyle(item);
        var fontSize = parseInt(itemStyle.getPropertyValue('font-size').replace("px", ""));

        if(acao != "diminuir"){
            if(fontSize < 24){
                return fontSize;
            }else{
                return false;
            }
        }else{
            if(fontSize > 18){
                return fontSize;
            }else{
                return false;
            }
        }
    }
    /* função que faz a soma para aumentar a fonte*/
    function somaFonte(valor){
        var fontSize = valor + 2;
        return fontSize.toString();
    }
    /*função que faz a subtração para diminuir a fonte*/
    function subtraiFonte(valor){
        var fontSize = valor - 2;
        return fontSize.toString();
    }
    /* botões com eventos de clique */
    var btnAumentarTexto = document.querySelector(".aumentar");
    var btnDiminuirTexto = document.querySelector(".diminuir");

    /* eventos de clique*/
    btnAumentarTexto.onclick = aumentarFonte;
    btnDiminuirTexto.onclick = diminuirFonte;
    /**** fim funcionalidade: aumentar e diminuir fontes da receita ****/

    var infoReceitaLi = document.querySelectorAll(".info-receita li");
    for(var k = 0; k < infoReceitaLi.length; k++){
        if(infoReceitaLi[k].textContent == ""){
            infoReceitaLi[k].parentNode.removeChild(infoReceitaLi[k]);
        }
    }

</script>
