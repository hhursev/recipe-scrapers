        var poststr = "mensagem=" + encodeURI( mensagem ) + "&name="+encodeURI( nome ) + "&destino="+encodeURI( destino ) + "&titulo=" +
        encodeURI( titulo ) + "&vlink="+ vlink + "&email="+encodeURI( email );

        var htmlsucess = '<p class="mensagem_enviada">'+'<strong>Sua mensagem foi enviada com sucesso!</strong>    </p>';

        defineAjax('/cs/Satellite?pagename=MiG%2FFormularios%2FenviarPorEmail&site=comida&sitepfx=CMD', poststr, function(){
            $('div.enviarporemail').html(htmlsucess);
            /* bug no IE6*/
            $('div.enviarporemail').css("border", "0px solid #FFF");
        })
    }

    /* Validação vários emails*/
    function validateEmail(value){
        return /^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i.test(value);
    }

    $.validator.addMethod('multemail', function (value) {
        var semErros = true;
        var emails = $.trim(value).split(',');
        $.each(emails, function(index, value) {
            if(validateEmail(value) == false && semErros == true){
                semErros = false;
            }
        });
        return semErros;
    }, 'Digite e-mail(s) válido(s)');

    $("#formResponderE").validate({
