            seuEmail:{required: "Digite um e-mail válido",email: "Digite um e-mail válido"},
            emailDestino:{required: "Digite e-mail(s) válido(s)",email: "Digite e-mail(s) válido(s)"}
        },
        submitHandler: function() {
            enviaPE();
            $('#boxEmail').html(
                '<div class="top">'+'</div>'+
                '<div class="titulo">'+
                    '<strong>enviar por e-mail</strong>'+
                    '<a href="javascript:void(0)" title="Fechar" class="close" onclick="$.colorbox.close()">'+'</a>'+
                '</div>'+
                '<div class="cont">'+
                    '<big>Mensagem enviada com sucesso.</big>'+
                    '<small>'+'</small>'+
                    '<br class="clear">'+
                '</div>'+
                '<div class="bot">'+'</div>'
            );
        }
    });

    $(document).ready(function() {
        $("#boxEmail").colorbox({width:"50%", inline:true, scrolling:false, href:"#boxEmail"});
    });

</script>
