<script type="text/javascript">
    $(document).ready(function () {
        $('ul.sf-menu').superfish({
            animation: { opacity: 'show' },
            speed: 'fast',
            speedOut: 'fast',
            delay: 150,
            autoArrows: false,
            cssArrows: false,
            dropShadows: true,
            disableHI: true
        });
        var sourceSwap = function (activeImage) {
            var newSource = activeImage.attr('title');
            activeImage.attr('title', activeImage.attr('src'));
            activeImage.attr('src', newSource);
        }
        var checkActiveImage = function () {
            var myUl = $(this).closest('ul').attr('class');
            var navImage = $(this).closest('ul').prev().children('img');
            var navImageClass = $(this).closest('ul').prev().children('img').attr('class');
            if (navImageClass == 'inactiveMainNav') {
                sourceSwap(navImage);
            };
        }

        $('.inactiveMainNav').hover(
                                function () {
                                    var myActiveImage = $(this);
                                    sourceSwap(myActiveImage);
                                },
                                function () {
                                    var myActiveImage = $(this);
                                    sourceSwap(myActiveImage);
                                });

        $('.mainNavDropdown').hover(checkActiveImage, checkActiveImage);


        $(".dropdown img.flag").addClass("flagvisibility");

        $(".dropdown dt a").click(function () {
            $(".dropdown dd ul").toggle();
        });

        $(".dropdown dd ul li a").click(function () {
            var text = $(this).html();
            $(".dropdown dt a span").html(text);
            $(".dropdown dd ul").hide();

            newsearch(getSelectedValue("sample"));
        });

        function getSelectedValue(id) {
            return $("#" + id).find("dt a span.value").html();
        }

        $(document).bind('click', function (e) {
            var $clicked = $(e.target);
            if (!$clicked.parents().hasClass("dropdown"))
                $(".dropdown dd ul").hide();
        });


        $("#flagSwitcher").click(function () {
            $(".dropdown img.flag").toggleClass("flagvisibility");
        });
    });
</script>
   
