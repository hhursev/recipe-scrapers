      // Remove hidden items from the visible menu
      hiddenItems.forEach(item => {
        menu.removeChild(item);
      });

      // Append hidden items to the dropdown
      moreMenu.innerHTML = '';
      hiddenItems.forEach(item => {
        const clone = item.cloneNode(true);
        moreMenu.appendChild(clone);
      });
    } else {
      moreBtn.style.display = 'none'; // Hide "plus" button
      hiddenItems.forEach(item => menu.appendChild(item)); // Return hidden items to the visible menu
      hiddenItems = [];
      moreMenu.innerHTML = ''; // Clear the dropdown
    }
  }

  // Toggle the dropdown visibility on click
  moreBtn.addEventListener('click', function () {
    moreMenu.style.display = moreMenu.style.display === 'block' ? 'none' : 'block';
  });

  // Run overflow check on window resize and initial load
  window.addEventListener('resize', checkOverflow);
  window.addEventListener('load', checkOverflow);
        
