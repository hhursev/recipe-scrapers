<!-- datalayer footer scripts -->
<script>
    function myDLbuyButton(element) {
        const dlbid = element.getAttribute("dlbid");
        const dlbslg = element.getAttribute("dlbslg");
        const dlbisbn = element.getAttribute("dlbisbn");
        const dlbdiv = element.getAttribute("dlbdiv");
        const dlbgnr = element.getAttribute("dlbgnr");
        const dlbfmt = element.getAttribute("dlbfmt");
        const dlbret = element.getAttribute("dlbret");
        const dlbath = element.getAttribute("dlbath");
        const dltag = element.getAttribute("dltag");
        window.adobeDataLayer.push({
            event: "interaction",
            interaction: {
                retailerClick: {
                    authors: dlbath,
                    bookFormat: dlbfmt,
                    bookGenres: dlbgnr,
                    bookTitle: dlbslg,
                    isbn: dlbisbn,
                    retailer: dlbret,
                    workID: dlbid,
                    workTitle: dlbslg,
                    tagName: dltag
                }
            }
        });
    }

    function myDLrecipeButton(element) {
        const linkname = element.getAttribute("linkname");
        const navelement = element.getAttribute("navelement");
        const recipename = element.getAttribute("recipename");

        window.adobeDataLayer.push({
            event: "interaction",
            interaction: {
                navigationElement: navelement,
                linkName: linkname,
                recipeTitle: recipename
            }
        });
    }

    function mytfdlmenufunction(element) {
        const tfdlmenuitem = element.getAttribute("tfdlmenuitem");
        window.adobeDataLayer.push({
            page: {
                pageInfo: {
                    pageSource: tfdlmenuitem
                }
            }
        })
    }
</script>
<!-- end: datalayer footer scripts -->
