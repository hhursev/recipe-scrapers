            (() => {
                // removing the # from URL is not possible without replaceState or full reload :/
                function removeHash () {
                    var loc = window.location;
                    if ("replaceState" in history)
                        history.replaceState("", document.title, window.location.pathname + window.location.search);
                    else {
                        // really old browsers will keep a #
                        window.location.hash = "";
                    }
                }
                try {
                    var hashContent = window.location.hash.substr(1);
                    var hashData = JSON.parse(atob(hashContent));
                    if (hashData.type !== 'search-tracking') {
                        return;
