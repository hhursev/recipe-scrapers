.arkb-svg.arkb-svg-point,
.arkb-svg.arkb-svg-star {
	fill: var(--highlight);
}

.fa-leaf.ark-block-notice__icon {
	color: var(--primary);
}

.arkb-svg-check.ark-block-notice__icon {
	color: var(--primary);
}

