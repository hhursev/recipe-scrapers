.ez-toc-list {
	border-left: 1px solid;
	padding-left: 20px !important;
	margin-top: 20px !important;
}

a.ez-toc-link {
	padding: 0;
	text-decoration: underline !important;
}





/* Comments */

.comment-respond {
	background: var(--light);
