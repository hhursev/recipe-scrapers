ugars":null,"dietaryFibre":null,"protein":null,"sodium":null}]},{"id":"1395659","typeHandle":"ingredient","label":"Salt","quantity":null,"toServe":false,"unit":[],"ingredient":[{"id":"43","title":"Salt","slug":"salt","uri":null,"groupId":5,"groupHandle":"ingredients","dateCreated":"2019-12-09T11:32:51+00:00","plural":"Salt","overrideNutritionalInformation":false,"volumeToMassConversion":null,"massPerUnit":null,"calories":null,"fat":null,"saturatedFat":null,"carbohydrates":null,"sugars":null,"dietaryFibre":null,"protein":null,"sodium":null}]},{"id":"1395660","typeHandle":"ingredient","label":"Vegetable Oil","quantity":null,"toServe":false,"unit":[],"ingredient":[{"id":"77494","title":"Vegetable Oil","slug":"vegetable-oil","uri":null,"groupId":5,"groupHandle":"ingredients","dateCreated":"2020-02-19T10:24:27+00:00","plural":"Vegetable Oil","overrideNutritionalInformation":false,"volumeToMassConversion":null,"massPerUnit":null,"calories":null,"fat":null,"saturatedFat":null,"carbohydrates":null,"sugars":null,"dietaryFibre":null,"protein":null,"sodium":null}]}],"method":[{"id":"1395642","typeHandle":"step","heading":null,"description":"\u003cp\u003eCombine the plain flour, a pinch of salt and hot water (from the kettle) in a bowl and mix with a wooden spoon. Once it is cool enough to handle, knead the dough for 5 minutes until smooth. Pour in a tablespoon of oil and knead through. Cover and rest for 10 minutes\u003c/p\u003e","time":null,"brightcoveVideoId":null},{"id":"1395643","typeHandle":"step","heading":null,"description":"\u003cp\u003eMeanwhile, prepare the garlic chilli oil. Grate the garlic into a small microwaveable bowl with the chilli powder, ¼ teaspoon salt and 2 tablespoons of oil. Microwave for 1 minute to soften and cook the garlic. \u003c/p\u003e","time":null,"brightcoveVideoId":null},{"id":"1395644","typeHandle":"step","heading":null,"description":"\u003cp\u003eRoughly chop your coriander and finely slice your spring onions.\u003c/p\u003e","time":null,"bri