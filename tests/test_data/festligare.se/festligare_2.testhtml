            <script>
                const openSearchButton = document.querySelector('.Header-fauxSearchLink');
                const searchApp = document.getElementById('search-app');
                //If body has is-state--search mutation observer
                new MutationObserver((mutations) => {

                    mutations.forEach((mutation) => {
                        if (mutation.attributeName === 'class') {
                            const classList = Array.from(mutation.target.classList);
                            if (classList.includes('is-state--search')) {
                                console.log('show');
                                showSearch();
                                focusSearch();
                            } else {
                                hideSearch();
                            }
                        }
                    });
                }).observe(document.body, {
                    attributes: true,
                    attributeFilter: ['class'],
                });

                openSearchButton.addEventListener('mouseenter', () => {
                    document.body.classList.add('is-state--search');
                });
                openSearchButton.addEventListener('mouseleave', () => {
                    const input = searchApp.querySelector('input');
                    if (!input) return;
                    //Check if input has focus
                    if (document.activeElement === input) return;
                    //check if input has text
                    if (input.value.length > 0) return;
                    document.body.classList.remove('is-state--search');
                });
                openSearchButton.addEventListener('click', () => {
                    focusSearch();
                });

                //Check if url has ?sok= !empty
                const urlParams = new URLSearchParams(window.location.search);
                const searchQuery = urlParams.get('sok');

                if (searchQuery && searchQuery.length > 0) {
                    document.body.classList.add('is-state--search');
                }

                function showSearch() {
                    searchApp.style.opacity = 1;
                    searchApp.style.pointerEvents = 'all';
                    searchApp.style.transition = 'opacity 0.5s ease-in-out';
                }

                function focusSearch() {
                    const input = searchApp.querySelector('input');
                    if (!input) return;
                    searchApp.querySelector('input').focus();
                }

                function hideSearch() {
                    const input = searchApp.querySelector('input');
                    if (!input) return;
                    searchApp.style.opacity = 0;
                    searchApp.style.pointerEvents = 'none';
                    searchApp.style.transition = 'opacity 0.5s ease-in-out';
                    input.blur();
                }

                //Add clicable background to close search
                const searchAppCloseArea = document.createElement('div');
                searchAppCloseArea.classList.add('Header-searchCloseArea');
                searchApp.appendChild(searchAppCloseArea);
                searchAppCloseArea.addEventListener('click', () => {
                    document.body.classList.remove('is-state--search');
                });

                //if 1024px or less
                const mediaQuery = window.matchMedia('(max-width: 1024px)');
                if (mediaQuery.matches) {
                    const headerContent = document.querySelector('.js-header-content');
                    headerContent.style.height = "100%";
                }
