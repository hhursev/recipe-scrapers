            if ( window.Zeit.user.state === 'active' ) {
                wt.customParameter[23] = 'angemeldet';
            }
            if ( window.Zeit.user.adFree ) {
                wt.customParameter[46] = 'true';
            }
        }

        wt.sendinfo();
    }
});
</script>



