    if (socialFooterLinks) {
        socialFooterLinks.addEventListener('click', raiseEvent, true);
    }
    
    // social feed links. need to check for when they are moved. annoyingly won't work on the socialLinksContainer
    var socialFeedOriginalLinks = document.querySelector('.socialFeedLinks');
    if (window.MutationObserver && socialFeedOriginalLinks) {
        var observer2 = new MutationObserver(function (mutationsList, observer) {
            setTimeout(function () {
                var socialFeedLinks = document.querySelector('.socialLinksContainer');
                socialFeedLinks.addEventListener('click', raiseEvent, true);
            }, 2000);
            observer.disconnect();
        });
        observer2.observe(socialFeedOriginalLinks, { childList: true, subtree: true });
    }
});
