<script type="text/javascript">
	var userId = 0;
	var userV2Id = 0;
	var initLocalStorage = false;
	var avatarSprite;
	(function () {
		var usernameC = null;
		var c = document.cookie.split(';');
		for (var i = 0; i < c.length; i++) {
			var x = c[i].substr(0, c[i].indexOf('='));
			var y = c[i].substr(c[i].indexOf('=') + 1);
			x = x.replace(/^\s+|\s+$/g, '');
			if (x == 'afperso' || x === 'afpperso') {
				var t = y.split('&');
				for (var j = 0; j < t.length; j++) {
					var a = t[j].substr(0, t[j].indexOf('='));
					if (a === 'cu') userId = unescape(t[j].substr(t[j].indexOf('=') + 1));
					if (a === 'cuv2') userV2Id = unescape(t[j].substr(t[j].indexOf('=') + 1));
					if (a === 'un') usernameC = unescape(t[j].substr(t[j].indexOf('=') + 1));
				}
			}
		}
	})();
	(function () {
		if (window.Mrtn && Mrtn.USER && Mrtn.USER.id > 0) {
			var username = Mrtn.USER.name;
			var avatarUrl = 'https://static.afcdn.com/relmrtn/shim.gif'; // TODO: remove
			var nbUnreadPM = 0;
			try {
				username = localStorage.getItem('username') || Mrtn.USER.name;
				avatarUrl = localStorage.getItem('avatarUrl') || avatarUrl;
				avatarSprite = 'af-avatar avatar-letter avatar-letter-' + username.substring(0, 1);
				nbUnreadPM = localStorage.getItem('nbUnreadPM');
				nbUnreadFavorites = localStorage.getItem('nbUnreadFavorites');
			} catch(e) {}
			if (username && avatarUrl) {
				buildconnectedHeader(Mrtn.USER.id, username, avatarUrl, nbUnreadPM, nbUnreadFavorites);
			} else {
				initLocalStorage = true;
			}
		}
	})();

	function buildconnectedHeader(userId, username, avatarUrl, nbUnreadPM, nbUnreadFavorites) {
		var e = document.querySelector('.top-menu') || document.getElementsByClassName('mrtn-login-zone')[0];
			var classLetter = avatarUrl === 'https://static.afcdn.com/relmrtn/shim.gif' ? ' class="' + avatarSprite + '"' : '';

            var myProfileUrl = '/my-profile/edit';
            var logoutUrl = '/reloaded/users/logout';

			e.innerHTML = '<div class="mrtn-login-link connected">'
							+ '<a href="' + myProfileUrl + '" class="avatar">'
								+ '<span class="mrtn-avatar--empty--xlarge mrtn-avatar--empty mrtn-hide-on-print">' + username.substring(0, 1) + '</span>'
								// + '<span id="unreadMessages" title=""  style="display:' + ((sum > 0) ? 'block' : 'none') + '">' + sum + '</span>'
							+ '</a>'
							+ '<div id="username" class="username">'
								+ '<a class="un" href="' + myProfileUrl + '">'
									+ username
								+ '</a>'
								+ '<br />'
								+ '<a href="' + myProfileUrl + '" class="mrtn-btn mrtn-btn-secondary mrtn-btn-text-only mrtn-btn--small">Mon profil</a>'
								+ '<a href="/espace-perso/mon-carnet-de-recettes.aspx" class="mrtn-btn mrtn-btn-secondary mrtn-btn-text-only mrtn-btn--small">Mes recettes</a>'
							+ '</div>'
							+ '<div class="opts">'
								// + '<i class="icon-icon_plus"></i>'
								+ '<a class="user-setting" href="/espace-perso/mes-parametres-personnels.aspx"><i class="icon-icon_setting"></i></a>'
								+ '<a class="user-unlog" href="' + logoutUrl + '" title="Me déconnecter"><i class="icon-locker-open"></i></a>'
							+ '</div>'
						+ '</div>';
	}
</script>
