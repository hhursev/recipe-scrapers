			.site-footer p {font-size:smaller}

body, .entry-title, .entry-title a, .sidebar .widget-title a {color:#000}

a {
    color: #3e8184;
}

p.entry-meta {
    font-size: 14px;
    line-height: 1.5;
}

a:focus, a:hover {
    color: #4f1b58;
    text-decoration: none;
	font-weight:bold;
}

@media only screen and (min-width: 1200px) {
.modern-menu-desktop-social .simple-social-icons li:nth-child(n+4)  {
    display: inherit;
}
	
.modern-menu-desktop-social .simple-social-icons li.ssi-twitter {display:none !important;}
	

.desktop-inline-modern-menu ul {font-size:smaller;}
}

/*WPRM

.site-container .wprm-recipe-jump, .site-container .wprm-recipe-print {
    background-color: #5a134a !important;
    color: #ffffff;
}
*/

/*Enews*/

.sidebar .enews-widget, .block-sidebar .enews {
    border:10px solid #3e8184; padding:10%;
	background:#fff;
}


.after-entry {padding:0; background-color:none;}


.content .after-entry .widget-title {border-top:none;}

.after-entry .enews {
    border:10px solid #3e8184; padding:10%;
	background-color:#fff;
}

.content .after-entry .enews-widget {
    background: #fff;
    border: none;
    padding: 0;
}

.enews input {
    font-family: inherit;
    font-size: 100%;
    line-height: 1.5;
}

.enews-widget input {
    background: #f7f9fc;
}

/*MBT*/


.mbt-book .mbt-book-buybuttons .mbt-book-buybutton a {border-bottom: none !important}

.mbt-book {
    clear: both;
    padding-bottom: 10px;
	font-family: inherit;}

.mbt-book.mbt-book-single.mbt-display-mode-singlecolumn .mbt-book-section{
font-size:20px;line-height:2.25;
}

.mbt-book.mbt-book-single.mbt-display-mode-singlecolumn .mbt-book-section-title {font-family: 'Signika', sans-serif;}

.mbt-book.mbt-book-single.mbt-display-mode-singlecolumn .mbt-book-title {font-size:30px;}

.mbt-book .mbt-primary-button, .mbt-book .mbt-primary-button:link, .mbt-book .mbt-primary-button:visited {font-family: 'Signika', sans-serif;font-size:20px;}

/*Ninja Forms*/

.nf-form-fields-required, .ninja-forms-req-symbol, .nf-form-title {display:none;}

.nf-form-content button, .nf-form-content input[type=button], .nf-form-content input[type=submit] {
    background: #5a134a !important;
}

.nf-form-content button:hover, .nf-form-content input[type=button]:hover, .nf-form-content input[type=submit]:hover {
    background: #234c62 !important;
    color: #fff !important;
}

.nf-response-msg {
    background-color: #3e8184;
color: #fff;
    padding: 5%;
    margin: 5%;
font-style:italic;
}

.nf-form-content .list-select-wrap .nf-field-element>div, .nf-form-content input:not([type=button]), .nf-form-content textarea {
    color: #000000 !important;
    font-size: 20px;
}

