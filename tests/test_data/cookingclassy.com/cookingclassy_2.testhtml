<style type="text/css">.wprm-glossary-term {color: #5A822B;text-decoration: underline;cursor: help;}</style><style type="text/css">.wprm-recipe-video-container {position:relative;}

.wprm-recipe-template-cc-recipe-2020 {margin:40px 0;background:#f8f8f8;position:relative;padding:30px;font-size:14px;}

.wprm-recipe-template-cc-recipe-2020 h2 {font-weight:bold !important;margin-bottom:15px !important;}
.wprm-recipe-template-cc-recipe-2020 .postactions {margin:0 0 25px 0;overflow:hidden;}
@media screen and (max-width:767px) {
	.wprm-recipe-template-cc-recipe-2020 {padding-left:10px;padding-right:10px;}
}

.wprm-recipe-template-cc-recipe-2020 p {margin-bottom:10px;}

.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container {float:right;display:inline;margin:0 0 20px 40px;width:180px;max-width:35%;text-align:center;}
.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container img {width:100%;display:block;}

.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container svg[width="0"] {display:block !important;}

@media screen and (max-width:479px) {
	.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container {max-width:28.125%;}
}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-rating {font-size:16px;text-align:center;margin-top:10px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-rating-details {font-size:12px;line-height:18px;white-space:normal;margin-top:3px;}

.wprm-recipe-template-cc-recipe-2020 .printlink {margin-top:10px;}
.wprm-recipe-template-cc-recipe-2020 .printlink a {color:inherit;text-decoration:none;padding:9px 15px;display:block;background:#fec24c;border-radius:4px;letter-spacing:letter-spacing:0.05em;transition:background 0.3s;text-transform:uppercase;font-weight:bold;font-size:12px;line-height:18px;}
.wprm-recipe-template-cc-recipe-2020 .printlink a:hover {background:#FDAB31;}
@media screen and (max-width:479px) {
	.wprm-recipe-template-cc-recipe-2020 .printlink .mobhide {display:none;}
}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-summary,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-ingredients-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-meta-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-nutrition-wrap,
.wprm-recipe-template-cc-recipe-2020 .wprm-watch-video {
	margin-bottom:30px;
}

@media screen and (max-width:767px) {
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-summary,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-ingredients-container,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-meta-container,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-nutrition-wrap,
	.wprm-recipe-template-cc-recipe-2020 .wprm-watch-video {
		margin-bottom:25px;
	}
}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-ingredients {margin:0;padding:0;list-style:none;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-ingredients li {margin:0 0 3px 0;padding:0;list-style:none !important;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container ul {margin:1em 0;padding:0 0 0 1.6em;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container ul li {margin:0 0 8px 0;padding:0 0 0 5px;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container ul {margin:0;padding:0 0 0 1.6em;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container ul li {margin:0 0 10px 0;padding:0 0 0 0.3125em;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container ul li p {margin:0;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container img {margin-top:20px;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-servings-container {font-size:16px;line-height:22px;font-weight:bold;margin-bottom:20px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-servings-container .wprm-recipe-details-name {font-weight:600 !important;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container {text-align:center;font-size:14px;line-height:20px;display:flex;flex-wrap:wrap;clear:both;margin-right:-20px;margin-left:-11px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>div {padding:0;margin-bottom:0 !important;box-sizing:border-box;margin:0;border-left:1px solid #ccc;text-align:left;font-size:16px;width:33.333333333333333%;padding-left:10px;padding-right:20px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>div:first-child {border-left:none;padding-left:11px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>.wprm-recipe-total-time-container {width:100%;margin-bottom:20px !important;border-left:none;padding-left:11px;padding-right:0;order:-1;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>.wprm-recipe-total-time-container+div {border-left:none;padding-left:0;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container .wprm-recipe-time {font-size:14px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container .wprm-recipe-time>span {font-size:inherit;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>.wprm-recipe-total-time-container>span {font-size:16px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>.wprm-recipe-total-time-container>.wprm-recipe-details-label {display:inline;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-servings-label,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-time-label {color:#2A3741;}


@media screen and (max-width:767px) {
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container-a>div {padding:0 10px;}
}

.wprm-recipe-template-cc-recipe-2020 .wprm-cc-footer-container {font-size:12px;line-height:18px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-cc-footer-container>div>div {margin-bottom:8px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-cc-footer-container>div>div:last-child {margin-bottom:0;}
.wprm-recipe-template-cc-recipe-2020 .wprm-cc-footer-container span {text-transform:uppercase !important;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-meta-container + .wprm-recipe-author-container {margin-top:-22px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-nutrition-label {margin-top:0;}
.wprm-recipe-template-cc-recipe-2020 .nutrition-disclaimer {margin-top:15px;font-size:12px;line-height:18px;}

.wprm-recipe-template-cc-recipe-2020 h3 {margin:25px 0 20px 0 !important;font-weight:bold !important;}
.wprm-recipe-template-cc-recipe-2020 h4 {margin:20px 0 15px 0 !important;font-weight:bold !important;}
.wprm-recipe-template-cc-recipe-2020 h4.wprm-recipe-group-name {margin-top:20px !important;}

@media screen and (max-width:767px) {
