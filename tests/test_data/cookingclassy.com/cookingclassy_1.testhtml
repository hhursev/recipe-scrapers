<style type="text/css">.wprm-glossary-term {color: #5A822B;text-decoration: underline;cursor: help;}</style><style type="text/css">.wprm-recipe-video-container {position:relative;}

.wprm-recipe-template-cc-recipe-2020 {margin:40px 0;background:#f8f8f8;position:relative;padding:30px;font-size:14px;}

.wprm-recipe-template-cc-recipe-2020 h2 {font-weight:bold !important;margin-bottom:15px !important;}
.wprm-recipe-template-cc-recipe-2020 .postactions {margin:0 0 25px 0;overflow:hidden;}
@media screen and (max-width:767px) {
	.wprm-recipe-template-cc-recipe-2020 {padding-left:10px;padding-right:10px;}
}

.wprm-recipe-template-cc-recipe-2020 p {margin-bottom:10px;}

.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container {float:right;display:inline;margin:0 0 20px 40px;width:180px;max-width:35%;text-align:center;}
.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container img {width:100%;display:block;}

.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container svg[width="0"] {display:block !important;}

@media screen and (max-width:479px) {
	.wprm-recipe-template-cc-recipe-2020 .wprm-cc-side-container {max-width:28.125%;}
}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-rating {font-size:16px;text-align:center;margin-top:10px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-rating-details {font-size:12px;line-height:18px;white-space:normal;margin-top:3px;}

.wprm-recipe-template-cc-recipe-2020 .printlink {margin-top:10px;}
.wprm-recipe-template-cc-recipe-2020 .printlink a {color:inherit;text-decoration:none;padding:9px 15px;display:block;background:#fec24c;border-radius:4px;letter-spacing:letter-spacing:0.05em;transition:background 0.3s;text-transform:uppercase;font-weight:bold;font-size:12px;line-height:18px;}
.wprm-recipe-template-cc-recipe-2020 .printlink a:hover {background:#FDAB31;}
@media screen and (max-width:479px) {
	.wprm-recipe-template-cc-recipe-2020 .printlink .mobhide {display:none;}
}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-summary,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-ingredients-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-meta-container,
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-nutrition-wrap,
