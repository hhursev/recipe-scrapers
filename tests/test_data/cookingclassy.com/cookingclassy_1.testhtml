.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>div:first-child {border-left:none;padding-left:11px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container>.wprm-recipe-total-time-container {width:100%;margin-bottom:20px !important;border-left:none;padding-left:11px;padding-right:0;order:-1;}
