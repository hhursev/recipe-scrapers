.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container img {margin-top:20px;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-servings-container {font-size:16px;line-height:22px;font-weight:bold;margin-bottom:20px;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-servings-container .wprm-recipe-details-name {font-weight:600 !important;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-times-container {text-align:center;font-size:14px;line-height:20px;display:flex;flex-wrap:wrap;clear:both;margin-right:-20px;margin-left:-11px;}
