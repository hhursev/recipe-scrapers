.wprm-recipe-template-cc-recipe-2020 .wprm-cc-footer-container>div>div:last-child {margin-bottom:0;}
.wprm-recipe-template-cc-recipe-2020 .wprm-cc-footer-container span {text-transform:uppercase !important;}

