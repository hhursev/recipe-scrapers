	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-meta-container,
	.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-nutrition-wrap,
	.wprm-recipe-template-cc-recipe-2020 .wprm-watch-video {
		margin-bottom:25px;
	}
}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-ingredients {margin:0;padding:0;list-style:none;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-ingredients li {margin:0 0 3px 0;padding:0;list-style:none !important;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container ul {margin:1em 0;padding:0 0 0 1.6em;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-notes-container ul li {margin:0 0 8px 0;padding:0 0 0 5px;}

.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container ul {margin:0;padding:0 0 0 1.6em;}
.wprm-recipe-template-cc-recipe-2020 .wprm-recipe-instructions-container ul li {margin:0 0 10px 0;padding:0 0 0 0.3125em;}
