<script>
    function sendRecipeOffCanvas() {
        return {
            recipeId: 1406384,
            show: false,
            loading: false,
            open() {
                this.show = true;
            },
            close() {
                this.show = false;
            },
            submit(e) {
                const validator = $(this.$refs.sendRecipeForm).validate({
                    rules: {
                        from_name: {
                            required: true
                        },
                        to_email: {
                            email: true,
                            required: true
                        },
                        to_name: {
                            required: true
                        }
                    },
                    errorClass: "error-message",
                    errorElement: "div"
                });
                if (validator.form()) {
                    this.loading = true;
                    shareRecipe(e, this.recipeId).then(response => response.json()).then((data) => {
                        if (data.errors) {
                            validator.showErrors(data.errors);
                            return;
                        }

                        this.clearFrom($(this.$refs.sendRecipeForm));
                        this.show = false;
                    }).finally(() => {
                        this.loading = false;
                    });
                }
            },
            clearFrom(form) {
                form.trigger("reset");
            }
        };
    }
</script>
