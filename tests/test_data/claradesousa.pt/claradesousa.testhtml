document.addEventListener('DOMContentLoaded', function() {
  // Get the logo element
  var headerLogo = document.getElementById('header-scroll-logo');
	
  var headerLockBar = document.getElementById('header-lock-bar');

  // Hide the logo by default
  headerLogo.style.display = 'none';

  // Add scroll event listener
  window.addEventListener('scroll', function() {
    // Check if scroll position is greater than 80px
    if (window.scrollY > 80) {
      // If scrolled down, show the logo
      headerLogo.style.display = 'block';
	  headerLockBar.style.backgroundColor = "#FAFAFA";
    } else {
      // If scrolled up or at the top, hide the logo
      headerLogo.style.display = 'none';
	  headerLockBar.style.backgroundColor = "#FFFFFF";
    }
  });
});</script>
