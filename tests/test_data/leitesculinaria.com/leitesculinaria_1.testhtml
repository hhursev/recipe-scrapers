a of the traditional Thai hot sauce.","breadcrumb":{"@id":"https://leitesculinaria.com/67202/recipes-homemade-sriracha-sauce.html#breadcrumb"},"mainEntity":[{"@id":"https://leitesculinaria.com/67202/recipes-homemade-sriracha-sauce.html#faq-question-1655159597491"},{"@id":"https://leitesculinaria.com/67202/recipes-homemade-sriracha-sauce.html#faq-question-1655159332891"}],"inLanguage":"en-US","potentialAction":[{"@type":"ReadAction","target":["https://leitesculinaria.com/67202/recipes-homemade-sriracha-sauce.html"]}]},{"@type":"ImageObject","inLanguage":"en-US","@id":"https://leitesculinaria.com/67202/recipes-homemade-sriracha-sauce.html#primaryimage","url":"https://leitesculinaria.com/wp-content/uploads/2021/05/homemade-sriracha-sauce.jpg","contentUrl":"https://leitesculinaria.com/wp-content/uploads/2021/05/homemade-sriracha-sauce.jpg","width":1200,"height":1620,"caption":"Three bottles filled with homemade Sriracha sauce."},{"@type":"BreadcrumbList","@id":"https://leitesculinaria.com/67202/recipes-homemade-sriracha-sauce.html#breadcrumb","itemListElement":[{"@type":"ListItem","position":1,"name":"Home","item":"https://leitesculinaria.com/"},{"@type":"ListItem","position":2,"name":"Recipes","item":"https://leitesculinaria.com/recipe-index"},{"@type":"ListItem","position":3,"name":"Condiments","item":"https://leitesculinaria.com/category/recipes/condiments"},{"@type":"ListItem","position":4,"name":"Homemade Sriracha Sauce"}]},{"@type":"WebSite","@id":"https://leitesculinaria.com/#website","url":"https://leitesculinaria.com/","name":"Leite's Culinaria","description":"Recipes, Food, and Cooking Blog","publisher":{"@id":"https://leitesculinaria.com/#organization"},"alternateName":"LC","potentialAction":[{"@type":"SearchAction","target":{"@type":"EntryPoint","urlTemplate":"https://leitesculinaria.com/?s={search_term_string}"},"query-input":"required name=search_term_string"}],"inLanguage":"en-US"},{"@type":"Organization","@id":"https://leitesculinaria.com/#organization",