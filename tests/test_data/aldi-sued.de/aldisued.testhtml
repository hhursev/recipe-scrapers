    $(".basket_price").on("change", function () {
        initSearchBarWidth();
    })

    $(document).ready(function () {
        initHeader();
    });

    $(window).resize(function () {
        resetHeader();
        if ($(window).width()  <992) {
             initSearchBarMobile();
             }
    });


</script>






