    function resetHeader() {
        if ($(window).width() > 751) {
            $("body").removeClass("mobile-menu-open ios-menu-open"); // iseto: remove mobile style menu on landscape rotation change
            resetSearchBar();
            resetMenu();
            initSubMenu();
            initMenuItems();
        } else {

        }
