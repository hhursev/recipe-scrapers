/**
 * For domain name comparisons using siteKey
 * @param {'atk'|'cio'|'cco'|''} siteKey
 * @returns {'americastestkitchen'|'cooksillustrated'|'cookscountry'|''}
 */
Dry.prototype.domainFromSiteKey = function domainFromSiteKey(siteKey) {
  switch (siteKey) {
