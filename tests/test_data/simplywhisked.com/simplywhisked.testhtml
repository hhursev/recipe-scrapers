	background: none;
	border: none;
	color: var(--global-palette3);
}

.button.more-button:hover:after {
	-webkit-transform:scaleX(1);
	transform: scaleX(1);
}

.button.more-button:after {
	width: calc(100% - 18px);
}

.button.more-button:after {
	content: "";
	position: absolute;
	bottom: 0px;
	left: 9px;
	width: calc(100% - 18px);
	height: 2px;
	background-color: var(--global-palette1);
	-webkit-transform: scaleX(0);
	transform: scaleX(0);
	transition: all .25s ease;
}

