<script type="rocketlazyloadscript" data-minify="1" data-rocket-type='text/javascript' data-rocket-src='https://www.errenskitchen.com/wp-content/cache/min/1/wp-content/plugins/wp-recipe-maker-premium/dist/public-elite.js?ver=1696514782' id='wprmp-public-js' defer></script>
<script type="rocketlazyloadscript" data-rocket-type='text/javascript' data-rocket-src='https://www.errenskitchen.com/wp-content/plugins/luckywp-table-of-contents/front/assets/main.min.js?ver=2.1.4' id='lwptoc-main-js' defer></script>
<style>.lwptoc .lwptoc_i{background-color:#f7f3ed;border:1px solid #cb336c;}.lwptoc_header{color:#234d76;}.lwptoc .lwptoc_i A{color:#000000;}.lwptoc .lwptoc_i A:hover,.lwptoc .lwptoc_i A:focus,.lwptoc .lwptoc_i A:active{color:#cb336c;border-color:#cb336c;}</style><style type="text/css">body:not(.wprm-print) .wprm-recipe-container p:first-letter {
	margin: 0;
	padding: 0;
}

/* -------- [ OC Code ] -------- */

/* [ Containers ] */

.wprm-print {
    width: 650px;
    max-width: calc(100% - 20px);
    margin: 0 auto;
    padding: 0;
}

.wprm-recipe-container { /* only in non-print displays */
    clear: both;
    margin: 8px 0 20px;
    font-size: 18px;
}

.wprm-recipe-container:focus {
    outline: 0; /* remove, as it shows when using the "jump" button */
}

.wprm-recipe {
    overflow: visible;
}

.wprm-recipe-errens-kitchen {
    margin: 100px auto 12px;
}

.wprm-toggle-switch-container {
	margin-bottom: 24px;
}

#nut-disclaimer-message {
	display: none;
}

@media screen and (min-width: 768px) {
    .wprm-recipe-errens-kitchen {
        margin-top: 72px;
        position: relative;
    }
}

.wprm-recipe-container .wprm-recipe-errens-kitchen {
	margin-left: -10px;
	margin-right: -10px;
}

@media screen and (min-width: 768px) {
	.wprm-recipe-container .wprm-recipe-errens-kitchen {
		margin-left: 0;
		margin-right: 0;
	}
}

/* [ Text ] */

.wprm-recipe .wprm-recipe-details-label {
    min-width: auto;
}

.wprm-recipe-details-unit,
.wprm-recipe .wprm-nutrition-label-text-nutrition-unit {
    font-size: 1em;
}

.wprm-recipe-container .wprm-entry-content ol li,
.wprm-recipe-instruction-group ul li {
    margin-left: 8px;
    list-style-type: none !important;
    counter-increment: instruction-counter;
}

.wprm-recipe-container .wprm-entry-content ol li::before,
.wprm-recipe-instruction-group ul li::before {
    content: counter(instruction-counter);
    display: block;
    float: left;
    color: #fff;
    background: #cb346d;
    width: 20px;
    height: 20px;
    line-height: 20px;
    border-radius: 20px;
    text-align: center;
    margin-left: -32px;
    margin-top: 4px;
    font-weight: 600;
    font-size: 14px;
}

.wprm-recipe-container .wprm-recipe .wprm-entry-header .wprm-recipe-header.wprm-recipe-lets-make-header,
.wprm-recipe-container .wprm-recipe .wprm-entry-content .wprm-recipe-header.wprm-recipe-ingredients-header,
.wprm-recipe-container .wprm-recipe .wprm-entry-content .wprm-recipe-header.wprm-recipe-instructions-header,
.wprm-recipe-container .wprm-recipe .wprm-entry-content .wprm-recipe-header.wprm-recipe-notes-header {
    text-indent: -9999px;
	height: 38px;
	background: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/text-ingredients.svg) no-repeat;
	background-size: contain;
    margin-top: 24px;
    margin-top: 0;
    margin-bottom: 8px;
}

.wprm-recipe-container .wprm-recipe .wprm-entry-header .wprm-recipe-header.wprm-recipe-lets-make-header {
	background-image: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/text-lets-make.svg);
	background-position: center;
	height: 44px;
	margin: -56px auto 0;
    position: relative;
	display: block;
}

.wprm-recipe-container .wprm-recipe .wprm-entry-content .wprm-recipe-header.wprm-recipe-instructions-header {
	background-image: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/text-instructions.svg);
	background-size: contain;
	height: 28px;
}

.wprm-recipe-container .wprm-recipe .wprm-entry-content .wprm-recipe-header.wprm-recipe-notes-header {
	background-image: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/text-tips-notes.svg);
	background-size: contain;
}

/* [ Header ] */

.wprm-recipe-container .wprm-entry-header {
    text-align: center;
    position: relative;
	background: #f7f2ed;
}

@media screen and (min-width: 768px) {
	.wprm-recipe-container .wprm-entry-header {
		background: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/bg-texture.jpg) no-repeat;
		background-size: cover;
	}

	.wprm-recipe-container .wprm-recipe-errens-kitchen .wprm-entry-header {
		border-top-left-radius: 8px;
		border-top-right-radius: 8px;
		margin-left: auto;
		margin-right: auto;
		background: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/bg-texture.jpg) center;
		background-size: 362px;
	}
}

@media (-webkit-min-device-pixel-ratio: 2) and (min-width: 768px),
	(min-resolution: 192dpi) and (min-width: 768px) {
	.wprm-recipe-container .wprm-recipe-errens-kitchen .wprm-entry-header {
		background-image: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/bg-texture@2x.jpg);
	}
}

.wprm-recipe-container .wprm-entry-header:after {
	content: '';
	background: url(/wp-content/themes/once-coupled-errens-kitchen-2/assets/images/logo-mark-tan-tilt.svg) no-repeat;
	background-size: contain;
	width: 90px;
    height: 90px;
	display: inline-block;
	margin: 0 12px 0 0;
	position: absolute;
	top: -8px;
   	right: 8px;
}

@media (min-width: 768px) {
	.wprm-recipe-container .wprm-entry-header:after {
		width: 126px;
	    height: 133px;
		display: inline-block;
		margin: 0 12px 0 0;
		top: -9px;
   		right: 0;
	}
}

/* Image */

.wprm-recipe-container .wprm-recipe-image {
    margin: -48px auto 16px;
    border-radius: 100%;
    width: 150px;
    height: 150px;
	position: relative;
	top: -32px;
}

@media (min-width: 768px) {
    .wprm-recipe-container .wprm-recipe-image {
        margin: -20px auto 20px;
		top: -24px;
    }
}

.wprm-recipe-container .wprm-recipe-image img {
    border-radius: 100%;
	width: 100%;
}

/* Name */

.wprm-recipe-container .wprm-entry-header .wprm-recipe-name {
    /* font-weight: 600; */
	font-style: italic;
    font-size: 32px;
	color: #000;
	margin: 4px 16px 16px;
}

@media screen and (min-width: 768px) {
    .wprm-recipe-container .wprm-entry-header .wprm-recipe-name {
        font-size: 38px;
 		margin: 4px 35px 8px;
    }
}

/* Summary */

.wprm-recipe-container .wprm-recipe-summary {
    margin: 4px 36px 8px;
    line-height: 1.4;
	font-size: 15px;
}

@media screen and (min-width: 768px) {
	.wprm-recipe-container .wprm-recipe-summary {
		font-size: 18px;
	}
}

/* Ratings */

.wprm-recipe-container .wprm-recipe-rating {
    margin: 0 0 16px;
}

.wprm-recipe-container .wprm-rating-star {
    margin: 0 0.05em;
	font-size: 24px;
}

.wprm-recipe-container .wprm-rating-star polygon {
    stroke: #cb346d;
}

.wprm-recipe-container .wprm-rating-star-full polygon {
    fill: #cb346d !important;
}

.wprm-recipe-container .wprm-recipe-rating-details {
    display: inline-block;
    margin-left: 8px;
	font-size: 11px;
	vertical-align: middle;
}

/* [ Info ] */

/* Columns (Times) */

.wprm-recipe-info-columns {
    display: flex;
    flex-wrap: wrap;
	margin: 0 16px;
	padding-bottom: 12px;
    line-height: 1.2;
}

.wprm-recipe-info-columns > .wprm-recipe-block-container {
    flex: 1 1 20%;
    padding: 8px 4px;
	margin: 0 4px;
	font-size: 14px;
	background-color: #fff;
}

@media (min-width: 768px) {
    .wprm-recipe-info-columns > .wprm-recipe-block-container {
        font-size: 18px;
    }
}

.wprm-recipe-info-columns .wprm-recipe-details-label {
    text-transform: uppercase;
    color: #000;
    letter-spacing: 0.1em;
	font-weight: 600;
	display: block;
}

@media (min-width: 768px) {
	.wprm-recipe-info-columns .wprm-recipe-details-label {
		display: inline-block;
		margin-right: 10px;
	}
}

/* Inline (Servings)  */

.wprm-recipe-info-inline {
	color: #fff;
	background-color: #1c4267;
	padding: 8px;
}

@media screen and (min-width: 768px) {
    .wprm-recipe-info-inline {
        padding: 12px 8px 12px 12px;
    }
}

.wprm-recipe-info-inline .wprm-recipe-block-container {
	display: flex;
	align-items: center;
	justify-content: center;
}

.wprm-recipe-info-inline .wprm-recipe-details-label {
    font-weight: 600 !important;
    font-size: 16px;
    color: #fff;
	display: inline-block;
    margin-right: 8px;
}

.wprm-recipe-info-inline .wprm-recipe-time-container {
    padding-bottom: 2px;
    border-bottom: 1px solid #15385c;
    margin-bottom: 8px;
}

input[type=number].wprm-recipe-servings {
