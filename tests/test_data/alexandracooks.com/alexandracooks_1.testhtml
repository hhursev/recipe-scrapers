			.tasty-recipes-print-view .adthrive-ad {
    display: none;
}

/* Center social sharing on wide template */
.post-template-single-wide #dpsp-content-top ul.dpsp-networks-btns-content {
	display: flex;
	justify-content: center;
}

/* Making hello bar thicker */
.top-bar {
	  font-size: 20px;
    height: auto;
	  line-height: 1.2;
	  padding: 10px 0;
}
.top-bar-spacer {
    min-height: 44px;
}
@media (max-width: 799px){
	.header{
		padding-top: 58px;
	}
}

.iframe-container{
  position: relative;
  width: 100%;
  padding-bottom: 56.25%; 
  height: 0;
}
.iframe-container iframe{
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height: 100%;
}
/************************/

/* FD - Update heading structure on /shop/ page */

.subcats.shop-subcats h2 {
	font-size: 18px;
	margin: 0 0 -10px;
}

/* FD - Open nav dropdowns on focus */

#main ul li:focus-within > ul {
	display: block;
}



/* FD - Popups - move to style.css */

.pum-theme-alexandras-kitchen .pum-container {
		background-image: var(--wpr-bg-86c3326e-e4e4-4d69-b5d4-2f90d6794fab);
    background-size: cover;
    background-position: 70% 50%;
		background-repeat: no-repeat;
}
#pum-81543 .pum-container, #pum-82305 .pum-container {
	background-image: var(--wpr-bg-bed1ca0d-6df4-48d8-9beb-e7d63a28096c);
}
#pum-81564 .pum-container {
	background-image: var(--wpr-bg-7b3619e5-0bdc-4971-96b2-e3f07b39a8d0);
}

.pum-theme-alexandras-kitchen .pum-content {
	margin: 0 auto;
	background-color: rgba(0,0,0,.7);
	padding: 25px;
	overflow: hidden;
}
.pum-theme-alexandras-kitchen .pum-content + .pum-close {
	letter-spacing: .1em;
}
.pum-theme-alexandras-kitchen .pum-content {
	text-align: center;
}
.pum-theme-alexandras-kitchen .title h3 {
	font-size: 34px;
	color: #fff;
	margin: 5px 0 10px;
	text-transform: none;
	letter-spacing: normal;
}
.pum-theme-alexandras-kitchen p {
	font-family: futura,Arial,Helvetica,Geneva,sans-serif;
	font-weight: 700;
	line-height: 1.5;
	text-transform: uppercase;
}
.pum-theme-alexandras-kitchen .title p {
	color: #b7cfb2;
	font-size: 15px;
	letter-spacing: .1em;
	margin: 0 0 10px;
}
.pum-theme-alexandras-kitchen .formkit-input {
    color: #434f57;
    font-style: italic;
    font-size: 18px!important;
    font-family: futura,Arial,Helvetica,Geneva,sans-serif;
}
.pum-theme-alexandras-kitchen .formkit-submit {
		text-transform: uppercase;
		font-family: futura,Arial,Helvetica,Geneva,sans-serif;
    font-size: 19px!important;
    text-transform: uppercase;
    letter-spacing: .15em;
    text-align: center;
}
.pum-theme-alexandras-kitchen .formkit-powered-by-convertkit-container {
	display: none!important;
}
@media (min-width: 800px){
	.pum-theme-alexandras-kitchen .formkit-form .formkit-field {
    flex: 1 0 48% !important;
    margin-right: 1% !important;
	}
}
/* Pizza school pop-up */
.pum-theme-alexandras-kitchen #popmake-83799 {
	background-image: var(--wpr-bg-179a0dcd-320d-40db-8790-aeea4ce6feda);
}
@media (min-width: 501px){
	.pum-theme-alexandras-kitchen #popmake-8379 {
		background-position-y: -100px;
	}
}

/* FD - TikTok icon in header */
.header-social svg,
#cmwa svg {
	fill: #86a183;
	height: 18px;
}
.header-social svg:hover {
	fill: #b7cfb2;
}

/* FD - Link pages */
.links-page.has-nested-images figure.wp-block-image figcaption {
	font-size: .8em;
	line-height: 1.2;
}

/* FD - Hide Instagram footer on Instagram and TikTok pages */
.page-id-82006 .instagram.footer-widget, .page-id-82191 .instagram.footer-widget {
	display: none;
}

/* FD - Home page video */
.home-section.video {
	max-width: 880px;
	margin: 0 auto;
	margin-bottom: 100px;
	background-color: #e9f0e8;
	padding: 25px;
}
.home-section.video h4 {
	margin: 25px 0 10px;
	font-size: 32px;
}
.home-section.video p {
	font-size: 90%;
}
.home-section.video .more a {
	color: #424242;
	background-color: #b7cfb2;
	display: inline-block;
	padding: 8px 13px 4px;
	text-transform: uppercase;
	letter-spacing: .1em;
	font-size: 14px;
	font-weight: 700;
	float: left;
}
.home-section.video .more a:hover {
    background-color: #86a183;
    color: #fff;
}

/* FD LB 22-11-28 - Pizza school opt-in */
.post-content .formkit-subscribe .textwidget {
	padding-bottom: 15px;
}
.post-content .formkit-subscribe .title {
	margin-bottom: 20px;
}
.subscribe .formkit-submit > span {
	padding: 3px 0 0!important;
	font-size: 17px;
}
.formkit-form[data-uid="b112762ef4"] {
	margin: 0 auto;
}
.subscribe p.note,
.pum-theme-alexandras-kitchen p.note {
		font-family: "Futura", "Trebuchet MS", Arial, helvetica, sans-serif;
	font-size: 14px;
	font-weight: normal;
	line-height: 1.6;
	letter-spacing: 1px;
	color: #fff;
	text-transform: none;
	font-weight: normal;
}
@media only screen and (min-width: 501px) {
	.inner-post .pizza-school {
		background-position-y: -100px;
	}
	.single .subscribe .formkit-fields,
	.pum-container .formkit-fields {
		gap: 0 2%!important;
	}
	.single .subscribe .formkit-field,
	.pum-container .formkit-field {
		flex: 0 1 49%!important;
	}
}

/* FD LB 2023-03-29 - Post header */
.post-meta .author a {
	text-decoration: underline;
}
.post-meta .jumpbuttons {
	display: block;
	margin-top: 10px;
}
/* Added 2023-08-18 */
.post-meta .tasty-recipes-quick-links {
	display: inline-block;
}
.post-meta .rating-label {
	text-transform: none;
	font-weight: 400;
	letter-spacing: normal;
	font-size: 13px;
}
.post-meta .tasty-recipes-quick-links a {
	padding-left: 0;
	padding-right: 5px;
	padding-bottom: 0;
}
.post-meta .spacer {
	margin-left: 0;
}

/* FD CU 2023-03-31 - Pizza school homepage optin bg */
.homepage-subscribe .wrap, .inner-post .subscribe.email-highlight {
    background: var(--wpr-bg-f2783886-fedd-46df-aa07-2d400bc927ce);
    background-position: center;
}

.sidebar .subscribe {
	background-image: var(--wpr-bg-8ec50571-1353-46e3-937e-7f795eeb5312);
}

/* FD LB 2023-04-28 - Image blocks */
.wp-block-image {
	margin-bottom: 1em;
}
